\[DoubleStruckCapitalC]]]\>\""}], "}"}]}], ";", 
       RowBox[{"$", "//", "ColumnBar"}]}]}], "]"}], ",", 
    "\[IndentingNewLine]", "next", ",", "\"\<(ii)' The situtation \>\"", ",", 
    
    RowBox[{"xtmp", "=", 
     RowBox[{"$0", "=", 
      RowBox[{"$", "=", 
       RowBox[{"\[Epsilon]", " ", 
        RowBox[{"a", ".", 
         RowBox[{"a", "'"}]}]}]}]}]}], ",", "\[IndentingNewLine]", "yield", 
    ",", 
    RowBox[{"$", "=", 
