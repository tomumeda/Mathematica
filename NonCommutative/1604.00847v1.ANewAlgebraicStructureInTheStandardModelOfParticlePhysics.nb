Cell[BoxData[
 RowBox[{
  RowBox[{"PR", "[", 
   RowBox[{
   "\"\<\[FilledCircle] Definitions: \>\"", ",", "\[IndentingNewLine]", 
    "next", ",", "\"\<(i) An algebra \>\"", ",", "iA", ",", 
    "\"\< over a field \[DoubleStruckCapitalF] is a vector space equipped \
with a \[DoubleStruckCapitalF]-bilinear product \>\"", ",", 
    RowBox[{
     RowBox[{"$", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"a", ".", 
            RowBox[{"a", "'"}]}], ",", "a", ",", 
           RowBox[{"a", "'"}], ",", 
           RowBox[{"a", "''"}]}], "}"}], "\[Element]", "iA"}], ",", 
        RowBox[{
         RowBox[{"iA", "[", 
          RowBox[{"CG", "[", "\"\<commutative\>\"", "]"}], "]"}], 
         "\[Implies]", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"CommutatorM", "[", 
            RowBox[{"a", ",", 
             RowBox[{"a", "'"}]}], "]"}], "\[Rule]", "0"}], ")"}]}], ",", 
        RowBox[{
         RowBox[{"iA", "[", 
          RowBox[{"CG", "[", "\"\<associative\>\"", "]"}], "]"}], 
         "\[Implies]", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"associator", "[", 
            RowBox[{"a", ",", 
             RowBox[{"a", "'"}], ",", 
             RowBox[{"a", "''"}]}], "]"}], "\[Rule]", "0"}], ")"}]}], ",", 
        RowBox[{
         RowBox[{"associator", "[", 
          RowBox[{"a_", ",", "b_", ",", "c_"}], "]"}], "\[Rule]", 
         RowBox[{"Inactivate", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{"a", ".", "b"}], ")"}], ".", "c"}], "-", 
            RowBox[{"a", ".", 
             RowBox[{"(", 
              RowBox[{"b", ".", "c"}], ")"}]}]}], ",", "Dot"}], "]"}]}]}], 
       "}"}]}], ";", 
     RowBox[{"$", "//", "ColumnBar"}]}], ",", 
    RowBox[{"accumDGA", "[", "$", "]"}], ",", "\[IndentingNewLine]", "next", 
    ",", "\"\<(ii) A *-algebra \>\"", ",", "iA", ",", 
    "\"\<(over \[DoubleStruckCapitalF]) has additionally a *-operator: \
