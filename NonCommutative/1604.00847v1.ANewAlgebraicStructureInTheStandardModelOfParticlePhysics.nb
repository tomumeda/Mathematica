         RowBox[{
          SubscriptBox["d", "L"], "\[SmallCircle]", 
          SubscriptBox["d", "L"]}], "\[Rule]", "0"}], ",", 
        RowBox[{
         RowBox[{
          SubscriptBox["d", "L"], "[", 
          RowBox[{
           SubscriptBox["a", "m"], ".", 
           SubscriptBox["a", "n"]}], "]"}], "->", 
         RowBox[{
          RowBox[{
           RowBox[{
            SubscriptBox["d", "L"], "[", 
            SubscriptBox["a", "m"], "]"}], ".", 
           SubscriptBox["a", "n"]}], "+", 
          RowBox[{
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"-", "1"}], ")"}], "m"], 
           RowBox[{
            SubscriptBox["a", "m"], ".", " ", 
            RowBox[{
             SubscriptBox["d", "L"], "[", 
             SubscriptBox["a", "n"], "]"}]}]}]}]}]}], " ", "}"}]}], ";", 
     RowBox[{"$", "//", "ColumnBar"}]}], ",", 
    RowBox[{"accumDGA", "[", "$", "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"CO", "[", "\"\<EG. differential forms\>\"", "]"}], 
    "\[IndentingNewLine]", ",", "next", ",", 
    "\"\<(v) A differential graded *-algebra (*DGA) is a DGA and *-algebra. \
\>\"", ",", 
    RowBox[{"$", "=", 
