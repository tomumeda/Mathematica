        RowBox[{"\[Gamma]", ",", "\"\<u\>\"", ",", 
         RowBox[{"{", 
          OverscriptBox["\[Mu]", "~"], "}"}]}], "]"}], 
       RowBox[{"tuDPartial", "[", 
        RowBox[{"_", ",", 
         OverscriptBox["\[Mu]", "~"]}], "]"}]}]}]}]}], ",", 
