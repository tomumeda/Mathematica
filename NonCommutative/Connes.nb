          OverscriptBox["\[Mu]", "~"], ",", 
          OverscriptBox["\[Nu]", "~"]}], "}"}]}], "]"}]}]}], "}"}], ",", 
   "\[IndentingNewLine]", "Yield", ",", "\"\<Dirac operator: \>\"", ",", 
   RowBox[{"$D", "=", 
    RowBox[{"\[ScriptCapitalD]", "->", 
