Cell[BoxData[
 RowBox[{"PR", "[", 
  RowBox[{"\"\<\[FilledCircle] If the \!\(\*
StyleBox[\"dynamics\",
FontSlant->\"Italic\"]\) are determined by the Dirac equation: \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"slash", "[", 
     RowBox[{"tuDPartial", "[", 
      RowBox[{"\[Psi]", ",", "_"}], "]"}], "]"}], "\[Rule]", "0"}], ",", 
   "\[IndentingNewLine]", "\"\< where \>\"", ",", 
   RowBox[{
    RowBox[{"slash", "[", 
     RowBox[{"tuDPartial", "[", 
      RowBox[{"\[Psi]", ",", "_"}], "]"}], "]"}], "\[Element]", 
    RowBox[{"End", "[", "\[ScriptCapitalH]", "]"}]}], ",", 
   RowBox[{"CR", "[", 
    RowBox[{"\"\< Is \>\"", ",", 
     RowBox[{
      RowBox[{"slash", "[", 
       RowBox[{"tuDPartial", "[", 
        RowBox[{"\[Psi]", ",", "_"}], "]"}], "]"}], "\[Element]", 
      RowBox[{"\[Rho]", "[", "\[ScriptCapitalA]", "]"}]}], ",", "\"\<?\>\""}],
     "]"}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<\[Bullet]Connes' algebraic data: \>\"", ",", 
   RowBox[{"{", 
    RowBox[{"\[ScriptCapitalA]", ",", "\[ScriptCapitalH]", ",", 
     RowBox[{"slash", "[", 
      RowBox[{"tuDPartial", "[", 
       RowBox[{"_", ",", "_"}], "]"}], "]"}]}], "}"}], ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<requires self-adjointness: \>\"", 
   ",", "\[IndentingNewLine]", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"x", "\[Element]", "M"}], ",", 
     RowBox[{
      RowBox[{"\[Psi]", "[", "x", "]"}], "\[Element]", "\[ScriptCapitalH]"}], 
     ",", 
     RowBox[{
      RowBox[{"slash", "[", 
       RowBox[{"tuDPartial", "[", 
        RowBox[{"_", ",", "_"}], "]"}], "]"}], "==", 
      SuperscriptBox[
       RowBox[{"slash", "[", 
        RowBox[{"tuDPartial", "[", 
         RowBox[{"_", ",", "_"}], "]"}], "]"}], "*"]}]}], "}"}], ",", 
   "\[IndentingNewLine]", "NL", ",", 
   "\"\<\[FilledCircle] Reconstruct Remannian geometry: \>\"", ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<\[Bullet]scalar product: \>\"", ",", 
   RowBox[{
    RowBox[{"BraKet", "[", 
     RowBox[{"\[Psi]", ",", 
      RowBox[{"\[Psi]", "'"}]}], "]"}], "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"tuIntegral", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"{", "V", "}"}], "}"}], ",", 
        RowBox[{
         RowBox[{"ConjugateTranspose", "[", 
          RowBox[{"\[Psi]", "[", "x", "]"}], "]"}], ".", 
         RowBox[{
          RowBox[{"\[Psi]", "'"}], "[", "x", "]"}]}]}], "]"}], ",", 
      RowBox[{
       RowBox[{"d", "[", "V", "]"}], "\[Rule]", 
       RowBox[{
        SqrtBox[
         RowBox[{"Det", "[", "g", "]"}]], 
        RowBox[{"d", "[", 
         SuperscriptBox["x", "4"], "]"}]}]}]}], "}"}]}], ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<\[Bullet]Dirac action: \>\"", ",", 
   RowBox[{
    SubscriptBox["S", "D"], "\[Rule]", 
    RowBox[{"BraKet", "[", 
     RowBox[{"\[Psi]", ",", 
      RowBox[{"slash", "[", 
