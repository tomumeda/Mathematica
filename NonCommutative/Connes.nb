        RowBox[{"\[Psi]", ",", "_"}], "]"}], "]"}]}], "]"}]}], ",", 
   RowBox[{"CO", "[", "\"\<\[LongLeftArrow]Euclidean metric\>\"", "]"}], ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<\[Bullet]representation: \>\"", ",", 
   RowBox[{"$r", "=", 
