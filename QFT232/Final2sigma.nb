Cell[BoxData[
 RowBox[{
  RowBox[{"PR1", "[", "\[IndentingNewLine]", 
   RowBox[{
   "\"\<The definition of charge/number density, \[Rho]: \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{
      RowBox[{"MCommutator", "[", 
       RowBox[{
        RowBox[{
         SuperscriptBox["\[Psi]", "\[Dagger]"], "[", 
         RowBox[{"x", ",", "t"}], "]"}], ",", 
        RowBox[{"\[Psi]", "[", 
         RowBox[{"x", ",", "t"}], "]"}]}], "]"}], "/", "2"}]}], ",", 
    "\[IndentingNewLine]", "NL", ",", 
    "\"\<and using energy eigen functions \>\"", ",", "\[IndentingNewLine]", 
    "sub\[Psi]", ",", "\[IndentingNewLine]", "\"\<POFF\>\"", ",", "yield", 
    ",", "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"tmp", "/.", "sub\[Psi]"}]}], ",", "\[IndentingNewLine]", "NL", 
    ",", "\"\<Extract Sums, drop [x] \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp1", "=", 
     RowBox[{"tmp", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"DotSum2SumDot", "[", "tmp", "]"}], "/.", 
        RowBox[{
         RowBox[{"a_", "[", "x", "]"}], "\[Rule]", "a"}]}], "//.", 
       RowBox[{"simpleDot1", "[", 
        RowBox[{"{", 
         RowBox[{"Exp", "[", "_", "]"}], "}"}], "]"}]}]}]}], ",", 
    "\[IndentingNewLine]", "\"\<PON\>\"", ",", "\[IndentingNewLine]", "NL", 
    ",", "\"\<Integrating \[Rho] over t introduces a KroneckerDelta[m,n] \
\>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{
      RowBox[{"tmp", "/.", 
       RowBox[{"Sum", "\[Rule]", "xSum"}]}], "/.", 
      RowBox[{
       RowBox[{"xSum", "[", 
        RowBox[{"a_", ",", "n_", ",", "m_"}], "]"}], ":>", 
       RowBox[{"xSum", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"a", "/.", 
           RowBox[{"n", "\[Rule]", "m"}]}], ")"}], ",", "m"}], "]"}]}]}]}], 
    ",", "\[IndentingNewLine]", "NL", ",", "\"\<For zero mode, n->0 \>\"", 
    ",", "Yield", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"tmp", "/.", 
      RowBox[{
       RowBox[{"xSum", "[", 
        RowBox[{"a_", ",", "n"}], "]"}], "\[RuleDelayed]", 
       RowBox[{"Sum", "[", 
        RowBox[{"a", ",", 
         RowBox[{"{", 
          RowBox[{"n", ",", "0", ",", "0"}], "}"}]}], "]"}]}]}]}], ",", 
    "Yield", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"tmp", "/.", 
        RowBox[{
         RowBox[{"Conjugate", "[", 
          RowBox[{"\[Psi]d", "[", "0", "]"}], "]"}], "\[Rule]", 
         RowBox[{"\[Psi]d", "[", "0", "]"}]}]}], "//.", 
       RowBox[{"simpleDot1", "[", 
        RowBox[{"{", 
         RowBox[{"\[Psi]d", "[", "0", "]"}], "}"}], "]"}]}], "//", 
      "Simplify"}]}], ",", "\[IndentingNewLine]", "NL", ",", 
    "\"\<Unit normalization for \>\"", ",", 
    RowBox[{"\[Psi]d", "[", "0", "]"}], ",", "yield", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp\[Rho]", "=", 
     RowBox[{
      RowBox[{"\[Rho]", "->", "tmp"}], "/.", 
      RowBox[{
       RowBox[{"\[Psi]d", "[", "0", "]"}], "\[Rule]", "1"}]}]}], ",", 
    "\[IndentingNewLine]", "NL", ",", 
    "\"\<and the anti-commutation relationship \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"subAC", "=", 
     RowBox[{"tmpAC", "/.", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"n", "\[Rule]", "0"}], ",", 
        RowBox[{"m", "\[Rule]", "0"}]}], "}"}]}]}], ",", "Imply", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp\[Rho]m", "=", 
     RowBox[{"tmp\[Rho]", "/.", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"#", "-", 
          RowBox[{"subAC", "[", 
           RowBox[{"[", 
            RowBox[{"1", ",", "1"}], "]"}], "]"}]}], "&"}], ",", "subAC"}], 
       "]"}]}]}], ",", "\"\< or \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp\[Rho]p", "=", 
     RowBox[{"tmp\[Rho]", "/.", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"#", "-", 
          RowBox[{"subAC", "[", 
           RowBox[{"[", 
            RowBox[{"1", ",", "2"}], "]"}], "]"}]}], "&"}], ",", "subAC"}], 
       "]"}]}]}], ",", "\[IndentingNewLine]", "NL", ",", 
    "\"\<So with the eigen state relationships \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"Column", "[", "\[IndentingNewLine]", 
     RowBox[{"sub1", "=", 
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Reverse", "[", "sp", "]"}], ",", "\[IndentingNewLine]", "sm",
         ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          SuperscriptBox[
           RowBox[{"bd", "[", "0", "]"}], "\[Dagger]"], ".", 
          RowBox[{"Ket", "[", "\"\<+\>\"", "]"}]}], "->", "0"}]}], 
       "\[IndentingNewLine]", "}"}]}], "\[IndentingNewLine]", "]"}], ",", 
    "\[IndentingNewLine]", "\"\<where \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"Column", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"Ket", "[", "0", "]"}], "\[Rule]", "\"\<vacuum state\>\""}], 
       ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Ket", "[", "\"\<+\>\"", "]"}], "\[Rule]", 
        "\"\<positive 0-energy eigen state\>\""}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Ket", "[", "\"\<-\>\"", "]"}], "\[Rule]", 
        "\"\<negative 0-energy eigen state\>\""}]}], "\[IndentingNewLine]", 
      "}"}], "]"}], ",", "\[IndentingNewLine]", "NL", ",", 
    "\"\<the charge eigen values are:\>\"", ",", "NL", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"\[Rho]", ".", 
      RowBox[{"Ket", "[", "\"\<-\>\"", "]"}]}]}], ",", "yield", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{
      RowBox[{"tmp", "/.", "tmp\[Rho]m"}], "//", "ExpandAll"}]}], ",", 
    "yield", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"tmp", "//.", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"DotExpand", ",", "simpleDot", ",", "sub1"}], "}"}], "//", 
        "Flatten"}], ")"}]}]}], ",", "\[IndentingNewLine]", "NL", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"\[Rho]", ".", 
      RowBox[{"Ket", "[", "\"\<+\>\"", "]"}]}]}], ",", "yield", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{
      RowBox[{"tmp", "/.", "tmp\[Rho]p"}], "//", "ExpandAll"}]}], ",", 
    "yield", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"tmp", "//.", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"DotExpand", ",", "simpleDot", ",", "sub1"}], "}"}], "//", 
        "Flatten"}], ")"}]}]}], ",", "\[IndentingNewLine]", "Imply", ",", 
    "\"\<Zero energy mode fractional charge or fractional Fermion number is \
possible.\>\""}], "\[IndentingNewLine]", "]"}], ";"}]], "Input",
