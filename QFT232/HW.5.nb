Cell[BoxData[
 RowBox[{"PR1", "[", 
  RowBox[{
  "\"\<The nonlinear sigma model for N==3 and \!\(\*SubscriptBox[\(n\), \
\(i\)]\)\[Element]\[DoubleStruckCapitalR], with the constraint: \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"conditionn", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"nu", "[", "i", "]"}], ".", 
      RowBox[{"nu", "[", "i", "]"}]}], "->", 
     RowBox[{
      RowBox[{
       RowBox[{"ConjugateTranspose", "[", 
        RowBox[{"nu", "[", "i", "]"}], "]"}], ".", 
       RowBox[{"nu", "[", "i", "]"}]}], "->", "1"}]}]}], ",", 
   "\[IndentingNewLine]", 
   "\"\< (Sum over i.  We use ConjugateTranspose for computational \
convenience.) Here the state vectors are points on a unit sphere.\nIn the \
\!\(\*SuperscriptBox[\(\[DoubleStruckCapitalC]P\), \(1\)]\) model the state \
vector is {\!\(\*SubscriptBox[\(z\), \(1\)]\),\!\(\*SubscriptBox[\(z\), \
\(2\)]\)} with constraint \>\"", ",", 
   RowBox[{"conditionz", "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"Conjugate", "[", 
        RowBox[{"zd", "[", "1", "]"}], "]"}], 
       RowBox[{"zd", "[", "1", "]"}]}], "+", 
      RowBox[{
       RowBox[{"Conjugate", "[", 
        RowBox[{"zd", "[", "2", "]"}], "]"}], 
       RowBox[{"zd", "[", "2", "]"}]}]}], "->", "1"}]}], ",", 
   "\"\<.  We define the vector: \>\"", ",", "\[IndentingNewLine]", 
   RowBox[{"MatrixForms", "[", "  ", 
    RowBox[{"subz", "=", 
     RowBox[{"z", "->", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"zd", "[", "1", "]"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"zd", "[", "2", "]"}], "}"}]}], "}"}]}]}], "]"}], "  ", ",", 
   "Yield", ",", "\[IndentingNewLine]", "\"\<Applying the relationship \>\"", 
   ",", "yield", ",", "\[IndentingNewLine]", 
   RowBox[{"subn", "=", 
    RowBox[{
     RowBox[{"nu", "[", "i_", "]"}], "->", 
     RowBox[{
      SuperscriptBox["z", "\[Dagger]"], ".", 
      RowBox[{"\[Sigma]u", "[", "i", "]"}], ".", "z"}]}]}], ",", 
   "\"\< to \>\"", ",", "Yield", ",", "\[IndentingNewLine]", 
   RowBox[{"Column", "[", " ", 
    RowBox[{"tmpn", "=", 
     RowBox[{"tmp", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"nu", "[", "1", "]"}], ",", 
        RowBox[{"nu", "[", "2", "]"}], ",", 
        RowBox[{"nu", "[", "3", "]"}]}], "}"}]}]}], "]"}], ",", "yield", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"Column", "[", " ", 
    RowBox[{"tmpx", "=", 
     RowBox[{"tmp", "=", 
      RowBox[{"tmp", "/.", "subn"}]}]}], "]"}], ",", "yield", ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Column", "[", " ", 
     RowBox[{"tmp", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"tmp", "/.", "subz"}], "/.", "sPauli"}], "/.", 
       "subDaggerCT"}]}], "]"}], "//", "Flatten"}], ",", "yield", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"Column", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"2", " ", 
       RowBox[{"Re", "[", 
        RowBox[{
         RowBox[{"Conjugate", "[", 
          RowBox[{"zd", "[", "1", "]"}], "]"}], 
         RowBox[{"zd", "[", "2", "]"}]}], "]"}]}], ",", 
      RowBox[{"2", " ", 
       RowBox[{"Im", "[", 
        RowBox[{
         RowBox[{"Conjugate", "[", 
          RowBox[{"zd", "[", "1", "]"}], "]"}], 
         RowBox[{"zd", "[", "2", "]"}]}], "]"}]}], ",", 
      RowBox[{
       RowBox[{"2", " ", 
        SuperscriptBox[
         RowBox[{"Abs", "[", 
          RowBox[{"zd", "[", "1", "]"}], "]"}], "2"]}], "-", "1"}]}], "}"}], 
    "]"}], ",", "Yield", ",", "\[IndentingNewLine]", "\"\<The Sum \>\"", ",", 
   
   RowBox[{"tmpc", "=", 
    RowBox[{"conditionn", "[", 
     RowBox[{"[", 
      RowBox[{"2", ",", "1"}], "]"}], "]"}]}], ",", 
   "\"\<  over i yields \>\"", ",", "yield", ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"tmpct", "=", 
     RowBox[{
      RowBox[{"tmp", "//", "ConjugateTranspose"}], "//", "Flatten"}]}], ";", 
    "Yield", ";", "\[IndentingNewLine]", 
    RowBox[{"tmps", "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"Sum", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"tmpct", "[", 
             RowBox[{"[", "i", "]"}], "]"}], 
            RowBox[{"tmp", "[", 
             RowBox[{"[", "i", "]"}], "]"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "3"}], "}"}]}], "]"}], "//", "Expand"}], "//", 
        "Simplify"}], "//", "First"}], "//", "First"}]}]}], ",", "yield", ",",
    "\[IndentingNewLine]", 
