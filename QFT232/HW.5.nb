         RowBox[{"I", " ", "\[Alpha]"}], "]"}]}]}], ",", 
      RowBox[{
       RowBox[{"zd", "[", "2", "]"}], "\[Rule]", 
       RowBox[{"r2", " ", 
        RowBox[{"Exp", "[", 
         RowBox[{"I", " ", "\[Beta]"}], "]"}]}]}]}], "}"}]}], ",", 
   "\[IndentingNewLine]", "\"\< with constraint\>\"", ",", "Yield", ",", 
