     "\[IndentingNewLine]", "NL", ",", 
     "\"\<Substituting this into the commutation relation: \>\"", ",", 
     "\[IndentingNewLine]", "\"\<POFF\>\"", ",", "\[IndentingNewLine]", 
     RowBox[{"tmp", "=", 
      RowBox[{
       RowBox[{"tComm", "/.", "tmpp"}], "//.", "simpleDot"}]}], ",", 
     "\[IndentingNewLine]", "\"\<PON\>\"", ",", "Yield", ",", 
     "\[IndentingNewLine]", 
     RowBox[{"tmp", "=", 
      RowBox[{
       RowBox[{"tmp", "/.", 
        RowBox[{
         RowBox[{
          OverscriptBox["a_", "_"], "[", "x__", "]"}], "->", 
