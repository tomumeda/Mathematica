      RowBox[{"DotIndexSum", "[", 
       RowBox[{"tmpp", ",", 
        RowBox[{"{", 
         RowBox[{"0", ",", "1"}], "}"}]}], "]"}]}], ",", "Yield", ",", 
     "\[IndentingNewLine]", "\"\<POFF\>\"", ",", "\[IndentingNewLine]", 
     RowBox[{"tmpp", "=", 
      RowBox[{
       RowBox[{"tmpp", "//.", 
