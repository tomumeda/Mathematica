          RowBox[{"_.", "_"}]}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"tmp1", "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "2"}], "]"}], "]"}]}], ",", "\[IndentingNewLine]", 
     "\"\< must -> 0 whenever m\[NotEqual]n.   So, \>\"", ",", 
