         RowBox[{"{", 
          RowBox[{"g", ",", "M", ",", "\[Lambda]", ",", 
           RowBox[{"\[Gamma]u", "[", "_", "]"}]}], "}"}], "]"}]}], "//.", 
       "simpleDot"}]}], ",", "Yield", ",", "\[IndentingNewLine]", 
     RowBox[{"tmpp", "=", 
      RowBox[{"tmpp", "/.", 
       RowBox[{
        RowBox[{"xPartialDu", "[", 
         RowBox[{"a_", ",", "b_"}], "]"}], "\[RuleDelayed]", 
        RowBox[{"Sum", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"xPartialD", "[", 
            RowBox[{"a", ",", "$i"}], "]"}], 
           RowBox[{"\[Eta]uu", "[", 
            RowBox[{"$i", ",", "b"}], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"$i", ",", "0", ",", "1"}], "}"}]}], "]"}]}]}]}], ",", 
     "Yield", ",", "\[IndentingNewLine]", 
     RowBox[{"tmpp", "=", 
