          RowBox[{"a", ",", "b"}], "]"}]}]}], "//.", "simpleDot"}]}], ",", 
     "Yield", ",", "\[IndentingNewLine]", 
     RowBox[{"tmpp", "=", 
      RowBox[{"tmpp", "/.", 
       RowBox[{
        RowBox[{"HoldPattern", "[", 
         RowBox[{"D", "[", 
          RowBox[{"a_", ",", "b_"}], "]"}], "]"}], "->", 
        RowBox[{"xPartialD", "[", 
         RowBox[{"a", ",", "b"}], "]"}]}]}]}], ",", "Yield", ",", 
     "\[IndentingNewLine]", "\"\<PON\>\"", ",", "\[IndentingNewLine]", 
     RowBox[{"tmpp", "=", 
      RowBox[{"tmpp", "/.", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"\[CapitalPi]", "->", 
