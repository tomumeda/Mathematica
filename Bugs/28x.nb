 RowBox[{"PR", "[", 
  RowBox[{
  "\"\<Attempt to compute two-loop contribution to propagator from \>\"", ",",
    "\[IndentingNewLine]", 
   "\"\<(14.04) to O[\!\(\*SuperscriptBox[\(\[Lambda]\), \(2\)]\)]\>\"", ",", 
   "Yield", ",", "\[IndentingNewLine]", 
   RowBox[{"e1404", "=", 
    RowBox[{
     RowBox[{"\[CapitalPi]", "[", "k_", "]"}], "->", 
     RowBox[{
      RowBox[{
       FractionBox["1", "2"], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"I", " ", "\[Lambda]"}], ")"}], "2"], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"-", "I"}], ")"}], "2"], 
       RowBox[{"IntegralOp", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"q", "[", "1", "]"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"q", "[", "2", "]"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"q", "[", "3", "]"}], "}"}]}], "}"}], ",", 
         RowBox[{
          FractionBox["1", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"2", " ", "\[Pi]"}], ")"}], "d"]], 
