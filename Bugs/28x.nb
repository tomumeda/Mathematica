Cell[BoxData[
 RowBox[{"PR", "[", "\[IndentingNewLine]", 
  RowBox[{
  "\"\<Following renormalization group procedure with(28.41):\\n\>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"e2841", "=", 
    RowBox[{"\[ScriptCapitalL]", "->", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", "2"]}], 
       SubscriptBox["Z", "\[CurlyPhi]"], 
       RowBox[{"guu", "[", 
        RowBox[{"\[Beta]", ",", "\[Alpha]"}], "]"}], 
       RowBox[{"xPartialD", "[", 
        RowBox[{"\[CurlyPhi]", ",", "\[Alpha]"}], "]"}], 
       RowBox[{"xPartialD", "[", 
        RowBox[{"\[CurlyPhi]", ",", "\[Beta]"}], "]"}]}], "-", 
      RowBox[{
       FractionBox["1", "2"], 
       SubscriptBox["Z", "m"], 
       SuperscriptBox["m", "2"], 
       SuperscriptBox["\[CurlyPhi]", "2"]}], "-", 
      RowBox[{
       FractionBox["1", "24"], 
       SubscriptBox["Z", "\[Lambda]"], "\[Lambda]", " ", 
       SuperscriptBox[
        OverscriptBox["\[Mu]", "~"], 
        RowBox[{"\[CurlyEpsilon]", "/", "2"}]], 
       SuperscriptBox["\[CurlyPhi]", "4"]}]}]}]}], ",", 
   "\[IndentingNewLine]", "\"\<\\nand its bare field equation:\>\"", ",", 
   "Yield", ",", "\[IndentingNewLine]", 
   RowBox[{"b2841", "=", 
    RowBox[{"\[ScriptCapitalL]", "->", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", "2"]}], 
       RowBox[{"guu", "[", 
        RowBox[{"\[Beta]", ",", "\[Alpha]"}], "]"}], 
       RowBox[{"xPartialD", "[", 
        RowBox[{
         SubscriptBox["\[CurlyPhi]", "0"], ",", "\[Alpha]"}], "]"}], 
       RowBox[{"xPartialD", "[", 
        RowBox[{
         SubscriptBox["\[CurlyPhi]", "0"], ",", "\[Beta]"}], "]"}]}], "-", 
      RowBox[{
       FractionBox["1", "2"], 
       SuperscriptBox[
        SubscriptBox["m", "0"], "2"], 
       SuperscriptBox[
        SubscriptBox["\[CurlyPhi]", "0"], "2"]}], "-", 
      RowBox[{
       FractionBox["1", "24"], 
       SubscriptBox["\[Lambda]", "0"], " ", 
       SuperscriptBox[
        SubscriptBox["\[CurlyPhi]", "0"], "4"]}]}]}]}], ",", 
   "\[IndentingNewLine]", "\"\<\\ndetermine the relationships:\>\"", ",", 
   "Yield", ",", "\[IndentingNewLine]", 
   RowBox[{"tmpr", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[CurlyPhi]", "0"], "==", 
       RowBox[{
        SqrtBox[
         SubscriptBox["Z", "\[CurlyPhi]"]], "\[CurlyPhi]"}]}], ",", 
      RowBox[{
       SubscriptBox["m", "0"], "==", 
       RowBox[{
        SqrtBox[
         FractionBox[
          SubscriptBox["Z", "m"], 
          SubscriptBox["Z", "\[CurlyPhi]"]]], "m"}]}], ",", 
      RowBox[{
       SubscriptBox["\[Lambda]", "0"], "==", 
       FractionBox[
        RowBox[{
         SubscriptBox["Z", "\[Lambda]"], "\[Lambda]", " ", 
         SuperscriptBox[
          OverscriptBox["\[Mu]", "~"], 
          RowBox[{"\[CurlyEpsilon]", "/", "2"}]]}], 
        SuperscriptBox[
         SubscriptBox["Z", "\[CurlyPhi]"], "2"]]}]}], "}"}]}], ",", 
   "\[IndentingNewLine]", 
   "\"\<\\nWe use the expression for Z's, (28.9), (28.8), (28.7):\>\"", ",", 
   "Yield", ",", "\[IndentingNewLine]", 
   RowBox[{"e2809", "=", 
    RowBox[{
     SubscriptBox["Z", "\[Lambda]"], "->", 
     RowBox[{"1", "+", 
      RowBox[{"HoldForm", "[", 
       RowBox[{"Sum", "[", 
        RowBox[{
         FractionBox[
          RowBox[{
           SubscriptBox["c", "n"], "[", "\[Alpha]", "]"}], 
          SuperscriptBox["\[CurlyEpsilon]", "n"]], ",", 
         RowBox[{"{", 
          RowBox[{"n", ",", "1", ",", "Inf"}], "}"}]}], "]"}], "]"}]}]}]}], 
   ",", "and", " ", ",", "\[IndentingNewLine]", 
   RowBox[{"e2808", "=", 
    RowBox[{"e2809", "/.", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"c", "->", "b"}], ",", 
       RowBox[{"\[Lambda]", "->", "m"}]}], "}"}]}]}], ",", "and", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"e2807", "=", 
    RowBox[{"e2809", "/.", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"c", "->", "a"}], ",", 
