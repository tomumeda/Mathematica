 RowBox[{"PR", "[", 
  RowBox[{
  "\"\<The vertex function to O[\[Lambda]\!\(\*SuperscriptBox[\(]\), \(3\)]\) \
is defined:\>\"", ",", "\[IndentingNewLine]", 
   RowBox[{"tmpv", "=", 
    RowBox[{
     RowBox[{"I", " ", 
      RowBox[{
       SubscriptBox["V", "4"], "[", 
       RowBox[{
        RowBox[{"k", "[", "1", "]"}], ",", 
        RowBox[{"k", "[", "2", "]"}], ",", 
        RowBox[{"k", "[", "3", "]"}], ",", 
        RowBox[{"k", "[", "4", "]"}]}], "]"}]}], "->", 
     RowBox[{
      RowBox[{"I", " ", 
       SubscriptBox["Z", "\[Lambda]"], "\[Lambda]"}], "+", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"I", " ", "\[Lambda]"}], ")"}], "2"], 
       SuperscriptBox[
        RowBox[{"(", 
         FractionBox["1", "I"], ")"}], "2"], 
       RowBox[{"IntegralOp", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"{", 
           RowBox[{"q", "[", "1", "]"}], "}"}], "}"}], ",", 
         RowBox[{
          FractionBox["1", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"2", "\[Pi]"}], ")"}], "d"]], 
          RowBox[{"\[CapitalDelta]", "[", 
           RowBox[{"q", "[", "1", "]"}], "]"}], 
          RowBox[{"\[CapitalDelta]", "[", 
           RowBox[{"q", "[", "2", "]"}], "]"}]}]}], "]"}]}]}]}]}], ",", 
   "\[IndentingNewLine]", "\"\<where momentum is conserved: \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"sub", "=", 
    RowBox[{
     RowBox[{"q", "[", "2", "]"}], "->", 
