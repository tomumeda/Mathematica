Cell[BoxData[
 RowBox[{"PR", "[", 
  RowBox[{"\"\<Proof: Suppose there are 5 roots in a chain, \>\"", ",", 
   RowBox[{"$", "=", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"\[Beta]", "-", 
       RowBox[{"i", " ", "\[Alpha]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", 
        RowBox[{"-", "2"}], ",", "2"}], "}"}]}], "]"}]}], ",", 
   "\[IndentingNewLine]", "NL", ",", 
   "\"\<In the \[Beta]-direction, the chain \>\"", ",", 
   RowBox[{"\[Beta]", "+", 
    RowBox[{"2", " ", "\[Alpha]"}]}], ",", "\"\< has 1 roots since \>\"", ",", 
   RowBox[{
    RowBox[{"$", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"$", "=", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"Inactive", "[", "Plus", "]"}], "[", 
            RowBox[{"\[Beta]", ",", 
             RowBox[{"2", " ", "\[Alpha]"}], ",", 
             RowBox[{"-", "\[Beta]"}]}], "]"}], ",", 
           RowBox[{
            RowBox[{"Inactive", "[", "Plus", "]"}], "[", 
            RowBox[{"\[Beta]", ",", 
             RowBox[{"2", " ", "\[Alpha]"}], ",", "\[Beta]"}], "]"}]}], 
          "}"}]}], ")"}], "\[Rule]", 
       RowBox[{"(", 
        RowBox[{"$", "//", "Activate"}], ")"}]}], "}"}]}], ";", 
    RowBox[{"$", "=", 
     RowBox[{
      RowBox[{"$", "//", "First"}], "//", "Thread"}]}], ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"ColumnForms", "[", 
        RowBox[{"#1", ",", "3"}], "]"}], "&"}], ")"}], "[", "$", "]"}]}], 
   ",", "\"\< (2 root) is not a root\>\"", ",", "\[IndentingNewLine]", 
   "Imply", ",", 
   RowBox[{"$", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"\[Beta]", "+", 
          RowBox[{"2", " ", "\[Alpha]"}]}], ")"}], "\[CenterDot]", 
        "\[Beta]"}], "\[Rule]", "0"}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"\[Beta]", "-", 
          RowBox[{"2", " ", "\[Alpha]"}]}], ")"}], "\[CenterDot]", 
        "\[Beta]"}], "\[Rule]", "0"}]}], "}"}]}], ",", "\[IndentingNewLine]", 
   "imply", ",", 
   RowBox[{"$", "=", 
    RowBox[{
     RowBox[{"$", "//", "tuRuleAdd"}], "//", 
     RowBox[{"expandDC", "[", "]"}]}]}], ",", 
   RowBox[{"CG", "[", "\"\<A Contradiction\>\"", "]"}]}], 
  "\[IndentingNewLine]", "]"}]], "Input",
