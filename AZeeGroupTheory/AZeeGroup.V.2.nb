Cell[BoxData[
 RowBox[{"PR", "[", 
  RowBox[{
  "\"\<Claim: for representations of SU[3] it suffices to consider traceless \
tensors with upper and lower indices symmetrized.\>\"", ",", 
   "\[IndentingNewLine]", "\[IndentingNewLine]", "line", ",", "NL", ",", 
   "\"\<The tensors \>\"", ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"0", ",", "0"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"1", ",", "0"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"0", ",", "1"}], "}"}]}], "}"}], ",", 
   "\"\< are not considered in claim and may be trivially 0 for SU[3]. \>\"", 
   ",", "\[IndentingNewLine]", "\[IndentingNewLine]", "line", ",", "NL", ",", 
   "\"\<For \>\"", ",", 
   RowBox[{"$", "=", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"2", ",", "0"}], "}"}], "\[LongRightArrow]", 
     RowBox[{"T", "[", 
      RowBox[{"\[CurlyPhi]", ",", "\"\<uu\>\"", ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "j"}], "}"}]}], "]"}]}]}], ",", 
   "\[IndentingNewLine]", 
   "\"\< separate into symmetric and antisymmetric combinations: \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"T", "[", 
       RowBox[{"\[CurlyPhi]", ",", "\"\<u\>\"", ",", 
        RowBox[{"{", "\"\<{ij}\>\"", "}"}]}], "]"}], "->", 
      RowBox[{
       RowBox[{"T", "[", 
        RowBox[{"\[CurlyPhi]", ",", "\"\<uu\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "j"}], "}"}]}], "]"}], "+", 
       RowBox[{"T", "[", 
        RowBox[{"\[CurlyPhi]", ",", "\"\<uu\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "i"}], "}"}]}], "]"}]}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"T", "[", 
       RowBox[{"\[CurlyPhi]", ",", "\"\<u\>\"", ",", 
        RowBox[{"{", "\"\<[ij]\>\"", "}"}]}], "]"}], "->", 
      RowBox[{
       RowBox[{"T", "[", 
        RowBox[{"\[CurlyPhi]", ",", "\"\<uu\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "j"}], "}"}]}], "]"}], "-", 
       RowBox[{"T", "[", 
        RowBox[{"\[CurlyPhi]", ",", "\"\<uu\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "i"}], "}"}]}], "]"}]}]}]}], 
    "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<\[Bullet] Since \>\"", ",", 
   RowBox[{"$", "=", 
    RowBox[{
     RowBox[{"T", "[", 
      RowBox[{"\[CurlyPhi]", ",", "\"\<d\>\"", ",", 
       RowBox[{"{", "k", "}"}]}], "]"}], "->", 
     RowBox[{
      RowBox[{"T", "[", 
       RowBox[{"\[Epsilon]", ",", "\"\<ddd\>\"", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "j", ",", "k"}], "}"}]}], "]"}], 
      RowBox[{"T", "[", 
       RowBox[{"\[CurlyPhi]", ",", "\"\<u\>\"", ",", 
        RowBox[{"{", "\"\<[ij]\>\"", "}"}]}], "]"}]}]}]}], ",", 
   "\"\< which is not part of the representation. So we are left with \>\"", 
   ",", 
   RowBox[{"T", "[", 
    RowBox[{"\[CurlyPhi]", ",", "\"\<u\>\"", ",", 
     RowBox[{"{", "\"\<{ij}\>\"", "}"}]}], "]"}], ",", "\"\< as claimed.\>\"",
    ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<\[Bullet] Similar arguements for representations of \>\"", ",", 
   RowBox[{"{", 
    RowBox[{"0", ",", "2"}], "}"}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<For representations of \>\"", ",", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"1", ",", "1"}], "}"}], "\[LongRightArrow]", 
    RowBox[{"T", "[", 
     RowBox[{"\[CurlyPhi]", ",", "\"\<ud\>\"", ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "j"}], "}"}]}], "]"}]}], ",", 
   "\"\< make traceless by \>\"", ",", 
   RowBox[{
    RowBox[{"T", "[", 
     RowBox[{"\[CurlyPhi]", ",", "\"\<ud\>\"", ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "j"}], "}"}]}], "]"}], "->", 
    RowBox[{
     RowBox[{"T", "[", 
      RowBox[{"\[CurlyPhi]", ",", "\"\<ud\>\"", ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "j"}], "}"}]}], "]"}], "-", 
     RowBox[{"Tr", "[", 
      RowBox[{"T", "[", 
       RowBox[{"\[CurlyPhi]", ",", "\"\<ud\>\"", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "j"}], "}"}]}], "]"}], "]"}]}]}], ",", 
   "\[IndentingNewLine]", "\[IndentingNewLine]", "line", ",", "NL", ",", 
   "\"\<For p\[Rule]3: \>\"", ",", 
   RowBox[{"T", "[", 
    RowBox[{"\[CurlyPhi]", ",", "\"\<uuu\>\"", ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "j", ",", "k"}], "}"}]}], "]"}], ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<Let \>\"", ",", 
   RowBox[{"$", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"T", "[", 
        RowBox[{"\[CurlyPhi]", ",", "\"\<uu\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"\"\<{ij}\>\"", ",", "k"}], "}"}]}], "]"}], "->", 
       RowBox[{
        RowBox[{"T", "[", 
         RowBox[{"\[CurlyPhi]", ",", "\"\<uuu\>\"", ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "j", ",", "k"}], "}"}]}], "]"}], "+", 
        RowBox[{"T", "[", 
         RowBox[{"\[CurlyPhi]", ",", "\"\<uuu\>\"", ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "i", ",", "k"}], "}"}]}], "]"}]}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"T", "[", 
        RowBox[{"\[CurlyPhi]", ",", "\"\<uu\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"\"\<[ij]\>\"", ",", "k"}], "}"}]}], "]"}], "->", 
       RowBox[{
        RowBox[{"T", "[", 
         RowBox[{"\[CurlyPhi]", ",", "\"\<uuu\>\"", ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "j", ",", "k"}], "}"}]}], "]"}], "-", 
        RowBox[{"T", "[", 
         RowBox[{"\[CurlyPhi]", ",", "\"\<uuu\>\"", ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "i", ",", "k"}], "}"}]}], "]"}]}]}]}], "}"}]}], 
   ",", "\[IndentingNewLine]", "NL", ",", "\"\<\[Bullet] \>\"", ",", 
   RowBox[{"T", "[", 
    RowBox[{"\[CurlyPhi]", ",", "\"\<uu\>\"", ",", 
     RowBox[{"{", 
      RowBox[{"\"\<[ij]\>\"", ",", "k"}], "}"}]}], "]"}], ",", 
   "\"\< is not considered, since \>\"", ",", 
   RowBox[{
    RowBox[{
     RowBox[{"T", "[", 
      RowBox[{"\[Epsilon]", ",", "\"\<ddd\>\"", ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "j", ",", "l"}], "}"}]}], "]"}], 
     RowBox[{"T", "[", 
      RowBox[{"\[CurlyPhi]", ",", "\"\<uu\>\"", ",", 
       RowBox[{"{", 
        RowBox[{"\"\<[ij]\>\"", ",", "k"}], "}"}]}], "]"}]}], "->", 
    RowBox[{
     RowBox[{"T", "[", 
      RowBox[{"\[CurlyPhi]", ",", "\"\<du\>\"", ",", 
       RowBox[{"{", 
        RowBox[{"l", ",", "k"}], "}"}]}], "]"}], "[", 
     RowBox[{"CG", "[", "\"\<{1,1}\>\"", "]"}], "]"}]}], ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<\[Bullet] \>\"", ",", 
   RowBox[{
    RowBox[{"3", 
     RowBox[{"(", 
      RowBox[{"$", "=", 
       RowBox[{"T", "[", 
        RowBox[{"\[CurlyPhi]", ",", "\"\<uu\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"\"\<{ij}\>\"", ",", "k"}], "}"}]}], "]"}]}], ")"}]}], 
    "\[Rule]", 
    RowBox[{"Inactivate", "[", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"{", 
         RowBox[{"$", "+", 
          RowBox[{"(", 
           RowBox[{"$1", "=", 
            RowBox[{
             RowBox[{"$", "/.", 
              RowBox[{"\"\<{ij}\>\"", "\[Rule]", "\"\<{jk}\>\""}]}], "/.", 
             RowBox[{"k", "\[Rule]", "i"}]}]}], ")"}], "+", 
          RowBox[{"(", 
           RowBox[{"$2", "=", 
            RowBox[{
             RowBox[{"$", "/.", 
              RowBox[{"\"\<{ij}\>\"", "\[Rule]", "\"\<{ki}\>\""}]}], "/.", 
             RowBox[{"k", "\[Rule]", "j"}]}]}], ")"}]}], "}"}], "[", 
        RowBox[{"CG", "[", "\"\<symmetric\>\"", "]"}], "]"}], "+", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"$", "-", "$1"}], ")"}], "+", 
          RowBox[{"(", 
           RowBox[{"$", "-", "$2"}], ")"}]}], "}"}], "[", 
        RowBox[{"CG", "[", "\"\<anti-symmetric\>\"", "]"}], "]"}]}], ",", 
      "Plus"}], "]"}]}]}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
