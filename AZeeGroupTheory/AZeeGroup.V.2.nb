  RowBox[{"\"\<The dimension of SU[3] tensor {m,n} \>\"", ",", 
   RowBox[{
    RowBox[{"$", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"T", "[", 
        RowBox[{"\[CurlyPhi]", ",", "\"\<uuuddd\>\"", ",", 
         RowBox[{"{", 
          RowBox[{
           SubscriptBox["i", "1"], ",", "\[Ellipsis]", ",", 
           SubscriptBox["i", "m"], ",", 
           SubscriptBox["j", "1"], ",", "\[Ellipsis]", ",", 
           SubscriptBox["j", "n"]}], "}"}]}], "]"}], ",", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"T", "[", 
           RowBox[{"\[CurlyPhi]", ",", "\"\<uuuddd\>\"", ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "\[Ellipsis]", ",", 
              SubscriptBox["i", "m"], ",", "i", ",", "\[Ellipsis]", ",", 
              SubscriptBox["j", "n"]}], "}"}]}], "]"}], "\[Rule]", "0"}], 
         "}"}], "[", 
        RowBox[{"CG", "[", "\"\<traceless condition\>\"", "]"}], "]"}]}], 
      "}"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"ColumnForms", "[", 
        RowBox[{"#1", ",", "2"}], "]"}], "&"}], ")"}], "[", "$", "]"}]}], ",",
    "\[IndentingNewLine]", "NL", ",", 
   "\"\<Dimension of the irreducible representation {m,n} of SU[3]: \>\"", 
   ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"e", "[", "1", "]"}], "=", 
    RowBox[{
     RowBox[{"\[ScriptCapitalD]", "[", 
      RowBox[{"m_", ",", "n_"}], "]"}], "\[Rule]", 
