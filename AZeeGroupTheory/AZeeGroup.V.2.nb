         RowBox[{"(", 
          RowBox[{"m", "+", "2"}], ")"}], 
         RowBox[{"(", 
          RowBox[{"n", "+", "1"}], ")"}], 
         RowBox[{"(", 
          RowBox[{"n", "+", "2"}], ")"}]}]}], ")"}], "-", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"$", "/.", 
         RowBox[{"m", "\[Rule]", 
          RowBox[{"m", "-", "1"}]}]}], "/.", 
        RowBox[{"n", "\[Rule]", 
         RowBox[{"n", "-", "1"}]}]}], ")"}]}]}]}], ",", "\[IndentingNewLine]",
