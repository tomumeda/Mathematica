 RowBox[{"PR", "[", 
  RowBox[{
   RowBox[{"CO", "[", 
    RowBox[{
    "\"\<1. The \!\(\*SuperscriptBox[\(\[InvisiblePrefixScriptBase]\), \(3\)]\
\)H nucleus(pnn bound state) and the \!\(\*SuperscriptBox[\(\
\[InvisiblePrefixScriptBase]\), \(3\)]\)He nucleus(ppn bound state) are known \
to form an isospin 1/2 doublet.  Use isospin to pridict the ratio of the \
cross section for \>\"", ",", 
     RowBox[{
      RowBox[{"$0", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Ket", "[", "p", "]"}], "\[CenterDot]", 
           RowBox[{"Ket", "[", "d", "]"}]}], "\[Rule]", 
          RowBox[{
           RowBox[{
           "Ket", "[", 
            "\"\<\!\(\*SuperscriptBox[\(\[InvisiblePrefixScriptBase]\), \
\(3\)]\)H\>\"", "]"}], "\[CenterDot]", 
           RowBox[{"Ket", "[", 
            SuperscriptBox["\[Pi]", "\"\<+\>\""], "]"}]}]}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{"Ket", "[", "p", "]"}], "\[CenterDot]", 
           RowBox[{"Ket", "[", "d", "]"}]}], "\[Rule]", 
          RowBox[{
           RowBox[{
           "Ket", "[", 
            "\"\<\!\(\*SuperscriptBox[\(\[InvisiblePrefixScriptBase]\), \
\(3\)]\)He\>\"", "]"}], "\[CenterDot]", 
           RowBox[{"Ket", "[", " ", 
            SuperscriptBox["\[Pi]", "\"\<0\>\""], "]"}]}]}]}], "}"}]}], ";", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"ColumnForms", "[", 
          RowBox[{"#1", ",", "2"}], "]"}], "&"}], ")"}], "[", "$0", "]"}]}], 
     ",", "\"\<.\>\""}], "\[IndentingNewLine]", "]"}], ",", 
   "\[IndentingNewLine]", "line", ",", "NL", ",", 
   "\"\<Isospin assignments: \>\"", ",", 
   RowBox[{
    RowBox[{"$s", "=", 
     RowBox[{"$", "=", 
      RowBox[{
       RowBox[{"Ket", "/@", 
        RowBox[{"{", 
         RowBox[{
         "p", ",", "d", ",", 
          "\"\<\!\(\*SuperscriptBox[\(\[InvisiblePrefixScriptBase]\), \
\(3\)]\)H\>\"", ",", 
          "\"\<\!\(\*SuperscriptBox[\(\[InvisiblePrefixScriptBase]\), \
\(3\)]\)He\>\"", ",", 
          SuperscriptBox["\[Pi]", "\"\<+\>\""], ",", 
          SuperscriptBox["\[Pi]", "\"\<0\>\""]}], "}"}]}], "\[Rule]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Ket", "[", 
          RowBox[{
           RowBox[{"1", "/", "2"}], ",", 
           RowBox[{"1", "/", "2"}]}], "]"}], ",", 
         RowBox[{"Ket", "[", 
          RowBox[{"0", ",", "0"}], "]"}], ",", 
         RowBox[{"Ket", "[", 
          RowBox[{
           RowBox[{"1", "/", "2"}], ",", 
           RowBox[{
            RowBox[{"-", "1"}], "/", "2"}]}], "]"}], ",", 
         RowBox[{"Ket", "[", 
          RowBox[{
           RowBox[{"1", "/", "2"}], ",", 
           RowBox[{"1", "/", "2"}]}], "]"}], ",", 
         RowBox[{"Ket", "[", 
          RowBox[{"1", ",", "1"}], "]"}], ",", 
         RowBox[{"Ket", "[", 
          RowBox[{"1", ",", "0"}], "]"}]}], "}"}]}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"$spartIso", "=", 
     RowBox[{"$s", "=", 
      RowBox[{"Thread", "[", "$s", "]"}]}]}], ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"ColumnForms", "[", 
        RowBox[{"#1", ",", "2"}], "]"}], "&"}], ")"}], "[", "$s", "]"}]}], 
   ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"$spartIsoRev", "=", 
     RowBox[{"Reverse", "/@", "$spartIso"}]}], ";", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", "NL"}], ",", "\"\<The reactions: \>\"", ",", 
   RowBox[{
    RowBox[{"$", "=", "$0"}], ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"ColumnForms", "[", 
        RowBox[{"#1", ",", "2"}], "]"}], "&"}], ")"}], "[", "$", "]"}]}], ",",
    "\[IndentingNewLine]", "NL", ",", "\"\<The isospin representation: \>\"", 
   ",", "\[IndentingNewLine]", "yield", ",", 
   RowBox[{
    RowBox[{"$", "=", 
     RowBox[{"$", "/.", "$s"}]}], ";", 
    RowBox[{"$outputs", "=", 
     RowBox[{"$", "=", 
      RowBox[{"$", "/.", 
       RowBox[{"Plus", "\[Rule]", "CenterDot"}]}]}]}], ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"ColumnForms", "[", 
        RowBox[{"#1", ",", "2"}], "]"}], "&"}], ")"}], "[", "$", "]"}]}], ",",
    "\[IndentingNewLine]", "\[IndentingNewLine]", "NL", ",", 
   "\"\<The 0\[CirclePlus]1/2 representation of inputs is: \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"$input", "=", 
     RowBox[{"$s", "=", 
      RowBox[{
       RowBox[{"tuKetCBDecomposeInv", "[", 
        RowBox[{"0", ",", 
         RowBox[{"1", "/", "2"}], ",", 
         RowBox[{"1", "/", "2"}]}], "]"}], "//", "Last"}]}]}], ";", "$s"}], 
   ",", "\[IndentingNewLine]", "\[IndentingNewLine]", "NL", ",", 
   "\"\<The output decomposes to: \>\"", ",", "\[IndentingNewLine]", 
   RowBox[{"$2", "=", 
    RowBox[{"$", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"tuKetCBDecompose", "[", 
        RowBox[{
         RowBox[{"1", "/", "2"}], ",", "1", ",", 
         RowBox[{"1", "/", "2"}]}], "]"}], "//", 
       RowBox[{"tuRuleSelect", "[", 
        RowBox[{"Ket", "[", 
         RowBox[{
          RowBox[{"1", "/", "2"}], ",", 
          RowBox[{"1", "/", "2"}]}], "]"}], "]"}]}], "//", "First"}]}]}], ",",
    "\[IndentingNewLine]", "\[IndentingNewLine]", "NL", ",", 
   "\"\<The transition amplitudes where \[ScriptCapitalT] are defined by: \
\>\"", ",", "\[IndentingNewLine]", 
   RowBox[{"$", "=", 
    RowBox[{
     RowBox[{"Bra", "[", "\"\<output\>\"", "]"}], "\[CenterDot]", 
     "\[ScriptCapitalT]", "\[CenterDot]", 
     RowBox[{"Ket", "[", "\"\<input\>\"", "]"}]}]}], ",", 
   "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"$3", "=", 
    RowBox[{"$", "/.", 
     RowBox[{
      RowBox[{"Ket", "[", "\"\<input\>\"", "]"}], "\[Rule]", 
      RowBox[{"$input", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}]}]}], ",", "\[IndentingNewLine]", "NL",
    ",", "\"\<The isospin decomposition of the inputs\>\"", ",", 
   "\[IndentingNewLine]", "\[IndentingNewLine]", "yield", ",", 
   RowBox[{"$4", "=", 
    RowBox[{"$", "/.", 
     RowBox[{
      RowBox[{"Ket", "[", "\"\<input\>\"", "]"}], "\[Rule]", 
      RowBox[{"$2", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}]}]}], ",", "\[IndentingNewLine]", 
   "Yield", ",", 
   RowBox[{"$", "=", 
    RowBox[{"$3", "\[Rule]", 
     RowBox[{
      RowBox[{"{", "$4", "}"}], "[", 
      RowBox[{"CG", "[", "\"\<isospin decomposition\>\"", "]"}], "]"}]}]}], 
   ",", "\[IndentingNewLine]", "\[IndentingNewLine]", "NL", ",", 
   "\"\<For the two reaction products: \>\"", ",", 
   RowBox[{
    RowBox[{"$s", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Bra", "[", "\"\<output\>\"", "]"}], ",", 
         RowBox[{"Bra", "[", "\"\<output\>\"", "]"}]}], "}"}], "->", 
       RowBox[{"Last", "/@", 
        RowBox[{"(", 
         RowBox[{"$outputs", "//", "tuBraKetSwap"}], ")"}]}]}], "//", 
      "Thread"}]}], ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"ColumnForms", "[", 
        RowBox[{"#1", ",", "2"}], "]"}], "&"}], ")"}], "[", "$s", "]"}]}], 
   ",", "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{
    RowBox[{"$5", "=", 
     RowBox[{"$", "=", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"$", "/.", "#"}], "&"}], ",", "$s"}], "]"}]}]}], ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"ColumnForms", "[", 
        RowBox[{"#1", ",", "2"}], "]"}], "&"}], ")"}], "[", "$", "]"}]}], ",",
    "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{
    RowBox[{"$", "=", 
     RowBox[{"$", "//", 
      RowBox[{"expandDC", "[", "]"}]}]}], ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"ColumnForms", "[", 
        RowBox[{"#1", ",", "2"}], "]"}], "&"}], ")"}], "[", "$", "]"}]}], ",",
    "\[IndentingNewLine]", "\[IndentingNewLine]", "NL", ",", "\"\<Let \>\"", 
   ",", 
   RowBox[{"$s", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"Bra", "[", "a__", "]"}], "\[CenterDot]", 
        RowBox[{"Bra", "[", "b__", "]"}], "\[CenterDot]", "\[ScriptCapitalT]",
         "\[CenterDot]", 
        RowBox[{"Ket", "[", "b__", "]"}], "\[CenterDot]", 
        RowBox[{"Ket", "[", "a__", "]"}]}], "\[Rule]", 
       RowBox[{"BraKet", "[", "\[ScriptCapitalT]", "]"}]}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"Bra", "[", "a__", "]"}], "\[CenterDot]", 
        RowBox[{"Bra", "[", "b__", "]"}], "\[CenterDot]", "\[ScriptCapitalT]",
         "\[CenterDot]", 
        RowBox[{"Ket", "[", "c__", "]"}], "\[CenterDot]", 
        RowBox[{"Ket", "[", "d__", "]"}]}], "\[RuleDelayed]", 
       RowBox[{"0", "/;", 
        RowBox[{
         RowBox[{"{", "a", "}"}], "\[NotEqual]", 
         RowBox[{"{", "c", "}"}]}]}]}]}], "}"}]}], ",", "\[IndentingNewLine]",
    "Yield", ",", 
   RowBox[{
    RowBox[{"$", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"MapAt", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"#", "/.", "$s"}], "&"}], ",", "#", ",", "2"}], "]"}], 
          "&"}], ",", "$"}], "]"}], "/.", "$spartIsoRev"}], "/.", 
      RowBox[{"tuBraKetSwap", "[", 
       RowBox[{
        RowBox[{"tuRuleSelect", "[", 
         RowBox[{
          RowBox[{"{", "_", "}"}], ",", 
          RowBox[{"{", "}"}], ",", 
          RowBox[{"{", "p", "}"}]}], "]"}], "[", "$spartIsoRev", "]"}], 
       "]"}]}]}], ";", 
    RowBox[{"$", "//", "Framed"}]}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<We find that the ratio of the cross-section: \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"$", "=", 
     RowBox[{"$", "//", "tuRule"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"$", "=", 
     RowBox[{
      RowBox[{
       SuperscriptBox[
        RowBox[{"Abs", "[", 
         RowBox[{"$", "[", 
          RowBox[{"[", 
           RowBox[{"1", ",", "1"}], "]"}], "]"}], "]"}], "2"], "/", 
       SuperscriptBox[
        RowBox[{"Abs", "[", 
         RowBox[{"$", "[", 
          RowBox[{"[", 
           RowBox[{"2", ",", "1"}], "]"}], "]"}], "]"}], "2"]}], "->", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"Abs", "[", 
         RowBox[{"$", "[", 
          RowBox[{"[", 
           RowBox[{"1", ",", "2"}], "]"}], "]"}], "]"}], "2"], "/", 
       SuperscriptBox[
        RowBox[{"Abs", "[", 
         RowBox[{"$", "[", 
          RowBox[{"[", 
           RowBox[{"2", ",", "2"}], "]"}], "]"}], "]"}], "2"]}]}]}], ";", 
    RowBox[{"$", "//", "Framed"}]}]}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", "\[IndentingNewLine]", "]"}]], "Input",
