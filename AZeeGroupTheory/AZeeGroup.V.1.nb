         RowBox[{"1", ",", "0"}], "]"}], ",", 
        RowBox[{"Ket", "[", 
         RowBox[{"1", ",", 
          RowBox[{"-", "1"}]}], "]"}]}], "}"}]}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"$spartIso", "=", 
     RowBox[{"$", "=", 
      RowBox[{"Thread", "[", "$", "]"}]}]}], ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"ColumnForms", "[", 
        RowBox[{"#1", ",", "2"}], "]"}], "&"}], ")"}], "[", "$", "]"}]}], ",",
    "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"$spartIsoRev", "=", 
     RowBox[{"Reverse", "/@", "$spartIso"}]}], ";", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", "NL"}], ",", 
   "\"\<\[Bullet] Isospins of reactants: \>\"", ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"$isoreactants", "=", 
     RowBox[{"$", "=", 
      RowBox[{"$evalReaction", "/.", "$spartIso"}]}]}], ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"ColumnForms", "[", 
        RowBox[{"#1", ",", "2"}], "]"}], "&"}], ")"}], "[", "$", "]"}]}], ",",
