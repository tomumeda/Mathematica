 RowBox[{
  RowBox[{"PR1", "[", 
   RowBox[{"\"\<Higgs potential \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"W", "->", 
     RowBox[{"\[Mu]", ".", 
      SubscriptBox["H", "u"], ".", 
      SubscriptBox["H", "d"]}]}], ",", "\[IndentingNewLine]", "NL", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmpV", "=", 
     RowBox[{"V", "->", 
      RowBox[{
       RowBox[{
