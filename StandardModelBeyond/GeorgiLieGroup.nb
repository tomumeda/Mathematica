Cell[BoxData[
 RowBox[{"PR1", "[", 
  RowBox[{
  "\"\<Roots and Weights: properties: Let the generators of a simple Lie \
algebra, \>\"", ",", 
   RowBox[{"Xd", "[", "a", "]"}], ",", 
   "\"\<.  Find all (m->Rank) linear combinations of \>\"", ",", 
   RowBox[{"Xd", "[", "a", "]"}], ",", "\"\< that are diagonal, \>\"", ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Hd", "[", "i", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", "m"}], "}"}]}], "}"}], ",", 
   "\"\<, such that: \>\"", ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"Hd", "[", "i", "]"}], "\[ConjugateTranspose]"}], "==", 
        RowBox[{"Hd", "[", "i", "]"}]}], "->", 
       RowBox[{
        RowBox[{"Cdd", "[", 
         RowBox[{"i", ",", "a"}], "]"}], 
        RowBox[{"Xd", "[", "a", "]"}]}]}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"MCommutator", "[", 
        RowBox[{
         RowBox[{"Hd", "[", "i", "]"}], ",", 
         RowBox[{"Hd", "[", "j", "]"}]}], "]"}], "->", "0"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Tr", "[", 
        RowBox[{
         RowBox[{"Hd", "[", "i", "]"}], ".", 
         RowBox[{"Hd", "[", "j", "]"}]}], "]"}], "->", 
       RowBox[{
        RowBox[{"kd", "[", "D", "]"}], 
        RowBox[{"\[Delta]dd", "[", 
         RowBox[{"i", ",", "j"}], "]"}]}]}]}], "\[IndentingNewLine]", "}"}], "//",
     "Column"}], ",", "\[IndentingNewLine]", "NL", ",", "\"\<where \>\"", ",", 
   RowBox[{"subH", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"Hd", "[", "i", "]"}], ".", 
      RowBox[{"Ket", "[", 
       RowBox[{"{", 
        RowBox[{"\[Mu]", ",", "D"}], "}"}], "]"}]}], "->", 
     RowBox[{
      RowBox[{"\[Mu]d", "[", "i", "]"}], ".", 
      RowBox[{"Ket", "[", 
       RowBox[{"{", 
        RowBox[{"\[Mu]", ",", "D"}], "}"}], "]"}]}]}]}], ",", "and", ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Hd", "[", "i", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", "m"}], "}"}]}], "}"}], "->", 
       "\"\<Cartan subalgebra\>\""}], ",", 
      RowBox[{
       RowBox[{"\[Mu]d", "[", "i", "]"}], "->", "\"\<weights\>\""}], ",", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"\[Mu]d", "[", "i", "]"}], "}"}], "->", 
       "\"\<weight vector\>\""}]}], "}"}], "//", "Column"}]}], 
  "\[IndentingNewLine]", "]"}]], "Input",
