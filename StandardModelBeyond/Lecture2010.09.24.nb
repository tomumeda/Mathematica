Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"PR1", "[", 
    RowBox[{
    "\"\<The following is a Toy model presentation to show some features of \
SUSY.  Assume fields \>\"", ",", "\[IndentingNewLine]", 
     RowBox[{"tmp", "=", 
      RowBox[{"{", 
       RowBox[{"A", ",", 
        RowBox[{"\[Psi]d", "[", "\[Alpha]", "]"}]}], "}"}]}], ",", 
     "\"\< where \>\"", ",", 
     RowBox[{"tmp", "[", 
      RowBox[{"[", "1", "]"}], "]"}], ",", "\"\< is a scalar field and \>\"", 
     ",", 
     RowBox[{"tmp", "[", 
      RowBox[{"[", "2", "]"}], "]"}], ",", 
     "\"\< is a fermionic field.  We take the interaction Lagrangian (with \
coupling parameters {y,\[Lambda]}) to be\>\"", ",", "Yield", ",", 
     "\[IndentingNewLine]", 
     RowBox[{"tmpLi", "=", 
      RowBox[{
       SubscriptBox["\[ScriptCapitalL]", "int"], "->", 
       RowBox[{
        RowBox[{
         RowBox[{"-", "y"}], " ", "A", " ", 
         RowBox[{"(", 
          RowBox[{"\[Psi]", ".", "\[Psi]"}], ")"}]}], "-", 
        RowBox[{
         SuperscriptBox["\[Lambda]", "2"], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["A", "\[Dagger]"], ".", "A"}], ")"}], "2"]}]}]}]}],
      ",", "\[IndentingNewLine]", "NL", ",", 
     "\"\<and the mass Lagrangian to be\>\"", ",", "Yield", ",", 
     "\[IndentingNewLine]", 
     RowBox[{"tmpLm", "=", 
      RowBox[{
       SubscriptBox["\[ScriptCapitalL]", "mass"], "->", 
       RowBox[{
        RowBox[{
         SuperscriptBox[
          SubscriptBox["m", "A"], "2"], 
         RowBox[{
          SuperscriptBox["A", "\[Dagger]"], ".", "A"}]}], "-", 
        RowBox[{
         SubscriptBox["m", "\[Psi]"], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"\[Psi]", ".", "\[Psi]"}], "+", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"\[Psi]", ".", "\[Psi]"}], ")"}], "\[Dagger]"]}], 
          ")"}]}]}]}]}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     "NL", ",", "\"\<The self-energy diagram from :\>\"", ",", 
     RowBox[{"tmpLi", "[", 
      RowBox[{"[", 
       RowBox[{"2", ",", "2"}], "]"}], "]"}], ",", "yield", ",", 
     "\[IndentingNewLine]", 
     RowBox[{"Graphics20", "[", 
      SuperscriptBox["\[Lambda]", "2"], "]"}], ",", "\[IndentingNewLine]", 
     "NL", ",", "\"\<These self-energy terms cancel if \>\"", ",", " ", 
     RowBox[{"\[Lambda]", "==", "y"}], ",", "\"\< for \>\"", ",", 
     RowBox[{"\[Lambda]", "\[GreaterGreater]", "m"}]}], "\[IndentingNewLine]",
     "]"}], ";"}]}]], "Input",
