        RowBox[{
         RowBox[{"\[Delta]", "[", 
          RowBox[{"\[Psi]d", "[", 
           RowBox[{"undotted", "@", "\[Alpha]"}], "]"}], "]"}], "->", 
         RowBox[{
          SqrtBox["2"], "I", " ", 
          RowBox[{"\[Sigma]udd", "[", 
           RowBox[{"\[Mu]", ",", 
            RowBox[{"undotted", "@", "\[Alpha]"}], ",", 
            RowBox[{"dotted", "@", "\[Alpha]"}]}], "]"}], 
          OverscriptBox[
           RowBox[{"\[Xi]u", "[", 
            RowBox[{"dotted", "@", "\[Alpha]"}], "]"}], "_"], 
          RowBox[{"xPartialD", "[", 
           RowBox[{"A", ",", "\[Mu]"}], "]"}]}]}]}], "}"}]}], 
     "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", "New", ",", 
    "\"\<Lorentz transformation for spinors with Weyl basis.\>\"", ",", 
    "Yield", ",", "\[IndentingNewLine]", 
