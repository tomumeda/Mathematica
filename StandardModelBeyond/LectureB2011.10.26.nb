 RowBox[{"PR1", "[", 
  RowBox[{
  "\"\<R-parity violation implication on SUSY: \>\"", ",", "NL", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Hd", "[", "d", "]"}], ",", "L"}], "}"}], ",", 
   "\"\< become indistinguishable under gauge invariance, i.e., they have \
same 3-2-1 quantum numbers: (1,2,1/2).\n  Example of a R-parity violating \
potential terms: \>\"", ",", 
   RowBox[{
    RowBox[{"Wud", "[", 
     RowBox[{"\[CapitalDelta]L", ",", "RPV"}], "]"}], "->", 
    RowBox[{"Column", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Q", ".", 
        RowBox[{"Du", "[", "c", "]"}], ".", "L", ".", "\[Lambda]"}], ",", 
       RowBox[{
        RowBox[{"Hd", "[", "u", "]"}], ".", "L"}], ",", 
       RowBox[{
        RowBox[{"Ld", "[", 
         RowBox[{"a", "[", "flavor", "]"}], "]"}], ".", 
        RowBox[{"T", "[", 
         RowBox[{"\"\<E\>\"", ",", "\"\<ud\>\"", ",", 
          RowBox[{"{", 
           RowBox[{"c", ",", "c"}], "}"}]}], "]"}], ".", 
        RowBox[{"Ld", "[", "b", "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"Hd", "[", "d", "]"}], ".", "\"\<E\>\"", ".", 
          RowBox[{"Hd", "[", "d", "]"}]}], "->", 
         RowBox[{"0", "\[DoubleLeftArrow]", 
          RowBox[{"(", "\"\<SU[2]antisymmetry\>\"", ")"}]}]}], "}"}]}], "}"}],
      "]"}]}], ",", "NL", ",", "\[IndentingNewLine]", 
   "\"\<The Kahler terms: \>\"", ",", 
   RowBox[{"K", "->", 
    RowBox[{
     RowBox[{
      SuperscriptBox["Q", "\[Dagger]"], ".", "Q"}], "+", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"Hd", "[", "d", "]"}], "\[Dagger]"], ".", 
      RowBox[{"Hd", "[", "d", "]"}]}], "+", 
     RowBox[{
      SuperscriptBox["L", "\[Dagger]"], ".", "L"}], "+", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SuperscriptBox["L", "\[Dagger]"], ".", 
        RowBox[{"Hd", "[", "d", "]"}]}], "->", 
       RowBox[{"0", "\[DoubleLeftArrow]", "\"\<flavor violation\>\""}]}], 
      ")"}]}]}], ",", "NL", ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Wud", "[", 
     RowBox[{"\[CapitalDelta]B", ",", "RPV"}], "]"}], "->", 
