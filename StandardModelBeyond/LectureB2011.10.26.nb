     RowBox[{"Uud", "[", 
      RowBox[{"c", ",", "a"}], "]"}], ".", 
     RowBox[{"Dud", "[", 
      RowBox[{"c", ",", "b"}], "]"}], ".", 
     RowBox[{"Dud", "[", 
      RowBox[{"c", ",", "c"}], "]"}], ".", 
     RowBox[{"T", "[", 
      RowBox[{"\"\<\[Lambda]''\>\"", ",", "\"\<ddd\>\"", ",", 
       RowBox[{"{", 
        RowBox[{"a", ",", "b", ",", "c"}], "}"}]}], "]"}]}]}], ",", 
   "\"\< where \[Lambda]'' is antisymmetic in {a,b,c}. \>\"", ",", "NL", ",", 
