         RowBox[{"Vd", "[", "b", "]"}]}], "]"}]}]}]}], ",", " ", 
    "\[IndentingNewLine]", "\"\< Show that \>\"", ",", 
    RowBox[{"Fdd", "[", 
     RowBox[{"a", ",", "b"}], "]"}], ",", 
    "\"\< is simple \[DoubleLongLeftRightArrow] \>\"", ",", 
    RowBox[{"tmp0", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"\[Epsilon]uuuu", "[", 
          RowBox[{"a", ",", "b", ",", "c", ",", "d"}], "]"}], 
         RowBox[{"Fdd", "[", 
          RowBox[{"a", ",", "b"}], "]"}], 
         RowBox[{"Fdd", "[", 
          RowBox[{"c", ",", "d"}], "]"}]}], "->", "0"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         SuperscriptBox[
          RowBox[{"Fuu", "[", 
           RowBox[{"a", ",", "b"}], "]"}], "*"], 
         RowBox[{"Fdd", "[", 
          RowBox[{"a", ",", "b"}], "]"}]}], "->", "0"}]}], "}"}]}], ",", 
    "\"\< where * represents the dual is defined by: \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"subdualF", "=", 
