Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"PR1", "[", 
   RowBox[{
   "\"\<A left-handed chiral superfield is one that satisfies: \>\"", ",", 
    RowBox[{
     RowBox[{"sub\[CapitalPhi]y", "=", 
      RowBox[{
       RowBox[{"\[CapitalPhi]", "[", 
        RowBox[{
         RowBox[{"xu", "[", "\[Mu]", "]"}], ",", "\[Theta]", ",", 
         OverscriptBox["\[Theta]", "_"]}], "]"}], "->", 
       RowBox[{"\[CapitalPhi]", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"yu", "[", "\[Mu]", "]"}], "[", 
          RowBox[{
           RowBox[{"xu", "[", "\[Mu]", "]"}], ",", 
           RowBox[{"\[Theta]u", "[", 
            RowBox[{"d0", "@", "\[Alpha]"}], "]"}], ",", 
           RowBox[{"OBu", "[", 
            RowBox[{"\[Theta]", ",", 
             RowBox[{"d1", "@", "\[Alpha]"}]}], "]"}]}], "]"}], ",", 
         RowBox[{"\[Theta]u", "[", 
          RowBox[{"d0", "@", "\[Alpha]"}], "]"}], ",", 
         RowBox[{"OBu", "[", 
          RowBox[{"\[Theta]", ",", 
           RowBox[{"d1", "@", "\[Alpha]"}]}], "]"}]}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"tmp", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"subSQD", "[", 
         RowBox[{"[", 
          RowBox[{"4", ",", "1"}], "]"}], "]"}], ".", 
        RowBox[{"sub\[CapitalPhi]y", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], "->", "0"}]}]}], ",", 
    "\[IndentingNewLine]", "NL", ",", "\"\<To solve this define \>\"", ",", 
    RowBox[{"suby", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"yu", "[", "\[Mu]_", "]"}], "[", 
       RowBox[{
        RowBox[{"xu", "[", "\[Mu]_", "]"}], ",", 
        RowBox[{"\[Theta]u", "[", 
         RowBox[{"d0", "@", "\[Alpha]"}], "]"}], ",", 
        RowBox[{"OBu", "[", 
         RowBox[{"\[Theta]", ",", 
          RowBox[{"d1", "@", "\[Alpha]"}]}], "]"}]}], "]"}], "->", 
      RowBox[{
       RowBox[{"xu", "[", "\[Mu]", "]"}], "+", 
       RowBox[{"I", " ", 
        RowBox[{
         RowBox[{"\[Theta]u", "[", 
          RowBox[{"d0", "@", "\[Alpha]"}], "]"}], ".", 
         RowBox[{"\[Sigma]udd", "[", 
          RowBox[{"\[Mu]", ",", 
           RowBox[{"d0", "@", "\[Alpha]"}], ",", 
           RowBox[{"d1", "@", "\[Alpha]"}]}], "]"}], ".", 
         RowBox[{"OBu", "[", 
          RowBox[{"\[Theta]", ",", 
           RowBox[{"d1", "@", "\[Alpha]"}]}], "]"}]}]}]}]}]}], ",", "Imply", 
    ",", "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"tmp", "/.", "sub\[CapitalPhi]y"}]}], ",", "Yield", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"tmp", "/.", "subSQD"}]}], ",", "Yield", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"tmp", "/.", "DotExpand"}], "/.", 
       RowBox[{"simpleDot2", "[", 
        RowBox[{"{", "}"}], "]"}]}], "/.", 
      RowBox[{
       RowBox[{
        RowBox[{"xPartialD", "[", 
         RowBox[{"_", ",", "a_"}], "]"}], ".", "b_"}], "->", 
       RowBox[{"xPartialD", "[", 
        RowBox[{"b", ",", "a"}], "]"}]}]}]}], ",", "\[IndentingNewLine]", 
    "NL", ",", "\"\<Expand derivative by rule: \>\"", ",", 
    RowBox[{"sub", "=", 
     RowBox[{
      RowBox[{"xPartialD", "[", 
       RowBox[{
        RowBox[{"\[CapitalPhi]", "[", 
         RowBox[{"a_", ",", "b_", ",", "c_"}], "]"}], ",", "d_"}], "]"}], "->", 
      RowBox[{
       RowBox[{
        RowBox[{"xPartialD", "[", 
         RowBox[{
          RowBox[{"\[CapitalPhi]", "[", 
           RowBox[{"a", ",", "b", ",", "c"}], "]"}], ",", "a"}], "]"}], 
        RowBox[{"xPartialD", "[", 
         RowBox[{"a", ",", "d"}], "]"}]}], "+", 
       RowBox[{
        RowBox[{"xPartialD", "[", 
         RowBox[{
          RowBox[{"\[CapitalPhi]", "[", 
           RowBox[{"a", ",", "b", ",", "c"}], "]"}], ",", "b"}], "]"}], 
        RowBox[{"xPartialD", "[", 
         RowBox[{"b", ",", "d"}], "]"}]}], "+", 
       RowBox[{
        RowBox[{"xPartialD", "[", 
         RowBox[{
          RowBox[{"\[CapitalPhi]", "[", 
           RowBox[{"a", ",", "b", ",", "c"}], "]"}], ",", "c"}], "]"}], 
        RowBox[{"xPartialD", "[", 
         RowBox[{"c", ",", "d"}], "]"}]}]}]}]}], ",", "Yield", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"tmp", "/.", "sub"}]}], ",", "\[IndentingNewLine]", "NL", ",", 
    "\"\<Explicit derivative evaluation: \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"sub", "=", 
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"xxPartialD", "[", 
         RowBox[{"a_", ",", "b_"}], "]"}], ":>", 
        RowBox[{"1", "/;", 
         RowBox[{
          RowBox[{
           RowBox[{"Head", "[", "a", "]"}], "===", 
           RowBox[{"yu", "[", "\[Mu]", "]"}]}], "&&", 
          RowBox[{"b", "===", "\[Mu]"}]}]}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"xPartialD", "[", 
         RowBox[{"a_", ",", "\[Mu]"}], "]"}], ":>", 
        RowBox[{"0", "/;", 
         RowBox[{"FreeQ", "[", 
          RowBox[{"a", ",", 
           RowBox[{"xu", "[", "\[Mu]", "]"}]}], "]"}]}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"xPartialD", "[", 
         RowBox[{"a_", ",", "b_"}], "]"}], ":>", 
        RowBox[{"0", "/;", 
         RowBox[{
          RowBox[{"FreeQ", "[", 
           RowBox[{"a", ",", "OverBar"}], "]"}], "&&", 
          RowBox[{"!", 
           RowBox[{"FreeQ", "[", 
            RowBox[{"b", ",", "OverBar"}], "]"}]}], "&&", 
          RowBox[{
           RowBox[{"Head", "[", "a", "]"}], "=!=", 
           RowBox[{"yu", "[", "\[Mu]", "]"}]}]}]}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"xPartialD", "[", 
         RowBox[{"a_", ",", "b_"}], "]"}], ":>", 
        RowBox[{"0", "/;", 
         RowBox[{
          RowBox[{"!", 
           RowBox[{"FreeQ", "[", 
            RowBox[{"a", ",", "OverBar"}], "]"}]}], "&&", 
          RowBox[{"FreeQ", "[", 
           RowBox[{"b", ",", "OverBar"}], "]"}], "&&", 
          RowBox[{
           RowBox[{"Head", "[", "a", "]"}], "=!=", 
           RowBox[{"yu", "[", "\[Mu]", "]"}]}]}]}]}]}], "\[IndentingNewLine]",
       "}"}]}], ",", "Yield", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"tmp", "/.", "sub"}]}], ",", "Yield", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{
      RowBox[{"tmp", "/.", 
       RowBox[{"simpleDot2", "[", 
        RowBox[{"{", 
         RowBox[{"xPartialD", "[", 
          RowBox[{
           RowBox[{"\[CapitalPhi]", "[", 
            RowBox[{"_", ",", "_", ",", "_"}], "]"}], ",", "_"}], "]"}], 
         "}"}], "]"}]}], "//", "Simplify"}]}], ",", "\[IndentingNewLine]", 
    "NL", ",", 
    "\"\<We have already shown that the terms of the derivative of y are \
zero: \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{
      RowBox[{"tmp", "/.", 
       RowBox[{
        RowBox[{"xPartialD", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"yu", "[", "_", "]"}], "[", 
           RowBox[{"_", ",", "_", ",", "_"}], "]"}], ",", "_"}], "]"}], "->", 
        "0"}]}], "/.", 
      RowBox[{"simpleDot2", "[", 
       RowBox[{"{", "}"}], "]"}]}]}], ",", "\[IndentingNewLine]", "NL", ",", 
    "\"\<Indicating that \>\"", ",", "\[CapitalPhi]", ",", 
    "\"\< is not a function of \>\"", ",", 
    RowBox[{"OBu", "[", 
     RowBox[{"\[Theta]", ",", 
      RowBox[{"d1", "@", "\[Alpha]"}]}], "]"}], ",", "\[IndentingNewLine]", 
    "NL", ",", "\"\<So \>\"", ",", 
    RowBox[{"subSQD", "//", "Column"}], ",", 
    "\"\< are charge operator in SUSY along with the translation charge \
operator \>\"", ",", 
    RowBox[{"Pd", "[", "\[Mu]", "]"}], ",", "\[IndentingNewLine]", 
    "\"\< and we can find Superfields \[CapitalPhi] that are separable into \
the {y,\[Theta]} or the {y,\!\(\*OverscriptBox[\(\[Theta]\), \(_\)]\)} \
domains by imposing the conditions \!\(\*OverscriptBox[\(D\), \(_\)]\).\
\[CapitalPhi]->0 or D.\[CapitalPhi]->0 .\>\""}], "\[IndentingNewLine]", "]"}],
