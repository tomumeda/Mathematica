Cell[BoxData[
 RowBox[{
  RowBox[{"PR1", "[", 
   RowBox[{
   "\"\<Unified Gauge Coupling SU[5] at high energy \
(\!\(\*SuperscriptBox[\(10\), \(16\)]\)GeV) \[DoubleLongRightArrow] Baryon \
violation\nExtropolating 3-2-1 coupling parameters {\[Alpha]'s} from MeV \
scale to \!\(\*SuperscriptBox[\(10\), \(16\)]\)GeV the renormalization method \
predicts that the coupling constant value converge.  For 1-loop corrections \
{fermion, gauge, scalar} to propagator the evolution of coupling constants \
{3-2-1} behave as \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp\[Beta]", "=", 
     RowBox[{
      RowBox[{"xPartialD", "[", 
       RowBox[{
        RowBox[{"gd", "[", "i", "]"}], ",", "t"}], "]"}], "->", 
      RowBox[{
       FractionBox[
        RowBox[{"bd", "[", "i", "]"}], 
        RowBox[{"16", " ", 
         SuperscriptBox["\[Pi]", "2"]}]], 
       SuperscriptBox[
        RowBox[{"gd", "[", "i", "]"}], "3"]}]}]}], ",", 
    "\[IndentingNewLine]", "\"\< where t->Log[E] {T==Casimir operator} \>\"", 
    ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"bd", "[", "i", "]"}], "->", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        FractionBox["11", "3"]}], 
       RowBox[{"Td", "[", "vector", "]"}]}], "+", 
      RowBox[{
       FractionBox["2", "3"], 
       RowBox[{"Td", "[", "fermion", "]"}]}], "+", 
      RowBox[{
       FractionBox["1", "3"], 
       RowBox[{"Td", "[", "scalar", "]"}]}]}]}], ",", "\[IndentingNewLine]", 
    "\"\< SUSY \[DoubleLongRightArrow] \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"bd", "[", "i", "]"}], "->", 
     RowBox[{
      RowBox[{
       RowBox[{"-", "3"}], " ", 
       RowBox[{"Td", "[", "vector", "]"}]}], "+", 
      RowBox[{"Td", "[", 
       RowBox[{"\[Chi]", "[", 
        RowBox[{"chiral", " ", "multiplier"}], "]"}], "]"}]}]}], ",", 
    "\[IndentingNewLine]", "\"\< Covariant derivative operator \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"xCovariantD", "[", 
      RowBox[{"a_", ",", "\[Mu]_"}], "]"}], "->", 
     RowBox[{
      RowBox[{"xPartialD", "[", 
       RowBox[{"a", ",", "\[Mu]"}], "]"}], "+", 
      RowBox[{"I", " ", 
       RowBox[{"gd", "[", "3", "]"}], " ", 
       RowBox[{"Tdu", "[", 
        RowBox[{"3", ",", "A"}], "]"}], " ", 
       RowBox[{"Adu", "[", 
        RowBox[{"\[Mu]", ",", "A"}], "]"}]}], "+", 
      RowBox[{"I", " ", 
       RowBox[{"gd", "[", "2", "]"}], " ", 
       RowBox[{"Tdu", "[", 
        RowBox[{"2", ",", "a"}], "]"}], " ", 
       RowBox[{"Wdu", "[", 
        RowBox[{"\[Mu]", ",", "a"}], "]"}]}], "+", 
      RowBox[{"I", " ", 
       RowBox[{"gd", "[", "1", "]"}], " ", "Y", " ", 
       RowBox[{"Bd", "[", "\[Mu]", "]"}]}]}]}], ",", "\[IndentingNewLine]", 
    "\"\< where the normalization of \>\"", ",", 
    RowBox[{
     RowBox[{"gd", "[", "1", "]"}], " ", "Y"}], " ", ",", 
    "\"\< is not fixed in general (hyper-charge).\nIn SU[5] the covariant \
derivative is \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"xCovariantD", "[", 
      RowBox[{"a_", ",", "\[Mu]_"}], "]"}], "->", 
     RowBox[{
      RowBox[{"xPartialD", "[", 
       RowBox[{"a", ",", "\[Mu]"}], "]"}], "+", 
      RowBox[{"I", " ", 
       RowBox[{
        RowBox[{"gd", "[", "5", "]"}], " ", ".", 
        RowBox[{"Tdu", "[", 
         RowBox[{"5", ",", "A"}], "]"}], ".", " ", 
        RowBox[{"Adu", "[", 
         RowBox[{"\[Mu]", ",", 
          RowBox[{"A", "[", 
           RowBox[{"{", 
            RowBox[{"1", ",", "\[CenterEllipsis]", ",", "12"}], "}"}], 
           "]"}]}], "]"}]}]}], "+", 
      RowBox[{
       RowBox[{"Tdu", "[", 
        RowBox[{"5", ",", "24"}], "]"}], ".", "B"}]}]}], ",", 
    "\[IndentingNewLine]", "\"\< where \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Tdu", "[", 
      RowBox[{"5", ",", "A"}], "]"}], "->", 
     RowBox[{"MatrixForm", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"\[Lambda]u", "[", "a", "]"}], ".", 
           RowBox[{"Guu", "[", 
            RowBox[{"\[Mu]", ",", "a"}], "]"}]}], ",", "X"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"X", ",", 
          RowBox[{
           RowBox[{
            RowBox[{"Tu", "[", "i", "]"}], ".", 
            RowBox[{"Wu", "[", "i", "]"}]}], "/", "2"}]}], "}"}]}], "}"}], 
      "]"}]}], ",", "and", ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Tdu", "[", 
      RowBox[{"5", ",", "24"}], "]"}], "->", 
     RowBox[{
      FractionBox["1", 
       SqrtBox["60"]], 
      RowBox[{"MatrixForm", "[", 
       RowBox[{"SparseArray", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"Band", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"1", ",", "1"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"3", ",", "3"}], "}"}]}], "]"}], "->", 
            RowBox[{"-", "2"}]}], ",", 
           RowBox[{
            RowBox[{"Band", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"4", ",", "4"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"5", ",", "5"}], "}"}]}], "]"}], "->", "3"}]}], "}"}], 
         ",", 
         RowBox[{"{", 
          RowBox[{"5", ",", "5"}], "}"}]}], "]"}], "]"}]}]}], ",", 
    "\[IndentingNewLine]", 
    "\"\< are 5x5 matrices where we normalize such that \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Tr", "[", 
      RowBox[{
       RowBox[{"Tdu", "[", 
        RowBox[{"5", ",", "a"}], "]"}], ".", 
       RowBox[{"Tdu", "[", 
        RowBox[{"5", ",", "b"}], "]"}]}], "]"}], "->", 
     RowBox[{"C", " ", 
