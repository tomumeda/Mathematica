       FractionBox[
        RowBox[{"-", "\[Alpha]"}], "r"], 
       RowBox[{"Tudd", "[", 
        RowBox[{"i", ",", "A", ",", "C"}], "]"}], 
       RowBox[{"Tudd", "[", 
        RowBox[{"i", ",", "B", ",", "D"}], "]"}]}]}]}]}], ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<The total potential, \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"tV", "=", 
    RowBox[{"AddRules", "[", 
     RowBox[{"{", 
      RowBox[{"tVgauge", ",", "tVsplit"}], "}"}], "]"}]}], ",", "yield", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"tV", "=", 
    RowBox[{
     RowBox[{"Vdddd", "[", 
