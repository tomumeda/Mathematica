   RowBox[{"eA51", "=", 
    RowBox[{"t\[ScriptCapitalL]int", "=", 
     RowBox[{
      SubscriptBox["\[ScriptCapitalL]", "int"], "->", 
      RowBox[{"g", ".", 
       OverscriptBox[
        RowBox[{"\[Chi]d", "[", "A", "]"}], "_"], ".", 
       OverscriptBox[
        RowBox[{"\[Sigma]u", "[", "\[Mu]", "]"}], "_"], ".", 
       RowBox[{"\[Chi]d", "[", "B", "]"}], ".", 
