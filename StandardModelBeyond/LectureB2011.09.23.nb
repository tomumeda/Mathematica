Cell[BoxData[
 RowBox[{"PR1", "[", 
  RowBox[{
  "\"\<How to make SUSY Lagrangian gauge invariant, i.e., under transform: \>\
\"", ",", "\[IndentingNewLine]", 
   RowBox[{"tmp", "=", 
    RowBox[{"\[Phi]", "->", 
     RowBox[{
      RowBox[{"E", "[", 
       RowBox[{"I", " ", 
        RowBox[{"Q", ".", "\[CapitalLambda]"}]}], "]"}], ".", "\[Phi]"}]}]}], 
   ",", "\"\< where we can expand \>\"", ",", "\[IndentingNewLine]", 
   RowBox[{"\[Phi]", "->", 
    RowBox[{"A", " ", "+", " ", 
     RowBox[{"\[Theta]", ".", "\[Psi]"}], "+", "\[CenterEllipsis]"}]}], ",", 
   "NL", ",", "\[IndentingNewLine]", 
   "\"\<If we modify the scalar kinetic term \>\"", ",", 
   RowBox[{"tmp1", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["\[Phi]", "\[Dagger]"], ".", "\[Phi]"}], ")"}], "[", 
      RowBox[{"\[Theta]", " ", "\[Theta]", " ", 
       OverscriptBox["\[Theta]", "_"], " ", 
       OverscriptBox["\[Theta]", "_"]}], "]"}], "->", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["\[Phi]", "\[Dagger]"], ".", 
        RowBox[{"Exp", "[", 
         RowBox[{"g", " ", 
          RowBox[{"Q", ".", "V"}]}], "]"}], ".", "\[Phi]"}], ")"}], "[", 
      RowBox[{"\[Theta]", " ", "\[Theta]", " ", 
       OverscriptBox["\[Theta]", "_"], " ", 
       OverscriptBox["\[Theta]", "_"]}], "]"}]}]}], ",", 
   "\[IndentingNewLine]", 
   "\"\< where vector superfield V \[Element] Real, \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"tmpV", "=", 
    RowBox[{"V", "->", 
     RowBox[{"\[CenterEllipsis]", "+", 
      RowBox[{"\[Theta]", ".", 
       RowBox[{"\[Sigma]u", "[", "\[Mu]", "]"}], ".", 
       OverscriptBox["\[Theta]", "_"], ".", 
       RowBox[{
        RowBox[{"Ad", "[", "\[Mu]", "]"}], "[", "x", "]"}]}], "+", 
      RowBox[{
       RowBox[{
        OverscriptBox["\[Theta]", "_"], ".", 
        OverscriptBox["\[Theta]", "_"], ".", "\[Theta]"}], " ", 
       RowBox[{"\[Lambda]", "[", "x", "]"}]}], "+", 
      RowBox[{"\[Theta]", " ", ".", "\[Theta]", ".", 
       OverscriptBox["\[Theta]", "_"], ".", 
       OverscriptBox["\[Lambda]", "_"]}], "+", 
      RowBox[{"\[Theta]", " ", ".", "\[Theta]", ".", 
       OverscriptBox["\[Theta]", "_"], ".", 
       OverscriptBox["\[Theta]", "_"], ".", 
       RowBox[{"Dd", "[", "x", "]"}]}]}]}]}], ",", "NL", ",", 
   "\[IndentingNewLine]", 
   "\"\<If we convert the kinetic terms to use covariant derivatives \>\"", 
   ",", "\[IndentingNewLine]", 
   RowBox[{"tmp", "=", 
    RowBox[{
     SubscriptBox["\[ScriptCapitalL]", "kinetic"], "->", 
     RowBox[{
      RowBox[{
       SuperscriptBox[
        RowBox[{"xCovariantDu", "[", 
         RowBox[{"A", ",", "\[Mu]"}], "]"}], "\[Dagger]"], ".", 
       RowBox[{"xCovariantD", "[", 
        RowBox[{"A", ",", "\[Mu]"}], "]"}]}], "+", 
      RowBox[{"I", " ", 
       RowBox[{"\[Psi]", ".", 
        RowBox[{"\[Sigma]u", "[", "\[Mu]", "]"}], ".", 
        RowBox[{"xCovariantD", "[", 
         RowBox[{
          OverscriptBox["\[Psi]", "_"], ",", "\[Mu]"}], "]"}]}]}]}]}]}], ",", 
   "\[IndentingNewLine]", "\"\< where \>\"", ",", " ", 
   RowBox[{
    RowBox[{"xCovariantDu", "[", 
     RowBox[{"_", ",", "\[Mu]"}], "]"}], "->", 
    RowBox[{
     RowBox[{"xPartialDu", "[", 
      RowBox[{"_", ",", "\[Mu]"}], "]"}], "-", 
     RowBox[{"I", " ", "g", " ", "Q", " ", 
      RowBox[{"Au", "[", "\[Mu]", "]"}]}]}]}], ",", "\[IndentingNewLine]", 
   "\"\< results in potential \>\"", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{
     SubscriptBox["V", 
      SuperscriptBox["D", "2"]], "->", 
     RowBox[{
      FractionBox[
       RowBox[{"g", " ", "g"}], "2"], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["A", "\[Dagger]"], ".", "Q", ".", "A"}], ")"}], 
       "2"]}]}]}], ",", "\[IndentingNewLine]", 
   "\"\< and Lagrangian gauge term \>\"", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{
     SubscriptBox["\[ScriptCapitalL]", "gauge"], "->", 
     RowBox[{
      SqrtBox["2"], "g", " ", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"Ad", "[", "i", "]"}], "\[Dagger]"], ".", 
       RowBox[{"Tudd", "[", 
        RowBox[{"a", ",", "i", ",", "j"}], "]"}], ".", 
       RowBox[{"\[Psi]ud", "[", 
        RowBox[{"a", ",", "j"}], "]"}], ".", 
       RowBox[{"Au", "[", "a", "]"}]}]}]}]}]}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", "\[IndentingNewLine]", "]"}]], "Input",
