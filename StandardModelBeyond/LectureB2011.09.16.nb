Cell[BoxData[
 RowBox[{
  RowBox[{"PR1", "[", 
   RowBox[{
   "\"\<The prescription for getting a SUSY lagrangian from the SM is \
changing the SM fields with SuperFields, for example, \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[ScriptCapitalL]", "SM"], "==", 
      RowBox[{
       RowBox[{"qd", "[", "i", "]"}], ".", 
       RowBox[{"ydd", "[", 
        RowBox[{"i", ",", "j"}], "]"}], ".", 
       RowBox[{"uud", "[", 
        RowBox[{"c", ",", "j"}], "]"}], ".", 
       RowBox[{"h", "\[LongRightArrow]", 
        SubscriptBox["\[ScriptCapitalL]", "SUSY"]}]}]}], "->", 
     RowBox[{
      RowBox[{"Qd", "[", "i", "]"}], ".", 
      RowBox[{"ydd", "[", 
       RowBox[{"i", ",", "j"}], "]"}], ".", 
      RowBox[{"Uud", "[", 
       RowBox[{"c", ",", "j"}], "]"}], ".", "H"}]}], ",", "NL", ",", 
    "\[IndentingNewLine]", 
    "\"\<The coupling constants are extracted by comparing \
\!\(\*SubscriptBox[\(W\), \(int\)]\) with SM terms. For example, the \
potential term, \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"W", "->", 
      RowBox[{
       OverscriptBox[
        RowBox[{"qd", "[", "i", "]"}], "~"], ".", 
       RowBox[{"ydd", "[", 
        RowBox[{"i", ",", "j"}], "]"}], ".", 
       RowBox[{"uud", "[", 
        RowBox[{"c", ",", "j"}], "]"}], ".", "h"}]}]}], " ", ",", 
    "\"\< would yield the coefficient: \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", " ", 
     RowBox[{
      RowBox[{"xPartialD", "[", 
       RowBox[{
        SuperscriptBox["W", "2"], ",", 
        RowBox[{"{", 
         RowBox[{
          OverscriptBox[
           RowBox[{"qd", "[", "i", "]"}], "~"], ",", 
          RowBox[{"uud", "[", 
           RowBox[{"c", ",", "j"}], "]"}]}], "}"}]}], "]"}], "->", 
      RowBox[{
       RowBox[{"ydd", "[", 
        RowBox[{"i", ",", "j"}], "]"}], ".", "h"}]}]}], ",", 
    "\[IndentingNewLine]", "NL", ",", 
    "\"\<multiplying by corresponding superpartners of the derivatives yield \
the SUSY interaction term: \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"qd", "[", "i", "]"}], ".", 
     RowBox[{"tmp", "[", 
      RowBox[{"[", "2", "]"}], "]"}], ".", 
     RowBox[{"Tensor", "[", 
      RowBox[{
       OverscriptBox["u", "~"], ",", 
       RowBox[{"{", 
        RowBox[{"c", ",", "Void"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"Void", ",", "j"}], "}"}]}], "]"}]}], ",", 
    "\[IndentingNewLine]", "NL", ",", "\"\<The SUSY lagrangian for the \>\"", 
    ",", 
    RowBox[{"ydd", "[", 
     RowBox[{"i", ",", "j"}], "]"}], ",", 
    "\"\< coupling cooresponds to 3 diagrams: \>\"", ",", 
    "\[IndentingNewLine]", ",", 
    "\"\< where the vertex has 2 fermions and 1 scalar of the possible \
interacting superfields. \>\""}], "\[IndentingNewLine]", "]"}], 
  ";"}]], "Input",
