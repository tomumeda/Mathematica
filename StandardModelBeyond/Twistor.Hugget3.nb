   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\[Lambda]", " ", 
       RowBox[{
        RowBox[{"\[Pi]d", "[", 
         SuperscriptBox["A", "+"], "]"}], ".", 
        RowBox[{"\[Pi]d", "[", 
         SuperscriptBox["B", "+"], "]"}], ".", " ", "\[Ellipsis]"}]}], " ", 
      "\[VerticalSeparator]", " ", 
      RowBox[{"\[Lambda]", "\[Element]", "\[DoubleStruckCapitalC]"}]}], "}"}],
     "->", 
