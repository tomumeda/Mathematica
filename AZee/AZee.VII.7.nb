Cell[BoxData[
 RowBox[{
  RowBox[{"PR1", "[", 
   RowBox[{
   "\"\<Generalizing spinors to d-dimensional space.  For n\[Element]\
\[DoubleStruckCapitalN] \[Exists] Hermitian {\[Gamma]d[i],{i,2n}} such that \
there is a Clifford algebra: \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"e1", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"ACom", "[", 
         RowBox[{
          RowBox[{"\[Gamma]d", "[", "i", "]"}], ",", 
          RowBox[{"\[Gamma]d", "[", "j", "]"}]}], "]"}], "->", 
        RowBox[{"2", 
         RowBox[{"\[Delta]dd", "[", 
          RowBox[{"i", ",", "j"}], "]"}]}]}], ",", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"\[Gamma]d", "[", "i", "]"}], ")"}], "\[Dagger]"], "->", 
        RowBox[{"\[Gamma]d", "[", "i", "]"}]}]}], "}"}]}], ",", "NL", ",", 
    "\[IndentingNewLine]", 
    "\"\<These are the same commutation relations as ones for the generators \
of SO[2n].\>\"", ",", "NL", ",", "\[IndentingNewLine]", 
    "\"\<The corresponding base-dimension \>\"", ",", 
    RowBox[{"d", "->", 
     RowBox[{"2", " ", "n"}]}], ",", 
    "\"\< is a subset of the Clifford algebra\>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"CR", "[", 
     RowBox[{"\"\< which has dimension \>\"", ",", " ", 
      RowBox[{"2", "^", 
       RowBox[{"(", 
        RowBox[{"2", "n"}], ")"}]}], ",", 
      "\"\< or independent components (includes the a\[Wedge]b type).  In \
spinor representation there are \>\"", ",", 
      RowBox[{"2", "^", 
       RowBox[{"(", 
        RowBox[{"2", " ", "n"}], ")"}]}], ",", 
      "\"\< elements in the \[Gamma] matrices. Is there a \
correspondence?\>\""}], "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]",
    "]"}], ";"}]], "Input",
