Cell[BoxData[{
 RowBox[{"PR1", "[", 
  RowBox[{
  "\"\<SO[N] special orthogonal group of N by N real matrices O such that \
\>\"", ",", "\[IndentingNewLine]", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"Transpose", "[", "O", "]"}], ".", " ", "O"}], "->", 
      RowBox[{"Id", "[", "N", "]"}]}], ",", 
     RowBox[{
      RowBox[{"Det", "[", "O", "]"}], "->", "1"}]}], "}"}], ",", "NL", ",", 
   "\[IndentingNewLine]", "\"\<Different constructs of \>\"", ",", 
   RowBox[{
    RowBox[{"Tuu", "[", 
     RowBox[{"i", ",", "j"}], "]"}], "\[Element]", 
    RowBox[{"SO", "[", "N", "]"}]}], ",", "yield", ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Tuu", "[", 
     RowBox[{"i", ",", "j"}], "]"}], "->", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Symmetric", "->", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"Suu", "[", 
             RowBox[{"i", ",", "j"}], "]"}], "->", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"Tuu", "[", 
                RowBox[{"i", ",", "j"}], "]"}], "+", 
               RowBox[{"Tuu", "[", 
                RowBox[{"j", ",", "i"}], "]"}]}], ")"}], "/", "2"}]}], ",", 
           "irreducible"}], "}"}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"Antisymmetric", "->", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"Auu", "[", 
             RowBox[{"i", ",", "j"}], "]"}], "->", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"Tuu", "[", 
                RowBox[{"i", ",", "j"}], "]"}], "-", 
               RowBox[{"Tuu", "[", 
                RowBox[{"j", ",", "i"}], "]"}]}], ")"}], "/", "2"}]}], ",", 
           "irreducible"}], "}"}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"Traceless", "->", 
         RowBox[{
          RowBox[{"Quu", "[", 
           RowBox[{"i", ",", "j"}], "]"}], "->", 
          RowBox[{
           RowBox[{"Tuu", "[", 
            RowBox[{"i", ",", "j"}], "]"}], "-", 
           RowBox[{
            RowBox[{
             RowBox[{"\[Delta]uu", "[", 
              RowBox[{"i", ",", "j"}], "]"}], ".", " ", 
             RowBox[{"Tr", "[", 
              RowBox[{"Tuu", "[", 
               RowBox[{"_", ",", "_"}], "]"}], "]"}]}], "/", "N"}]}]}]}]}], 
       "\[IndentingNewLine]", "}"}], "//", "Column"}], ")"}]}], ",", "NL", 
   ",", "\[IndentingNewLine]", 
   "\"\<We can decompose a product of 2 vectors \>\"", ",", 
   RowBox[{
    RowBox[{
     RowBox[{"v", "[", "N", "]"}], "\[CircleTimes]", 
     RowBox[{"w", "[", "N", "]"}]}], "->", 
    RowBox[{
     RowBox[{"N", "\[CircleTimes]", "N"}], "->", 
     RowBox[{
      RowBox[{"SymmetricTraceless", "[", 
       RowBox[{
        RowBox[{"N", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"N", "+", "1"}], ")"}], "/", "2"}]}], "-", "1"}], "]"}], 
      "\[CirclePlus]", 
      RowBox[{"\"\<Trace\>\"", "[", "1", "]"}], "\[CirclePlus]", 
      RowBox[{"Antisymmetric", "[", 
       RowBox[{"N", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"N", "-", "1"}], ")"}], "/", "2"}]}], "]"}]}]}]}], ",", 
   "NL", ",", "\[IndentingNewLine]", 
   "\"\<General commutation relation for generators in N-dimensions\>\"", ",",
    "yield", ",", "\[IndentingNewLine]", 
   RowBox[{"e8", "=", 
    RowBox[{
     RowBox[{"Com", "[", 
      RowBox[{
       RowBox[{"Juu", "[", 
        RowBox[{"i_", ",", "j_"}], "]"}], ",", 
       RowBox[{"Juu", "[", 
        RowBox[{"k_", ",", "l_"}], "]"}]}], "]"}], "->", 
     RowBox[{"I", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"\[Delta]uu", "[", 
          RowBox[{"i", ",", "k"}], "]"}], 
         RowBox[{"Juu", "[", 
          RowBox[{"j", ",", "l"}], "]"}]}], "-", 
        RowBox[{
         RowBox[{"\[Delta]uu", "[", 
          RowBox[{"j", ",", "k"}], "]"}], 
         RowBox[{"Juu", "[", 
          RowBox[{"i", ",", "l"}], "]"}]}], "+", 
        RowBox[{
         RowBox[{"\[Delta]uu", "[", 
          RowBox[{"j", ",", "l"}], "]"}], 
         RowBox[{"Juu", "[", 
          RowBox[{"i", ",", "k"}], "]"}]}], "-", 
        RowBox[{
