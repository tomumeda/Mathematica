Cell[BoxData[
 RowBox[{"PR1", "[", 
  RowBox[{
  "\"\<Lorentz transformation S[\[CapitalLambda]]: \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"tmp\[CapitalLambda]", "=", 
    RowBox[{
     RowBox[{"\[Psi]", "[", 
      RowBox[{"x", "'"}], "]"}], "->", 
     RowBox[{
      RowBox[{"S", "[", "\[CapitalLambda]", "]"}], ".", 
      RowBox[{"\[Psi]", "[", "x", "]"}]}]}]}], ",", "\[IndentingNewLine]", 
   "NL", ",", "\"\<determined to be: \>\"", ",", 
   RowBox[{"subS", "=", 
    RowBox[{
     RowBox[{"S", "[", "\[CapitalLambda]", "]"}], "->", 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{"(", 
         RowBox[{"I", "/", "4"}], ")"}]}], 
       RowBox[{"\[Omega]", "@", 
        RowBox[{"dd", "[", 
         RowBox[{"\[Alpha]", ",", "\[Beta]"}], "]"}]}], 
       RowBox[{"\[Sigma]", "@", 
        RowBox[{"uu", "[", 
         RowBox[{"\[Alpha]", ",", "\[Beta]"}], "]"}]}]}], "]"}]}]}], ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<where the generators \>\"", ",", 
   RowBox[{"sub\[Sigma]", "=", 
    RowBox[{
     RowBox[{"\[Sigma]", "@", 
      RowBox[{"uu", "[", 
       RowBox[{"\[Mu]_", ",", "\[Nu]_"}], "]"}]}], "->", 
     RowBox[{"I", " ", 
      RowBox[{
       RowBox[{"MCommutator", "[", 
        RowBox[{
         RowBox[{"\[Gamma]", "@", 
          RowBox[{"u", "[", "\[Mu]", "]"}]}], ",", 
         RowBox[{"\[Gamma]", "@", 
          RowBox[{"u", "[", "\[Nu]", "]"}]}]}], "]"}], "/", "2"}]}]}]}], ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<Apply Lorentz transform to: \>\"", 
   ",", 
   RowBox[{"sub", "=", 
    RowBox[{"{", 
     RowBox[{"\[Psi]", "->", 
      RowBox[{"\[Psi]", "[", 
       RowBox[{"x", "'"}], "]"}]}], "}"}]}], ",", "\[IndentingNewLine]", "NL",
    ",", "\"\<Generalize the list of terms as: \>\"", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{
     OverscriptBox["\[Psi]", "_"], ".", "A", ".", "\[Psi]"}]}], ",", 
   "\[IndentingNewLine]", "yield", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{"tmp", "/.", "sub"}]}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<Apply transform: \>\"", ",", "\[IndentingNewLine]", 
   RowBox[{"tmp", "=", 
    RowBox[{"tmp", "/.", "tmp\[CapitalLambda]"}]}], ",", 
   "\[IndentingNewLine]", "yield", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{"tmp", "/.", "subS"}]}], ",", "\[IndentingNewLine]", "yield", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{"tmp", "/.", 
     RowBox[{
      OverscriptBox[
       RowBox[{"a_", " ", ".", " ", "b_"}], "_"], "->", 
      RowBox[{
       OverscriptBox["b", "_"], " ", ".", " ", 
       OverscriptBox["a", "_"]}]}]}]}], ",", "\[IndentingNewLine]", "yield", 
   ",", 
   RowBox[{"tmp", "=", 
    RowBox[{"tmp", "/.", 
     RowBox[{
      OverscriptBox[
       RowBox[{"Exp", "[", " ", "a_", "]"}], "_"], "->", 
      RowBox[{"Exp", "[", 
       RowBox[{"-", " ", "a"}], "]"}]}]}]}], ",", "\[IndentingNewLine]", "NL",
    ",", "\"\<The transformation properties is determined by the \
transformation of the \>\"", ",", 
   RowBox[{"tmp0", "=", 
    RowBox[{"tmp", "[", 
     RowBox[{"[", 
      RowBox[{"2", ";;", 
       RowBox[{"-", "2"}]}], "]"}], "]"}]}], ",", "\"\< term.\>\"", ",", 
   "\[IndentingNewLine]", "NL", ",", 
   RowBox[{"CO", "[", 
    RowBox[{
    "\"\<We compute each expression using the definitions and expansion:\>\"",
      ",", "\[IndentingNewLine]", "sub\[Sigma]", ",", "and", ",", 
     RowBox[{"subE", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"tmp", "=", 
          RowBox[{"Exp", "[", "A", "]"}]}], ")"}], "->", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Series", "[", 
           RowBox[{"tmp", ",", 
            RowBox[{"{", 
             RowBox[{"A", ",", "0", ",", "1"}], "}"}]}], "]"}], "//", 
          "Normal"}], ")"}]}], "//", 
       RowBox[{"RuleX2PatternVar", "[", "A", "]"}]}]}]}], 
    "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<For \>\"", ",", 
   RowBox[{"sub", "=", 
    RowBox[{"A", "->", "1"}]}], ",", "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"tmp", "=", "tmp0"}], ",", "\[IndentingNewLine]", "yield", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{
     RowBox[{"tmp", "/.", "sub"}], "//.", 
     RowBox[{"simpleDot2", "[", 
      RowBox[{"{", "}"}], "]"}]}]}], ",", "\[IndentingNewLine]", "yield", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{
     RowBox[{"tmp", "/.", 
      RowBox[{
       RowBox[{
        RowBox[{"Exp", "[", "a_", "]"}], " ", ".", " ", 
        RowBox[{"Exp", "[", "b_", "]"}]}], "->", 
       RowBox[{"Exp", "[", 
        RowBox[{"a", " ", "+", "b"}], "]"}]}]}], "/.", 
     RowBox[{"simpleDot2", "[", 
      RowBox[{"{", "}"}], "]"}]}]}], ",", "\[IndentingNewLine]", "imply", 
   ",", "\"\< scalar.\>\"", ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<For \>\"", ",", 
