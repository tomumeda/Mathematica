Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"PR1", "[", 
   RowBox[{
   "\"\<In the SO[3,1] representation the (1,0) and (0,1) objects can be \
represented by 3+3 states: \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp\[Psi]", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"\[Psi]d", "[", "i", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", 
           RowBox[{"-", "1"}], ",", "1"}], "}"}]}], "]"}], ",", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"\[Chi]d", "[", "i", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", 
           RowBox[{"-", "1"}], ",", "1"}], "}"}]}], "]"}]}], "}"}]}], ",", 
    "\"\<\nThe label (1,0) means: \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Jdd", "[", 
          RowBox[{"\"\<+\>\"", ",", "i"}], "]"}], ",", 
         RowBox[{"Jdd", "[", 
          RowBox[{"\"\<-\>\"", ",", "i"}], "]"}]}], "}"}], ".", 
       RowBox[{"\[Psi]d", "[", "m", "]"}]}], "->", 
      RowBox[{"{", 
       RowBox[{"1", ",", "0"}], "}"}]}]}], ",", "\"\< where \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"sub", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"Jdd", "[", 
         RowBox[{"\"\<+\>\"", ",", "i_"}], "]"}], "->", 
        RowBox[{
         FractionBox["1", "2"], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Jd", "[", "i", "]"}], "+", 
           RowBox[{"I", " ", 
            RowBox[{"Kd", "[", "i", "]"}]}]}], ")"}]}]}], ",", 
       RowBox[{
        RowBox[{"Jdd", "[", 
         RowBox[{"\"\<-\>\"", ",", "i_"}], "]"}], "->", 
        RowBox[{
         FractionBox["1", "2"], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Jd", "[", "i", "]"}], "-", 
           RowBox[{"I", " ", 
            RowBox[{"Kd", "[", "i", "]"}]}]}], ")"}]}]}]}], "}"}]}], ",", 
    "and", ",", "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Jd", "[", "i", "]"}], ",", 
      RowBox[{"Kd", "[", "i", "]"}]}], "}"}], ",", 
    "\"\< are the rotation and boost generators of the Lorentz group.\nSo the \
correspondence is \>\"", ",", 
    RowBox[{"tmp", "=", 
     RowBox[{
      RowBox[{"tmp", "/.", "sub"}], "/.", 
      RowBox[{"m", "->", "1"}]}]}], ",", 
    "\"\< and the J's and K's are 3x3 matrices with the property that \>\"", 
    ",", "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{
      RowBox[{"MapAt", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Distribute", "[", 
          RowBox[{"#", ",", "List"}], "]"}], "&"}], ",", "tmp", ",", "1"}], 
       "]"}], "//", "Thread"}]}], ",", 
    "\"\< so we have the same conclusion as (13) and (14): \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"Jd", "[", "i", "]"}], "\[LeftRightArrow]", " ", 
       RowBox[{"\[CapitalSigma]d", "[", "i", "]"}]}], ",", 
      RowBox[{
       RowBox[{"I", " ", 
        RowBox[{"Kd", "[", "i", "]"}]}], "\[LeftRightArrow]", 
       RowBox[{"\[CapitalSigma]d", "[", "i", "]"}]}]}], "}"}], ",", 
    "\"\< and for the representation of (0,1) objects we similarly conclude \
that the representation of Lorentz generators correspond to \>\"", ",", " ", 
    "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"Jd", "[", "i", "]"}], "\[LeftRightArrow]", " ", 
       RowBox[{"\[CapitalSigma]d", "[", "i", "]"}]}], ",", 
      RowBox[{
       RowBox[{"I", " ", 
        RowBox[{"Kd", "[", "i", "]"}]}], "\[LeftRightArrow]", 
       RowBox[{"-", 
        RowBox[{"\[CapitalSigma]d", "[", "i", "]"}]}]}]}], "}"}], ",", 
    "\"\<\nSimilarly, parity invariance where \>\"", ",", 
    RowBox[{
     RowBox[{"Jdd", "[", 
      RowBox[{"\"\<+\>\"", ",", "i"}], "]"}], "\[LeftRightArrow]", 
     RowBox[{"Jdd", "[", 
      RowBox[{"\"\<-\>\"", ",", "i"}], "]"}]}], ",", 
    "\"\< or (1,0)\[LeftRightArrow](0,1) would be exchanged would require all \
six objects \>\"", ",", "tmp\[Psi]", ",", 
    "\"\< to be incorporated in an invariant theory.  \>\""}], 
   "\[IndentingNewLine]", "]"}], ";"}]], "Input",
