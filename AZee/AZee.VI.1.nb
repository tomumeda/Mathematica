            "0"}], "]"}]}]}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"tmp2a", "=", 
       RowBox[{"tmp2", "->", 
        RowBox[{
         RowBox[{
          RowBox[{"T", "[", 
           RowBox[{"\[Epsilon]", ",", "\"\<uuu\>\""}], "]"}], "[", 
          RowBox[{"0", ",", "\[Mu]", ",", "\[Nu]"}], "]"}], ".", 
         RowBox[{
          RowBox[{"T", "[", 
           RowBox[{"a", ",", "\"\<d\>\""}], "]"}], "[", "0", "]"}], ".", " ", 
         
         RowBox[{"xPartialD", "[", 
