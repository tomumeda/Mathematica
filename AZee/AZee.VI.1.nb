      RowBox[{
       RowBox[{"T", "[", 
        RowBox[{"a", ",", "\"\<d\>\""}], "]"}], "[", "\[Nu]", "]"}], "+", 
      RowBox[{"xPartialD", "[", 
       RowBox[{"\[CapitalLambda]", ",", "\[Nu]"}], "]"}]}]}]}], ",", "imply", 
   ",", 
   RowBox[{"\[ScriptCapitalL]", "\[Rule]", "\[ScriptCapitalL]"}], ",", 
   "\[IndentingNewLine]", "NL", ",", 
   "\"\<In (2+1)-dimensions rewrite terms as \>\"", ",", 
   RowBox[{"sub", "=", 
    RowBox[{"{", 
