Cell[BoxData[{
 RowBox[{"PR", "[", 
  RowBox[{
  "\"\<Combine Hawking model with Page(unitarity) dynamics\>\"", ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<Pair production model \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"$", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"selectLMU", "[", "\[Phi]", "]"}], ",", "\[IndentingNewLine]", 
       
       RowBox[{
        RowBox[{"T", "[", 
         RowBox[{"q", ",", "\"\<d\>\"", ",", 
          RowBox[{"{", "i", "}"}]}], "]"}], "[", 
        RowBox[{"CG", "[", "\"\<outgoing\>\"", "]"}], "]"}]}], 
      "\[IndentingNewLine]", "\[IndentingNewLine]", "}"}]}], ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"ColumnForms", "[", 
        RowBox[{"#1", ",", "2"}], "]"}], "&"}], ")"}], "[", "$", "]"}]}], ",",
    "\[IndentingNewLine]", "NL", ",", 
   "\"\<Schwinger effect: pair production of particles in electric field. \
{electron-hole} in electric field\>\"", ",", "\[IndentingNewLine]", "NL", ",", 
   RowBox[{
    RowBox[{"$0", "=", 
     RowBox[{"$", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         SubscriptBox["\[ScriptCapitalE]", "iE"], "\[Rule]", 
         RowBox[{"e", " ", "iE", " ", "L"}]}], ",", 
        RowBox[{
         SubscriptBox["\[ScriptCapitalE]", "pair"], "\[Rule]", 
         RowBox[{"2", "m"}]}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          SubscriptBox["P", "pair"], "[", 
          RowBox[{"CG", "[", "\"\<probability of production\>\"", "]"}], 
          "]"}], "\[Rule]", 
         RowBox[{"Exp", "[", 
          RowBox[{
           RowBox[{"-", "L"}], "/", "\[Lambda]"}], "]"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"\[Lambda]", "[", 
          RowBox[{"CG", "[", "\"\<wave length of electron\>\"", "]"}], "]"}], 
         "\[Rule]", 
         RowBox[{"\[HBar]", "/", "m"}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"L", "[", 
         RowBox[{"CG", "[", "\"\<distance between electrons\>\"", "]"}], 
         "]"}]}], "\[IndentingNewLine]", "}"}]}]}], ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"ColumnForms", "[", 
        RowBox[{"#1", ",", "2"}], "]"}], "&"}], ")"}], "[", "$", "]"}]}], ",", 
   RowBox[{"accumLMU", "[", "$", "]"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"$", "=", 
     RowBox[{"$0", "//", "tuRule"}]}], ";", "\[IndentingNewLine]", "NL"}], 
   ",", "\"\<Let \>\"", ",", 
   RowBox[{"$s", "=", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["\[ScriptCapitalE]", "iE"], "->", 
      SubscriptBox["\[ScriptCapitalE]", "pair"]}], "}"}]}], ",", 
   "\[IndentingNewLine]", "imply", ",", 
   RowBox[{"$", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"$", "/.", "$s"}], "//", 
      RowBox[{"tuRuleEliminate", "[", 
       RowBox[{"{", 
        RowBox[{"\[Lambda]", ",", 
         SubscriptBox["\[ScriptCapitalE]", "pair"], ",", "L"}], "}"}], 
       "]"}]}], "//", "First"}]}], ",", "\[IndentingNewLine]", "NL", ",", 
   RowBox[{"$pp", "=", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       SubscriptBox["R", "pp"], "\[Rule]", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"e", " ", "iE"}], ")"}], "2"], 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "\[Pi]"}], " ", 
          RowBox[{
           SuperscriptBox["m", "2"], "/", 
           RowBox[{"(", 
            RowBox[{"e", " ", "iE"}], ")"}]}]}], "]"}]}]}], "}"}], "[", 
     RowBox[{"CG", "[", "\"\<rate of pair production\>\"", "]"}], "]"}]}]}], 
  "\[IndentingNewLine]", "]"}], "\n", 
 RowBox[{"PR", "[", 
  RowBox[{
  "\"\<\[FilledCircle] pair production in gravitational field (replacing \
electric field) \>\"", ",", "\[IndentingNewLine]", 
   RowBox[{"$e", "=", 
    RowBox[{"$", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"e", " ", "iE"}], "\[Rule]", 
        RowBox[{"G", " ", 
         RowBox[{
          SubscriptBox["M", "BH"], "/", 
          SuperscriptBox["r", "2"]}], " ", 
         SubscriptBox["\[ScriptCapitalE]", "part"]}]}], ",", " ", 
       RowBox[{
        RowBox[{"e", " ", "iE"}], "->", 
        RowBox[{"\[ScriptCapitalE]", "/", 
         SubscriptBox["R", "s"]}]}], ",", 
       RowBox[{"m", "\[Rule]", "\[ScriptCapitalE]"}]}], "}"}]}]}], ",", 
   "\[IndentingNewLine]", "yield", ",", 
   RowBox[{
    RowBox[{"$", "=", 
     RowBox[{
      RowBox[{"$pp", "//", "tuRule"}], "//", "First"}]}], ";", 
    "\[IndentingNewLine]", "yield"}], ",", 
   RowBox[{"$pp1", "=", 
    RowBox[{"$", "=", 
     RowBox[{
      RowBox[{"$", "/.", 
       RowBox[{"tuRuleSolve", "[", 
        RowBox[{
         RowBox[{"$e", "[", 
          RowBox[{"[", "2", "]"}], "]"}], ",", "e"}], "]"}]}], "/.", 
      RowBox[{"$e", "[", 
       RowBox[{"[", "3", "]"}], "]"}]}]}]}], ",", "\[IndentingNewLine]", 
   "\[IndentingNewLine]", "NL", ",", 
   "\"\<\[Bullet] To find maximum \!\(\*SubscriptBox[\(R\), \(pp\)]\) \
Substitute \>\"", ",", 
   RowBox[{"$s", "=", 
    RowBox[{"\[ScriptCapitalE]", "\[Rule]", 
     RowBox[{"\[ScriptCapitalE]", "/", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["R", "s"], "\[Pi]"}], ")"}]}]}]}], ",", 
   "\[IndentingNewLine]", "yield", ",", 
   RowBox[{
    RowBox[{"$", "=", 
     RowBox[{"$", "/.", "$s"}]}], ";", "\[IndentingNewLine]", "NL"}], ",", 
   "\"\<And \>\"", ",", 
   RowBox[{"$m", "=", 
    RowBox[{
