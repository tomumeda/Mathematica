     RowBox[{
      RowBox[{
       RowBox[{"Exp", "[", 
        RowBox[{"-", "\[ScriptCapitalE]"}], "]"}], 
       SuperscriptBox["\[ScriptCapitalE]", "2"]}], ",", "\[ScriptCapitalE]"}],
      "]"}]}], ",", "\[IndentingNewLine]", "yield", ",", 
   RowBox[{"$m", "=", 
    RowBox[{"$m", "//", "Activate"}]}], ",", "\[IndentingNewLine]", "Imply", 
   ",", "\"\<Maximum at \>\"", ",", 
   RowBox[{"$m", "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"$m", "[", 
        RowBox[{"[", 
         RowBox[{"2", ",", "1"}], "]"}], "]"}], "/.", 
       RowBox[{"\[ScriptCapitalE]", "\[Rule]", 
        RowBox[{"\[ScriptCapitalE]", " ", 
         SubscriptBox["R", "s"], "\[Pi]"}]}]}], "//", 
      RowBox[{"tuRuleSolveF", "[", "\[ScriptCapitalE]", "]"}]}], "//", 
     "First"}]}], ",", "\[IndentingNewLine]", "Imply", ",", 
   RowBox[{"$", "=", 
    RowBox[{"$pp1", "/.", "$m"}]}], ",", "\[IndentingNewLine]", "NL", ",", 
