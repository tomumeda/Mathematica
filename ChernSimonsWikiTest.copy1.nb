Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"nWedge", "[", 
    RowBox[{"f_", ",", "n_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "}"}], ",", 
     RowBox[{
      RowBox[{"Array", "[", 
       RowBox[{"f", ",", "n"}], "]"}], "/.", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"f", "[", "_", "]"}], "->", "f"}], ",", 
        RowBox[{"List", "->", "Wedge"}]}], "}"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"PR1", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"tmpd0", "=", 
     RowBox[{
      RowBox[{"d", "[", 
       RowBox[{"\[Omega]", "[", 
        RowBox[{
         RowBox[{"2", " ", "k"}], " ", "-", "1"}], "]"}], "]"}], "->", 
      RowBox[{"xtr", "[", 
       RowBox[{"xnWedge", "[", 
        RowBox[{"F", ",", "k"}], "]"}], "]"}]}]}], ";", "\[IndentingNewLine]",
     "NL"}], ",", 
   RowBox[{"CO", "[", "\"\<For example in 1-dimensions: \>\"", "]"}], ",", 
   RowBox[{"sub", "=", 
    RowBox[{"k", "->", "1"}]}], ",", "\[IndentingNewLine]", "yield", ",", 
   RowBox[{"tmpd", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"tmpd0", "/.", "sub"}], "/.", 
      RowBox[{"xnWedge", "->", "nWedge"}]}], "/.", 
     RowBox[{"xtr", "->", "tr"}]}]}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<which is d[1-form]->2-form.  Wiki states that the LHS 1-form is: \
\>\"", ",", "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{
     RowBox[{"tmpd", "[", 
      RowBox[{"[", 
       RowBox[{"1", ",", "1"}], "]"}], "]"}], "->", "tmp1f"}]}], ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<Its exterior derivative is: \>\"", 
   ",", 
   RowBox[{"tmp", "=", 
    RowBox[{"d", "@", "tmp"}]}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<From: \>\"", ",", "subF", ",", "\[IndentingNewLine]", "yield", ",", 
   RowBox[{"sub", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"tr", "/@", 
       RowBox[{
        RowBox[{"RuleX", "[", 
         RowBox[{"subF", ",", 
          RowBox[{"d", "[", "A", "]"}]}], "]"}], "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ",", 
      RowBox[{
       RowBox[{"tr", "[", 
        RowBox[{"A", "\[Wedge]", "A"}], "]"}], "->", "0"}]}], "}"}]}], ",", 
   "\[IndentingNewLine]", "imply", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{"tmp", "//.", "sub"}]}], ",", "OK", ",", "\[IndentingNewLine]", 
   "NL", ",", "\[IndentingNewLine]", "NL", ",", 
   RowBox[{"CO", "[", "\"\<In 3-dimensions: \>\"", "]"}], ",", 
   RowBox[{"sub", "=", 
    RowBox[{"k", "->", "2"}]}], ",", "\[IndentingNewLine]", "yield", ",", 
   RowBox[{"tmpd", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"tmpd0", "/.", "sub"}], "/.", 
      RowBox[{"xnWedge", "->", "nWedge"}]}], "/.", 
     RowBox[{"xtr", "->", "tr"}]}]}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<which is d[3-form]->4-form.  Wiki states that the LHS 3-form is: \
\>\"", ",", "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{
     RowBox[{"tmpd", "[", 
      RowBox[{"[", 
       RowBox[{"1", ",", "1"}], "]"}], "]"}], "->", "tmp3f"}]}], ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<Its exterior derivative is: \>\"", 
   ",", 
   RowBox[{"tmp", "=", 
    RowBox[{"d", "@", "tmp"}]}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<From \>\"", ",", "subF", ",", "\[IndentingNewLine]", 
   "\"\< Compute additional substitution: \>\"", ",", 
   RowBox[{"sub1", "=", 
    RowBox[{
     RowBox[{"d", "/@", "subF"}], "//", "reWrite"}]}], ",", 
   "\[IndentingNewLine]", "Imply", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{
     RowBox[{"tmp", "/.", "sub1"}], "/.", "subF"}]}], ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<The RHS: \>\"", ",", 
   RowBox[{"tmp0", "=", 
