Cell[BoxData[
 RowBox[{"PR", "[", 
  RowBox[{
  "\"\<Surface gravity of BH's, \[Kappa], is defined on static Killing \
horizon is the acceleration needed to keep an object at this horizon.\>\"", 
   "\[IndentingNewLine]", ",", "NL", ",", 
   RowBox[{"$wiki0", "=", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"T", "[", 
          RowBox[{"k", ",", "\"\<u\>\"", ",", 
           RowBox[{"{", "a", "}"}]}], "]"}], "[", 
         RowBox[{
         "CG", "[", "\"\<suitably normalized Killing vector\>\"", "]"}], 
         "]"}], "\[CenterDot]", 
        RowBox[{"tuDCovariant", "[", 
         RowBox[{
          RowBox[{"T", "[", 
           RowBox[{"k", ",", "\"\<u\>\"", ",", 
            RowBox[{"{", "b", "}"}]}], "]"}], ",", "a"}], "]"}]}], "\[Rule]", 
       
       RowBox[{
        RowBox[{"\[Kappa]", "[", 
         RowBox[{"CG", "[", "\"\<surface gravity defined\>\"", "]"}], "]"}], 
        " ", 
        RowBox[{"T", "[", 
         RowBox[{"k", ",", "\"\<u\>\"", ",", 
          RowBox[{"{", "b", "}"}]}], "]"}]}]}], "}"}], "[", 
     RowBox[{"CG", "[", "\"\<at Killing horizion, b not summed\>\"", "]"}], 
     "]"}]}], "\[IndentingNewLine]", "\[IndentingNewLine]", ",", "NL", ",", 
   "\"\<\[FilledCircle] For example, a static, asymptotically flat spacetime \
where \>\"", "\[IndentingNewLine]", ",", 
   RowBox[{"$1", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"T", "[", 
           RowBox[{"k", ",", "\"\<d\>\"", ",", 
            RowBox[{"{", "a", "}"}]}], "]"}], "\[CenterDot]", 
          RowBox[{"T", "[", 
           RowBox[{"k", ",", "\"\<u\>\"", ",", 
            RowBox[{"{", "a", "}"}]}], "]"}]}], "\[Rule]", 
         RowBox[{"-", "1"}]}], "}"}], "[", 
       RowBox[{"CG", "[", 
        RowBox[{"r", "\[LongRightArrow]", "\[Infinity]"}], "]"}], "]"}], "//", 
      RowBox[{"tuAddPatternVariable", "[", "a", "]"}]}], "}"}]}], 
   "\[IndentingNewLine]", ",", "Imply", ",", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\[Kappa]", "\[GreaterEqual]", "0"}], "}"}], "[", 
    RowBox[{"CR", "[", "\"\<claim, show this\>\"", "]"}], "]"}], 
   "\[IndentingNewLine]", ",", "NL", ",", "\"\<\[Bullet]From definition \>\"",
    ",", 
   RowBox[{"$", "=", 
    RowBox[{"$wiki0", "//", "tuRule1"}]}], "\[IndentingNewLine]", ",", "NL", 
   ",", "\"\<Contract over b: \>\"", ",", 
   RowBox[{"$", "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"T", "[", 
        RowBox[{"k", ",", "\"\<d\>\"", ",", 
         RowBox[{"{", "b", "}"}]}], "]"}], "\[CenterDot]", "#"}], "&"}], "/@",
      "$"}]}], "\[IndentingNewLine]", ",", "yield", ",", 
   RowBox[{"$", "=", 
    RowBox[{"$", "//", 
     RowBox[{"expandDot", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"$1", ",", 
         RowBox[{
          RowBox[{
           RowBox[{"T", "[", 
            RowBox[{"k", ",", "\"\<d\>\"", ",", 
             RowBox[{"{", "b", "}"}]}], "]"}], "\[CenterDot]", 
           RowBox[{"T", "[", 
            RowBox[{"k", ",", "\"\<u\>\"", ",", 
             RowBox[{"{", "a", "}"}]}], "]"}]}], "->", 
          RowBox[{
           RowBox[{"T", "[", 
            RowBox[{"k", ",", "\"\<u\>\"", ",", 
             RowBox[{"{", "a", "}"}]}], "]"}], "\[CenterDot]", 
           RowBox[{"T", "[", 
            RowBox[{"k", ",", "\"\<d\>\"", ",", 
             RowBox[{"{", "b", "}"}]}], "]"}]}]}]}], "}"}], ",", 
       RowBox[{"{", "\[Kappa]", "}"}]}], "]"}]}]}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", ",", "NL", ",", "\"\<Since \>\"", ",", 
   RowBox[{"$s", "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"$s", "=", 
       RowBox[{"tuDCovariant", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"T", "[", 
           RowBox[{"k", ",", "\"\<d\>\"", ",", 
            RowBox[{"{", "b", "}"}]}], "]"}], "\[CenterDot]", 
          RowBox[{"T", "[", 
           RowBox[{"k", ",", "\"\<u\>\"", ",", 
            RowBox[{"{", "b", "}"}]}], "]"}]}], ",", "a"}], "]"}]}], ")"}], 
     "\[Rule]", 
     RowBox[{"(", 
      RowBox[{"$s", "//", 
       RowBox[{"tuDerivativeExpand", "[", "]"}]}], ")"}]}]}], 
   "\[IndentingNewLine]", ",", "yield", ",", 
   RowBox[{"$s", "=", 
    RowBox[{
     RowBox[{"$s", "/.", 
      RowBox[{"tuRule1", "[", "$1", "]"}]}], "//", 
     RowBox[{"tuDerivativeExpand", "[", "]"}]}]}], "\[IndentingNewLine]", ",",
    "yield", ",", 
   RowBox[{"$s", "=", 
    RowBox[{
     RowBox[{"tuRuleSolve", "[", 
      RowBox[{"$s", ",", 
       RowBox[{"$s", "[", 
        RowBox[{"[", 
         RowBox[{"2", ",", "1"}], "]"}], "]"}]}], "]"}], "//", 
     RowBox[{"part", "[", "1", "]"}]}]}], "\[IndentingNewLine]", ",", "Yield",
    ",", 
   RowBox[{"$", "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"$", "/.", "$s"}], "//", 
       RowBox[{"expandDot", "[", 
        RowBox[{"{", "toTimes", "}"}], "]"}]}], "//", 
      RowBox[{"tuRuleSolveF", "[", "\[Kappa]", "]"}]}], "//", 
     RowBox[{"part", "[", "1", "]"}]}]}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", ",", "NL", ",", 
   "\"\<Apply Killing field relationship \>\"", "\[IndentingNewLine]", ",", 
   RowBox[{
    RowBox[{"$s", "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"tuPermuteSymbol", "[", 
          RowBox[{"1", ",", 
           RowBox[{"{", 
            RowBox[{"\[Mu]", ",", "\[Nu]"}], "}"}]}], "]"}], "[", 
         RowBox[{"tuDCovariant", "[", 
          RowBox[{
           RowBox[{"T", "[", 
            RowBox[{"X", ",", "\"\<d\>\"", ",", 
             RowBox[{"{", "\[Nu]", "}"}]}], "]"}], ",", "\[Mu]"}], "]"}], 
         "]"}], "\[Rule]", "0"}], "//", "tuRuleSimplify"}], "//", 
      RowBox[{"part", "[", "1", "]"}]}]}], "\[IndentingNewLine]", ";", 
    RowBox[{"$s", "=", 
     RowBox[{
      RowBox[{"tuRuleSolve", "[", 
       RowBox[{"$s", ",", 
        RowBox[{"$s", "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "1"}], "]"}], "]"}]}], "]"}], "//", 
      RowBox[{"tuAddPatternVariable", "[", 
       RowBox[{"{", 
        RowBox[{"\[Mu]", ",", "\[Nu]", ",", "X"}], "}"}], "]"}]}]}]}], 
   "\[IndentingNewLine]", ",", "Yield", ",", 
   RowBox[{"$", "=", 
    RowBox[{"$", "/.", "$s"}]}], "\[IndentingNewLine]", ",", 
   RowBox[{
   "CO", "[", 
    "\"\<The dummy indices can be switched implying that \[Kappa]\[Rule]0 as \
r\[LongRightArrow]\[Infinity]\>\"", "]"}]}], "\[IndentingNewLine]", 
  "]"}]], "Input",
