      RowBox[{"{", "\[Mu]", "}"}]}], "]"}], "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"0", ",", 
      RowBox[{"1", "/", 
       RowBox[{"Sqrt", "[", 
        RowBox[{"metric", "[", 
         RowBox[{"[", 
          RowBox[{"2", ",", "2"}], "]"}], "]"}], "]"}]}]}], "}"}]}], 
   "\[IndentingNewLine]", ",", "NL", ",", "\"\<The red-shift factor V: \>\"", 
   ",", 
   RowBox[{"$", "=", 
