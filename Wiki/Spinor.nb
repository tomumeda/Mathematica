Cell[BoxData[
 RowBox[{
  RowBox[{"PR", "[", 
   RowBox[{
   "aside", ",", "NL", ",", 
    "\"\<We examine the correspondence between quantum 1/2-spins and \
classical SphericalHarmonicY by plotting \>\"", ",", "\n", 
    RowBox[{"$0", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"SphericalHarmonicY", "[", 
        RowBox[{
         FractionBox["1", "2"], ",", 
         RowBox[{"pm", "[", 
          RowBox[{"1", "/", "2"}], "]"}], ",", "\[Theta]", ",", "\[Phi]"}], 
        "]"}], ",", 
       RowBox[{"\[Phi]", "\[Rule]", "0"}]}], "}"}]}], ",", 
    "\[IndentingNewLine]", "NL", ",", 
    "\"\<which is the analog of quantum 1/2-spins.\>\"", ",", 
    "\[IndentingNewLine]", "NL", ",", "\"\<\[Bullet] For pmUp: \>\"", ",", 
    RowBox[{"$", "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"$0", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "/.", 
         RowBox[{"$0", "[", 
          RowBox[{"[", "2", "]"}], "]"}]}], "/.", "pmUp"}], "//", 
       "FunctionExpand"}], "//", "FullSimplify"}]}], ",", 
    "\[IndentingNewLine]", "Yield", ",", 
    RowBox[{"Plot", "[", 
     RowBox[{"$", ",", 
      RowBox[{"{", 
       RowBox[{"\[Theta]", ",", "0", ",", "\[Pi]"}], "}"}]}], "]"}], ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", "NL", ",", 
    "\"\<\[Bullet] For pmDn: \>\"", ",", 
    RowBox[{"$", "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"$0", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "/.", 
         RowBox[{"$0", "[", 
          RowBox[{"[", "2", "]"}], "]"}]}], "/.", "pmDn"}], "//", 
       "FunctionExpand"}], "//", "FullSimplify"}]}], ",", 
    "\[IndentingNewLine]", "Yield", ",", 
    RowBox[{"Plot", "[", 
     RowBox[{"$", ",", 
      RowBox[{"{", 
       RowBox[{"\[Theta]", ",", "0", ",", "\[Pi]"}], "}"}]}], "]"}], ",", 
    "\[IndentingNewLine]", "NL", ",", 
    "\"\<It is not apparent what is the connection.\>\"", ",", 
    "\[IndentingNewLine]", "NL", ",", 
    "\"\<\[Bullet] The SphericalHarmonicY's can be computed for \>\"", ",", 
    RowBox[{
     RowBox[{"Abs", "[", "m", "]"}], ">", 
     RowBox[{"l", "/", "2"}]}], ",", "\"\< as in the case \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"$", "=", 
     RowBox[{
      RowBox[{"FunctionExpand", "[", " ", 
       RowBox[{"SphericalHarmonicY", "[", 
        RowBox[{
         FractionBox["1", "2"], ",", "1", ",", "\[Theta]", ",", "0"}], "]"}], 
       "]"}], "//", "FullSimplify"}]}], ",", "\[IndentingNewLine]", 
    RowBox[{"Plot", "[", 
     RowBox[{"$", ",", 
      RowBox[{"{", 
       RowBox[{"\[Theta]", ",", "0", ",", "\[Pi]"}], "}"}]}], "]"}], ",", 
    "\[IndentingNewLine]", "NL", ",", 
    "\"\<What quantum interpretation can we ascribe to this result?\>\"", ",",
     "\[IndentingNewLine]", "asideout"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", "]"}], "\[IndentingNewLine]"}]], "Input",
