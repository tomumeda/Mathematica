Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"PR", "[", 
  RowBox[{
  "\"\<Legendre transform is related to integration by parts: \>\"", ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<Basic integration by parts: \>\"", 
   ",", 
   RowBox[{"$", "=", 
    RowBox[{
     RowBox[{"p", " ", 
      RowBox[{"d", "[", "x", "]"}]}], "\[Rule]", " ", 
     RowBox[{
      RowBox[{"d", "[", 
       RowBox[{"p", " ", "x"}], "]"}], "-", 
      RowBox[{"x", " ", 
       RowBox[{"d", "[", "p", "]"}]}]}]}]}], ",", "\[IndentingNewLine]", "NL",
    ",", "\"\<Then for: \>\"", ",", 
   RowBox[{"$", "=", 
    RowBox[{"f", "[", 
     RowBox[{"x", ",", "y"}], "]"}]}], ",", "\[IndentingNewLine]", "imply", 
   ",", 
   RowBox[{
    RowBox[{"$", "=", 
     RowBox[{"d", "[", "$", "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"$", "=", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"$", "\[Rule]", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"$", "//", 
           RowBox[{"tuDExpandF", "[", "d", "]"}]}], "//", 
          RowBox[{"tuD2tuDOp", "[", 
           RowBox[{"tuDDown", "[", "d", "]"}], "]"}]}], ")"}]}], "//", 
       RowBox[{"tuArgRemove", "[", 
        RowBox[{"f", "|", "g"}], "]"}]}], ")"}]}]}], ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<Define \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"$s1", "=", 
    RowBox[{"$s", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"tuDDown", "[", "d", "]"}], "[", 
         RowBox[{"f", ",", "x"}], "]"}], "\[Rule]", "p"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"tuDDown", "[", "d", "]"}], "[", 
         RowBox[{"f", ",", "y"}], "]"}], "\[Rule]", "v"}]}], "}"}]}]}], ",", 
   "\[IndentingNewLine]", "yield", ",", 
   RowBox[{"$df", "=", 
    RowBox[{"$", "=", 
     RowBox[{"$", "/.", "$s"}]}]}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<For \>\"", ",", 
   RowBox[{"p", "[", 
    RowBox[{"CG", "[", "\"\<define new variable conjugate to x\>\"", "]"}], 
    "]"}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<Legendre transform \>\"", ",", 
   RowBox[{"$", "=", 
    RowBox[{"g", "\[Rule]", 
     RowBox[{"f", "-", 
      RowBox[{"p", " ", "x"}]}]}]}], ",", "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"$", "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"d", "[", "#", "]"}], "&"}], "/@", "$"}], "//", 
       RowBox[{"tuDExpandF", "[", "d", "]"}]}], "//", 
      RowBox[{"tuD2tuDOp", "[", 
       RowBox[{"tuDDown", "[", "d", "]"}], "]"}]}], "//", 
     RowBox[{"tuArgRemove", "[", 
      RowBox[{"f", "|", "g"}], "]"}]}]}], ",", "\[IndentingNewLine]", "yield",
    ",", 
   RowBox[{"$", "=", 
    RowBox[{"$", "/.", "$df"}]}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<where \>\"", ",", 
   RowBox[{"$s", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"tuDDown", "[", "d", "]"}], "[", 
        RowBox[{"g", ",", "p"}], "]"}], "\[Rule]", 
       RowBox[{"-", "x"}]}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"tuDDown", "[", "d", "]"}], "[", 
        RowBox[{"g", ",", "y"}], "]"}], "\[Rule]", "v"}]}], "}"}]}], ",", 
   "\[IndentingNewLine]", "note", ",", 
   "\"\<The non-conjugated variable y derivatives is the same \>\"", ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"$s1", "[", 
      RowBox[{"[", "2", "]"}], "]"}], ",", 
     RowBox[{"$s", "[", 
      RowBox[{"[", "2", "]"}], "]"}]}], "}"}], ",", "\[IndentingNewLine]", 
   "\"\< while the conjugate variables are mapped into each other \>\"", ",", 
   
   RowBox[{"{", 
    RowBox[{
     RowBox[{"$s1", "[", 
      RowBox[{"[", "1", "]"}], "]"}], ",", 
     RowBox[{"$s", "[", 
      RowBox[{"[", "1", "]"}], "]"}]}], "}"}]}], "\[IndentingNewLine]", 
  "]"}]], "Input",
