Cell[CellGroupData[{

Cell["Doing the sum over Bound-states ", "Section",
 CellChangeTimes->{{3.614930732621221*^9, 3.614930738493793*^9}}],

Cell["Here we use t=\[Tau]-\[ImaginaryI]\[Phi]", "Text",
 CellChangeTimes->{{3.614930855766303*^9, 3.614930861319343*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"sum", "=", 
  RowBox[{
   RowBox[{
    RowBox[{"FullSimplify", "[", 
     RowBox[{"#", ",", 
      RowBox[{"t", ">", "0"}]}], "]"}], "&"}], "@", 
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"a", "=", "1"}], "\[Infinity]"], 
    FractionBox[
     RowBox[{
      RowBox[{
       RowBox[{"h", "[", "a", "]"}], "[", "u", "]"}], " ", 
      RowBox[{
       RowBox[{"\[Mu]", "[", "a", "]"}], "[", "u", "]"}], " ", 
      RowBox[{"Exp", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"-", "t"}], " ", "a"}], "+", 
        RowBox[{"2", " ", "\[ImaginaryI]", " ", "u", " ", "\[Sigma]"}]}], 
       "]"}]}], 
     RowBox[{"2", " ", "\[Pi]"}]]}]}]}]], "Input",
 CellChangeTimes->{{3.614930840397135*^9, 3.6149309368867598`*^9}, {
  3.614931040204576*^9, 3.614931042189362*^9}, {3.6149310877786407`*^9, 
  3.6149310882802277`*^9}}],

Cell[BoxData[
 RowBox[{"-", 
  FractionBox[
   RowBox[{"2", " ", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{
      RowBox[{"-", "t"}], "+", 
      RowBox[{"2", " ", "\[ImaginaryI]", " ", "u", " ", "\[Sigma]"}]}]], " ", 
    
    RowBox[{"Csch", "[", 
     RowBox[{"2", " ", "\[Pi]", " ", "u"}], "]"}], " ", 
    RowBox[{"Sinh", "[", 
     RowBox[{"2", " ", "u", " ", 
      RowBox[{"ArcSec", "[", 
       RowBox[{"2", " ", 
        SuperscriptBox["\[ExponentialE]", "t"]}], "]"}]}], "]"}]}], 
   SqrtBox[
    RowBox[{"4", "-", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "2"}], " ", "t"}]]}]]]}]], "Output",
 CellChangeTimes->{{3.614930881837502*^9, 3.614930940852035*^9}, 
   3.6149310427658567`*^9, 3.614931088977088*^9}]
}, Open  ]]
