Cell["Restricting the Match of a Pattern", "Subsection"],

Cell[BoxData[{
 RowBox[{"ClearAll", "[", "fac", "]"}], "\n", 
 RowBox[{
  RowBox[{"fac", "[", 
   RowBox[{"n_Integer", "/;", 
