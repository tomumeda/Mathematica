Cell[BoxData[
 RowBox[{
  RowBox[{"getmomentum", "[", 
   RowBox[{"n_", ",", "vector_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"translated", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"translationnconM", "[", "n", "]"}], ".", "vector"}], "//", 
        "Chop"}]}], ",", "ratios"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"ratios", "=", 
      RowBox[{"Select", "[", 
       RowBox[{
        RowBox[{"Quiet", "[", 
         RowBox[{"translated", "/", 
          RowBox[{"Chop", "[", "vector", "]"}]}], "]"}], ",", 
        RowBox[{
         RowBox[{"#", "=!=", "Indeterminate"}], "&"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"Max", "[", 
          RowBox[{"Abs", "[", "ratios", "]"}], "]"}], "-", 
         RowBox[{"Min", "[", 
          RowBox[{"Abs", "[", "ratios", "]"}], "]"}]}], ">", 
        SuperscriptBox["10", 
         RowBox[{"-", "3"}]]}], ",", 
       RowBox[{"Print", "[", 
        RowBox[{
        "\"\<warning: perhaps vector is not an eigenvector of T (\>\"", ",", 
         "ratios", ",", "\"\<)\>\""}], "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"k", "/.", 
        RowBox[{
         RowBox[{"Solve", "[", 
          RowBox[{
           RowBox[{
            SuperscriptBox["\[ExponentialE]", 
             RowBox[{"\[ImaginaryI]", " ", 
              FractionBox[
               RowBox[{"2", "\[Pi]"}], "n"], "k"}]], "\[Equal]", 
            RowBox[{"Mean", "[", "ratios", "]"}]}], ",", "k"}], "]"}], 
         "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], "//", 
       "Chop"}], "//", "Quiet"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
