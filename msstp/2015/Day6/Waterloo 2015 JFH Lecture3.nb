   SubscriptBox["y", "i"], TraditionalForm]]],
 ", etc... So we can simply "
}], "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"MyCentralMomentS", "[", 
   RowBox[{"data_List", ",", "moments_List"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"n", ",", "w", ",", "means"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"n", "=", 
      RowBox[{"Length", " ", "@", " ", "data"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"w", "=", 
      RowBox[{"Length", " ", "@", " ", "moments"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"means", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"Plus", "@@", "data"}], ")"}], "/", "n"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      FractionBox["1", "n"], 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"i", "=", "1"}], "n"], 
       RowBox[{
        UnderoverscriptBox["\[Product]", 
         RowBox[{"j", "=", "1"}], "w"], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           RowBox[{"data", "\[LeftDoubleBracket]", 
            RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}], "-", 
           RowBox[{
           "means", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}]}], 
          ")"}], 
         RowBox[{
         "moments", "\[LeftDoubleBracket]", "j", 
          "\[RightDoubleBracket]"}]]}]}]}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input"]
}, Open  ]],

