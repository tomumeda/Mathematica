    RowBox[{"(", 
     RowBox[{"a1", "+", 
      RowBox[{"a2", " ", 
       SqrtBox["b"]}]}], ")"}], 
    RowBox[{"(", 
     RowBox[{"a2", "+", 
      RowBox[{"a3", 
       SqrtBox["b1"]}]}], ")"}]}]], "//", "KSqr"}]], "Input",
