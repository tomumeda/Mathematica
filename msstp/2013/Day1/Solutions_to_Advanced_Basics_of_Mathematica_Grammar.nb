Cell[BoxData[
 RowBox[{
  RowBox[{"KSqr", ":=", 
   RowBox[{
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", "tmp", "}"}], ",", 
      RowBox[{
       RowBox[{"tmp", "=", 
        RowBox[{"Factor", "[", "#", "]"}]}], ";", 
       FractionBox[
        RowBox[{"Collect", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Numerator", "[", "tmp", "]"}], " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Denominator", "[", "tmp", "]"}], "/.", 
             "\[InvisibleSpace]", 
             RowBox[{
              SqrtBox["a_"], "\[RuleDelayed]", 
              RowBox[{"-", 
               SqrtBox["a"]}]}]}], ")"}]}], ",", 
          SqrtBox["_"], ",", "Factor"}], "]"}], 
        RowBox[{"Collect", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Denominator", "[", "tmp", "]"}], " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Denominator", "[", "tmp", "]"}], "/.", 
             "\[InvisibleSpace]", 
             RowBox[{
              SqrtBox["a_"], "\[RuleDelayed]", 
              RowBox[{"-", 
               SqrtBox["a"]}]}]}], ")"}]}], ",", 
          SqrtBox["_"], ",", "Factor"}], "]"}]]}]}], "]"}], "&"}]}], 
  ";"}]], "Input",
