        RowBox[{"-", 
         RowBox[{"\[Delta]uu", "[", 
          RowBox[{"a", ",", "b"}], "]"}]}]}]}], "/.", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"{", 
         RowBox[{"xd", "[", "\[Alpha]", "]"}], "}"}], "}"}], "->", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"t", ",", "0", ",", "\[Beta]"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"space", "@", "x"}], "}"}]}], "}"}]}]}], "/.", 
     RowBox[{
