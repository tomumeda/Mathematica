 RowBox[{"PR", "[", 
  RowBox[{
  "\"\<The lagrangian for the Klein-Gordon equation: \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"tmp", "=", 
    RowBox[{"LE", "->", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"xPartialD", "[", 
          RowBox[{"\[CurlyPhi]", ",", "\[Mu]"}], "]"}], 
         RowBox[{"xPartialD", "[", 
          RowBox[{"\[CurlyPhi]", ",", "\[Nu]"}], "]"}], 
         RowBox[{"guu", "[", 
          RowBox[{"\[Nu]", ",", "\[Mu]"}], "]"}]}], "-", 
        RowBox[{
         SuperscriptBox["m", "2"], "\[CurlyPhi]", " ", "\[CurlyPhi]"}]}], 
       ")"}], "/", "2"}]}]}], ",", "\"\< and the action: \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"tmp", "=", 
     RowBox[{"Map", "[", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"IntegralOp", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"{", 
            RowBox[{"xd", "[", "\[Alpha]", "]"}], "}"}], "}"}], ",", "#"}], 
         "]"}], "&"}], ",", "tmp"}], "]"}]}], ";", "yield", ";", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"S", "->", 
      RowBox[{"tmp", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}]}]}], ",", "\[IndentingNewLine]", 
   "\"\<\nLetting: \>\"", ",", "\[IndentingNewLine]", 
   RowBox[{"t", "->", 
    RowBox[{
     RowBox[{"-", "I"}], " ", "\[Tau]"}]}], ",", 
   "\"\< we get the Euclidean action: \>\"", ",", "\[IndentingNewLine]", 
   RowBox[{"tmpS", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"tmp", "/.", 
       RowBox[{
