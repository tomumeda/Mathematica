    "\[IndentingNewLine]", "\"\<POFF\>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"tmpc", "=", 
     RowBox[{"tmpc", "//", 
      RowBox[{"ConjugateCTSimplify1", "[", "reals", "]"}]}]}], ",", "Yield", 
    ",", "\[IndentingNewLine]", 
    RowBox[{"tmpc", "=", 
