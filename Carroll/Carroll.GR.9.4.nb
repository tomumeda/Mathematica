Cell[BoxData[
 RowBox[{
  RowBox[{"PR1", "[", 
   RowBox[{
   "\"\<? Are these conditions satisfied for EM tensor with conditions: \>\"",
     ",", "\[IndentingNewLine]", 
    RowBox[{"subF", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"F", "@", 
         RowBox[{"dd", "[", 
          RowBox[{"a_", ",", "b_"}], "]"}]}], "+", 
        RowBox[{"F", "@", 
         RowBox[{"dd", "[", 
          RowBox[{"b_", ",", "a_"}], "]"}]}]}], "->", "0"}], "}"}]}], ",", 
    "\[IndentingNewLine]", "NL", ",", 
    "\"\<To start off with we follow Rendall in that a time-like vector can \
be composed of 2 null vectors(above): \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"subt", "=", 
     RowBox[{
      RowBox[{"t", "@", 
       RowBox[{"u", "[", "\[Mu]_", "]"}]}], "->", 
      RowBox[{
       RowBox[{
        RowBox[{"T", "[", 
         RowBox[{"la", ",", "\"\<u\>\""}], "]"}], "[", "\[Mu]", "]"}], "+", 
       RowBox[{
        RowBox[{"T", "[", 
         RowBox[{"lb", ",", "\"\<u\>\""}], "]"}], "[", "\[Mu]", "]"}]}]}]}], 
    ",", "\[IndentingNewLine]", "NL", ",", 
    "\"\<And choosing a basis with light-like vectors: \>\"", ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"T", "[", 
         RowBox[{"e", ",", "\"\<d\>\""}], "]"}], "[", "1", "]"}], "->", 
       RowBox[{"{", 
        RowBox[{"1", ",", "1", ",", "0", ",", "0"}], "}"}]}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"T", "[", 
         RowBox[{"e", ",", "\"\<d\>\""}], "]"}], "[", "2", "]"}], "->", 
       RowBox[{"{", 
        RowBox[{"1", ",", 
         RowBox[{"-", "1"}], ",", "0", ",", "0"}], "}"}]}]}], "}"}], ",", 
    "\[IndentingNewLine]", "\"\< and two space-like basis vectors: \>\"", ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"T", "[", 
         RowBox[{"e", ",", "\"\<d\>\""}], "]"}], "[", "3", "]"}], "->", 
       RowBox[{"{", 
        RowBox[{"0", ",", "0", ",", "1", ",", "0"}], "}"}]}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"T", "[", 
         RowBox[{"e", ",", "\"\<d\>\""}], "]"}], "[", "4", "]"}], "->", 
       RowBox[{"{", 
        RowBox[{"0", ",", "0", ",", "0", ",", "1"}], "}"}]}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    "\"\< where we can recover the traditional time-space basis by linear \
combinations of \>\"", ",", 
    RowBox[{"e", "@", 
     RowBox[{"d", "[", "_", "]"}]}], ",", 
    "\"\<'s.  Remember that the contravariant forms are: \>\"", ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"e", "@", 
        RowBox[{"u", "[", "1", "]"}]}], "->", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"-", "1"}], ",", "1", ",", "0", ",", "0"}], "}"}]}], ",", 
      RowBox[{
       RowBox[{"e", "@", 
        RowBox[{"u", "[", "2", "]"}]}], "->", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"-", "1"}], ",", 
         RowBox[{"-", "1"}], ",", "0", ",", "0"}], "}"}]}]}], "}"}], ",", 
    "\[IndentingNewLine]", "NL", ",", 
    "\"\<We form an antisymmetric tensor by the outer bivector basis of \
these: For example, \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Wedge", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"T", "[", 
          RowBox[{"e", ",", "\"\<d\>\""}], "]"}], "[", "1", "]"}], " ", ",", 
        " ", 
        RowBox[{
         RowBox[{"T", "[", 
          RowBox[{"e", ",", "\"\<d\>\""}], "]"}], "[", "2", "]"}]}], "]"}], 
      ",", "\[IndentingNewLine]", 
      RowBox[{"Wedge", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"T", "[", 
          RowBox[{"e", ",", "\"\<d\>\""}], "]"}], "[", "2", "]"}], " ", ",", 
        " ", 
        RowBox[{
         RowBox[{"T", "[", 
          RowBox[{"e", ",", "\"\<d\>\""}], "]"}], "[", "4", "]"}]}], "]"}]}], 
     "}"}], ",", "    ", "\[IndentingNewLine]", "NL", ",", 
    "\"\<So the EM energy-momentum tensor, can be represented as a sum of \
bivectors. \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"tmpp", "=", 
      RowBox[{"Permutations", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"1", ",", "2", ",", "3", ",", "4"}], "}"}], ",", 
        RowBox[{"{", "2", "}"}]}], "]"}]}], ";", 
     "                           ", "\[IndentingNewLine]", 
     RowBox[{"tmpp", "=", 
      RowBox[{
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Sort", "[", "#", "]"}], "&"}], ",", "tmpp"}], "]"}], "//", 
       "DeleteDuplicates"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"tmpp", "=", 
      RowBox[{"Apply", "[", 
       RowBox[{"Plus", ",", 
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"T", "[", 
              RowBox[{"A", ",", "\"\<dd\>\""}], "]"}], "[", 
             RowBox[{"Apply", "[", 
              RowBox[{"Sequence", ",", "#"}], "]"}], "]"}], 
            RowBox[{
             RowBox[{"T", "[", 
              RowBox[{"e", ",", "\"\<dd\>\""}], "]"}], "[", 
             RowBox[{"Apply", "[", 
              RowBox[{"Sequence", ",", "#"}], "]"}], "]"}]}], "&"}], ",", 
          "tmpp"}], "]"}]}], "]"}]}]}], ",", "\"\< where \>\"", ",", 
    RowBox[{
     RowBox[{"subl", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"T", "[", 
         RowBox[{"e", ",", "\"\<dd\>\""}], "]"}], "[", 
        RowBox[{"i_", ",", "j_"}], "]"}], "->", 
       RowBox[{"Wedge", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"T", "[", 
           RowBox[{"e", ",", "\"\<d\>\""}], "]"}], "[", "i", "]"}], ",", 
         RowBox[{
          RowBox[{"T", "[", 
           RowBox[{"e", ",", "\"\<d\>\""}], "]"}], "[", "j", "]"}]}], 
        "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"subl", "=", 
      RowBox[{"{", 
       RowBox[{"subl", ",", 
        RowBox[{"(", 
         RowBox[{"subl", "/.", 
          RowBox[{"sRaiseAllIndices", "[", 
           RowBox[{"{", "}"}], "]"}]}], ")"}]}], "}"}]}]}]}], 
