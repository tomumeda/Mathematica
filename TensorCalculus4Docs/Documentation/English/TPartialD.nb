  Cell[BoxData[
      RowBox[{
        StyleBox["PartialD",
          "MR"], 
        StyleBox["[",
          "MR"], 
        StyleBox[\(tensor, {i, j,  ... }\),
          "TI"], 
        StyleBox["]",
          "MR"]}]]],
  " represents higher order derivatives.\n\[FilledSmallSquare] ",
  Cell[BoxData[
      RowBox[{
        RowBox[{
          StyleBox["PartialD",
            "MR"], 
          StyleBox["[",
            "MR"], 
          RowBox[{
            StyleBox["{",
              "MR"], 
            StyleBox[\(x, \[Delta], g, \[CapitalGamma]\),
              "TI"], 
            StyleBox["}",
