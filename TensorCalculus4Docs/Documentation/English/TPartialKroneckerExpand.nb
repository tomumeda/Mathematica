    \(\[Delta]uuuudddd[i_, j_, k_, l_, r_, s_, t_, u_] := 
      Tensor[\[Delta], {i, j, k, l, Void, Void, Void, Void}, {Void, Void, 
          Void, Void, r, s, t, u}]\), "\n", 
