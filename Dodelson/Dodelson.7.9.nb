    "\[IndentingNewLine]", 
    RowBox[{"hold784F", "=", 
     RowBox[{
      RowBox[{"HoldForm", "[", " ", 
       RowBox[{
        SubscriptBox["\[CapitalDelta]", "R"], "[", 
        OverscriptBox["k", "\[RightVector]"], "]"}], " ", "]"}], "==", " ", 
      RowBox[{"HoldForm", "[", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"2", " ", "\[Pi]"}], ")"}], 
         RowBox[{"3", "/", "2"}]], " ", 
        RowBox[{"\[CapitalDelta]", "[", 
         OverscriptBox["k", "\[RightVector]"], "]"}], " ", 
        RowBox[{
         SubscriptBox["W", "R"], "[", 
         OverscriptBox["k", "\[RightVector]"], "]"}]}], " ", "]"}]}]}], ",", 
    "\"\<\nwhere the capital Greek letters indicate k-space functions.\>\""}],
    "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{
   "\"\<The definition of Fourier transform of a top-hat function : \>\"", 
    ",", 
    RowBox[{"hold", "=", 
     RowBox[{"HoldForm", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        SubscriptBox["W", "R"], "[", "k_", "]"}], ":=", 
       RowBox[{
        FractionBox["1", 
         RowBox[{"4", " ", "\[Pi]", " ", 
          RowBox[{
           SuperscriptBox["R", "3"], "/", "3"}]}]], " ", "2", " ", "\[Pi]", 
        RowBox[{
         SubsuperscriptBox["\[Integral]", "0", "R"], 
         RowBox[{
          SuperscriptBox["x", "2"], 
          RowBox[{"(", 
           RowBox[{
            SubsuperscriptBox["\[Integral]", 
             RowBox[{"-", "1"}], "1"], 
            RowBox[{
             SuperscriptBox["\[ExponentialE]", 
              RowBox[{"\[ImaginaryI]", " ", "k", " ", "x", " ", "\[Mu]"}]], 
             RowBox[{"\[DifferentialD]", "\[Mu]"}]}]}], ")"}], 
          RowBox[{"\[DifferentialD]", "x"}]}]}]}]}], "]"}]}], ",", 
    "\"\<\n(assume k in z direction) is :  \>\""}], "]"}], 
