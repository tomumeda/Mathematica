Cell["\<\
TensorCheck[expr_]:=
Module[{number0,notensortensor,work},
number0 = {
Tensor[a_] \[RuleDelayed] NumberQ[a] Tensor[a]
};
notensortensor ={ Tensor[Tensor[a_]] \[Rule] Tensor[a]};
work = expr/.notensortensor;
work = work/.number0;
work = work /. True \[Rule] 0;
work = work /. False \[Rule] 1
]
(*****End ContantTensor*******************)

ConstantTensorDif[expr_]:=
Module[{constant0,number0,work},
constant0 = {
Tensor[a_,List[Void],List[Dif[k_]]] \[RuleDelayed] \
MemberQ[Attributes[a],Constant] Tensor[a,List[Void],List[Dif[k]]] };
number0 = {
Tensor[a_,List[Void],List[Dif[k_]]] \[RuleDelayed] NumberQ[a] \
Tensor[a,List[Void],List[Dif[k]]]
};
work = expr/.constant0;
work = work/.number0;
work = work /. True \[Rule] 0;
work = work /. False \[Rule] 1
]
(*****End ContantTensorDif*******************)

ExpandTensor[expr_]:=
Module[{difSum,difProd,work},
difSum =
{ Tensor[Plus[a_,b_],List[Void],List[Dif[k_]]] \[Rule]  
  Plus[ 
  Tensor[a, List[Void],List[Dif[k]]],         
  Tensor[b, List[Void],List[Dif[k]]] ]};
difProd =
{ Tensor[Times[a_,b_],List[Void],List[Dif[k_]]] \[Rule]
  Plus[
  Tensor[a,List[Void],List[Dif[k]]]Tensor[b],         
  Tensor[a] Tensor[b, List[Void],List[Dif[k]]] 
  ]};
tensorPlus =
{ Tensor[Plus[a_,b_]] \[Rule]
  Plus[Tensor[a], Tensor[b] ]}; 
  
  work = expr//.difSum;
  work = work//.difProd;
  work = work//.tensorPlus;
  work = work//ExpandAll;
  work = work//TensorCheck
 ] 
 (******* End ExpandTensor[expr] ***) 
 
