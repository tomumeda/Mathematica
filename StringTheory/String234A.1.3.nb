Cell[BoxData[
 RowBox[{"PR", "[", 
  RowBox[{"CO", "[", 
   RowBox[{
   "\"\<In quantum field theory, the momentum-space propagator for a paticle \
of mass m in D->d+1 dimensional Minkowski space equals \>\"", ",", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"2", "\[Pi]"}], ")"}], "^", "D"}], " ", 
     RowBox[{
      RowBox[{"\[Delta]", "[", 
       RowBox[{"p", "-", 
        RowBox[{"p", "'"}]}], "]"}], "/", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"p", "^", "2"}], "+", 
        RowBox[{"m", "^", "2"}]}], ")"}]}]}], ",", "\"\< Here, \>\"", ",", 
    RowBox[{
     RowBox[{"p", "^", "2"}], "->", 
     RowBox[{
      RowBox[{
       RowBox[{"T", "[", 
        RowBox[{"p", ",", "\"\<u\>\""}], "]"}], "[", "\[Mu]", "]"}], 
      RowBox[{
       RowBox[{"T", "[", 
        RowBox[{"p", ",", "\"\<u\>\""}], "]"}], "[", "\[Nu]", "]"}], "  ", 
      RowBox[{
       RowBox[{"T", "[", 
        RowBox[{"\[Eta]", ",", "\"\<dd\>\""}], "]"}], "[", 
       RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}]}]}], ",", 
    "\"\<, p and p' are the momenta of the incoming and outgoing particle, \
and \[Delta] is a D-dimensinal delta function.  In this problem, we will \
derive this propagator from the path integral \>\"", ",", 
    RowBox[{"IntegralOp", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"\[ScriptCapitalD]", "[", "h", "]"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"\[ScriptCapitalD]", "[", "X", "]"}], "}"}]}], "}"}], ",", 
      RowBox[{"Exp", "[", 
       RowBox[{
        RowBox[{"-", "I"}], " ", 
        RowBox[{"S", "[", 
         RowBox[{"X", ",", "h"}], "]"}]}], "]"}]}], "]"}], ",", 
    "\"\< where S is the action in (1.1), and the integral over h should be \
understood as an integral over all metrics \>\"", ",", 
    RowBox[{
     RowBox[{"T", "[", 
      RowBox[{"h", ",", "\"\<dd\>\""}], "]"}], "[", 
     RowBox[{"\[Tau]", ",", "\[Tau]"}], "]"}], ",", 
    "\"\< on the interval, modulo diffeomorphisms.  \nIt is some what simpler \
to consider first the amplitude for a particle to propagate from \
\!\(\*SubscriptBox[\(X\), \(0\)]\) to \!\(\*SubscriptBox[\(X\), \(1\)]\): \
\>\"", ",", 
    RowBox[{"tmpXX", "=", 
     RowBox[{
      RowBox[{"BraKet", "[", 
       RowBox[{
        SubscriptBox["X", "0"], ",", 
        SubscriptBox["X", "1"]}], "]"}], "->", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"IntegralOp", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"\[ScriptCapitalD]", "[", "h", "]"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"\[ScriptCapitalD]", "[", "X", "]"}], ",", 
            SubscriptBox["X", "0"], ",", 
            SubscriptBox["X", "1"]}], "}"}]}], "}"}], ",", 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "I"}], "/", "2"}], " ", 
          RowBox[{"IntegralOp", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"\[Tau]", "[", "\[CapitalSigma]", "]"}], "}"}], ",", 
            RowBox[{
             SqrtBox[
              RowBox[{"-", 
               RowBox[{
                RowBox[{"T", "[", 
                 RowBox[{"h", ",", "\"\<dd\>\""}], "]"}], "[", 
                RowBox[{"\[Tau]", ",", "\[Tau]"}], "]"}]}]], 
             RowBox[{"(", 
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{"T", "[", 
                  RowBox[{"h", ",", "\"\<uu\>\""}], "]"}], "[", 
                 RowBox[{"\[Tau]", ",", "\[Tau]"}], "]"}], 
                RowBox[{"xPartialD", "[", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"T", "[", 
                    RowBox[{"X", ",", "\"\<u\>\""}], "]"}], "[", "\[Nu]", 
                   "]"}], ",", "\[Tau]"}], "]"}], 
                RowBox[{"xPartialD", "[", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"T", "[", 
                    RowBox[{"X", ",", "\"\<u\>\""}], "]"}], "[", "\[Mu]", 
                   "]"}], ",", "\[Tau]"}], "]"}], 
                RowBox[{
                 RowBox[{"T", "[", 
                  RowBox[{"\[Eta]", ",", "\"\<dd\>\""}], "]"}], "[", 
                 RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}]}], "-", 
               RowBox[{"m", "^", "2"}]}], ")"}]}]}], "]"}]}], "]"}]}], 
       "]"}]}]}], ",", 
    "\"\< Here we have arbitrarily set the propagation over {\[Tau],0,1}; due \
to reparametrization invariance, the answer only depends on \>\"", ",", 
    RowBox[{"tmpL", "=", 
     RowBox[{"L", "->", 
      RowBox[{
       RowBox[{"IntegralOp", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"{", 
           RowBox[{"\[Tau]", ",", "0", ",", "1"}], "}"}], "}"}], ",", 
         SqrtBox[
          RowBox[{"-", "h"}]]}], "]"}], "->", 
       RowBox[{"IntegralOp", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"{", 
           RowBox[{"\[Tau]", ",", "0", ",", "1"}], "}"}], "}"}], ",", 
         RowBox[{"e", "[", "\[Tau]", "]"}]}], "]"}]}]}]}], ",", 
    "\"\< where \>\"", ",", 
    RowBox[{"tmph", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"T", "[", 
        RowBox[{"h", ",", "\"\<dd\>\""}], "]"}], "[", 
       RowBox[{"\[Tau]", ",", "\[Tau]"}], "]"}], "->", 
      RowBox[{"-", 
       RowBox[{
        RowBox[{"e", "[", "\[Tau]", "]"}], "^", "2"}]}]}]}], ",", 
    "\"\<.  Without changing the endpoints, we can use reparmetrization \
invariance to set e[\[Tau]] to a constant, in this case \>\"", ",", 
    RowBox[{"tmpe", "=", 
     RowBox[{
      RowBox[{"e", "[", "\[Tau]", "]"}], "->", "L"}]}], ",", "\"\<.\>\""}], 
   "]"}], "\[IndentingNewLine]", "]"}]], "Input",
