 RowBox[{"PR", "[", 
  RowBox[{
   RowBox[{
   "CO", "[", 
    "\"\<1.1:  What is the Hamiltonian H for this system?  Find the spectrum \
of the Hamiltonian and the corresponding basis of eigenstates.\>\"", "]"}], 
   ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<The Hamiltonian for the harmonic oscillator: \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"tmpH", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"H", "->", 
       RowBox[{
        RowBox[{
         RowBox[{"p", "^", "2"}], "/", "2"}], "+", 
        RowBox[{
         RowBox[{"k", "^", "2"}], " ", 
         RowBox[{
          RowBox[{"x", "^", "2"}], "/", "2"}]}]}]}], ",", 
      RowBox[{"p", "->", 
       RowBox[{
        RowBox[{"-", "I"}], " ", 
        RowBox[{"xPartialD", "[", 
         RowBox[{"_", ",", "x"}], "]"}]}]}]}], "}"}]}], ",", 
   "\[IndentingNewLine]", "NL", ",", 
   "\"\<And the commutation relationship: \>\"", ",", 
   RowBox[{"tmpxp", "=", 
    RowBox[{
     RowBox[{"MCommutator", "[", 
      RowBox[{"x", ",", "p"}], "]"}], "->", "I"}]}], ",", 
   "\[IndentingNewLine]", "\"\<POFF\>\"", ",", "\[IndentingNewLine]", "Yield",
    ",", 
   RowBox[{"tmp", "=", 
    RowBox[{"tmp", "/.", "tmpH"}]}], ",", "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{"Map", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"#", ".", "fn"}], "&"}], ",", "tmp"}], "]"}]}], ",", 
   "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{"tmp", "//.", 
     RowBox[{"simpleDot2", "[", 
      RowBox[{"{", "}"}], "]"}]}]}], ",", "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{"tmp", "/.", " ", 
     RowBox[{
      RowBox[{"Dot", "[", 
       RowBox[{
        RowBox[{"xPartialD", "[", 
         RowBox[{"_", ",", "x"}], "]"}], ".", "Q__"}], "]"}], ":>", 
      RowBox[{"xPartialD", "[", 
       RowBox[{
        RowBox[{"Dot", "[", "Q", "]"}], ",", "x"}], "]"}]}]}]}], ",", 
   "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{
     RowBox[{"tmp", "//", 
      RowBox[{"DerivativeExpand", "[", 
       RowBox[{"{", "}"}], "]"}]}], "//", "Expand"}]}], ",", 
   "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{
     RowBox[{"tmp", "//.", 
      RowBox[{"simpleDot2", "[", 
       RowBox[{"{", "}"}], "]"}]}], "/.", 
     RowBox[{"fn", "->", "1"}]}]}], ",", "\[IndentingNewLine]", "\"\<PON\>\"",
    ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<Defining creation, annihilation operators: \>\"", ",", 
   RowBox[{"suba", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"a", "->", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"k", " ", "x"}], " ", "+", " ", 
          RowBox[{"I", " ", 
           RowBox[{"p", " ", "/", "k"}]}]}], ")"}], "/", 
        SqrtBox["2"]}]}], ",", 
      RowBox[{
       RowBox[{"SuperDagger", "[", "a", "]"}], "->", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"k", " ", "x"}], " ", "-", " ", 
          RowBox[{"I", " ", 
           RowBox[{"p", " ", "/", "k"}]}]}], ")"}], "/", 
        SqrtBox["2"]}]}]}], "}"}]}], ",", "\[IndentingNewLine]", "Imply", ",", 
   RowBox[{"subxp", "=", 
    RowBox[{"RuleX", "[", 
     RowBox[{"suba", ",", 
      RowBox[{"{", 
       RowBox[{"p", ",", "x"}], "}"}]}], "]"}]}], ",", "\[IndentingNewLine]", 
   "NL", ",", "\"\<Check commutation relationship for a's: \>\"", ",", 
   "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"tmp", "=", "tmpxp"}], ",", "\[IndentingNewLine]", "yield", ",", 
   RowBox[{
    RowBox[{"tmp", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"tmp", "/.", 
        RowBox[{"RuleX", "[", 
         RowBox[{"suba", ",", 
          RowBox[{"{", 
           RowBox[{"p", ",", "x"}], "}"}]}], "]"}]}], "//", 
       RowBox[{"DotExpandAll2", "[", 
        RowBox[{"{", "k", "}"}], "]"}]}], "//", "Simplify"}]}], ";", 
    RowBox[{"subaa", "=", 
