       RowBox[{"subaa", ",", 
        RowBox[{"a", ".", 
         RowBox[{"SuperDagger", "[", "a", "]"}]}]}], "]"}]}], "//", 
     RowBox[{"DotExpandAll2", "[", 
      RowBox[{"{", "k", "}"}], "]"}]}]}], ",", "\[IndentingNewLine]", "yield",
    ",", 
   RowBox[{"tmp", "=", 
