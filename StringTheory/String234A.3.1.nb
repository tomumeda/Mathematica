   RowBox[{"CO", "[", 
    RowBox[{
    "\"\<2: Free Boson CFT\nConsider a theory of a single free boson X(z,\!\(\
\*OverscriptBox[\(z\), \(~\)]\)) on a plane,\>\"", ",", "\n", 
     RowBox[{"tmpS", "=", 
      RowBox[{"S", "->", 
       RowBox[{
        FractionBox["1", 
         RowBox[{"2", "\[Pi]", " ", 
          RowBox[{"\[Alpha]", "'"}]}]], 
        RowBox[{"IntegralOp", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", "z", "}"}], ",", 
            RowBox[{"{", 
             OverscriptBox["z", "~"], "}"}]}], "}"}], ",", 
          RowBox[{
           RowBox[{"xPartialD", "[", 
            RowBox[{"X", ",", "z"}], "]"}], ".", 
           RowBox[{"xPartialD", "[", 
            RowBox[{"X", ",", 
             OverscriptBox["z", "~"]}], "]"}]}]}], "]"}]}]}]}], ",", 
     RowBox[{"(*", 
      RowBox[{
      "\"\< where \[PartialD] = \!\(\*SubscriptBox[\(\[PartialD]\), \(z\)]\), \
\!\(\*OverscriptBox[\(\[PartialD]\), \(~\)]\) = \!\(\*SubscriptBox[\(\
\[PartialD]\), OverscriptBox[\(z\), \(~\)]]\).\>\"", ","}], "*)"}], "\n", 
     "NL", ",", "New", ",", "\"\<2.1  Work out the relation between \>\"", 
     ",", "\n", 
     RowBox[{"tmpXX", "=", " ", 
      RowBox[{
       RowBox[{"X", "[", 
