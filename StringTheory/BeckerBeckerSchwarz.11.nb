  RowBox[{
  "\"\<Example of \[ScriptCapitalN]\[Rule]8 supersymmetric black holes in \
5-dim with 3 different charges containing 1-form and 2-form gauge fields.  \
This theory contains 27 U[1] gauge fields and has noncompact \
\!\(\*SubscriptBox[\(E\), \(6, 6\)]\) global U-duality symmetry.\>\"", ",", 
   "\[IndentingNewLine]", "line", ",", "NL", ",", 
   RowBox[{"CO", "[", "\"\<Black hole solution\>\"", "]"}], ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<3-charge BH in 5-dim\>\"", ",", 
   "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{
    RowBox[{"$", "=", 
     RowBox[{"{", 
      RowBox[{
      "\"\<\!\(\*SubscriptBox[\(Q\), \(1\)]\) D1-branes[on \
\!\(\*SuperscriptBox[\(S\), \(1\)]\)[radius R] inside \
\!\(\*SuperscriptBox[\(T\), \(5\)]\)]\>\"", ",", "\[IndentingNewLine]", 
       "\"\<\!\(\*SubscriptBox[\(Q\), \(5\)]\) D5-branes on \
\!\(\*SuperscriptBox[\(T\), \(5\)]\)\[Rule]\!\(\*SuperscriptBox[\(T\), \(4\)]\
\)\[Cross]\!\(\*SuperscriptBox[\(S\), \(1\)]\)\>\"", ",", 
       "\[IndentingNewLine]", 
       "\"\<n units of KK momentum along the same circle\>\""}], " ", "}"}]}],
     ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"ColumnForms", "[", 
        RowBox[{"#1", ",", "2"}], "]"}], "&"}], ")"}], "[", "$", "]"}]}], ",",
    "\[IndentingNewLine]", "line", ",", "NL", ",", 
   "\"\<5-dim metric from 10-dim IIB theory \>\"", ",", "\[IndentingNewLine]",
    "Yield", ",", 
   RowBox[{
    RowBox[{
     RowBox[{"e", "[", "11.46", "]"}], "=", 
     RowBox[{"$", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         SuperscriptBox[
          RowBox[{"d", "[", "s", "]"}], "2"], "\[Rule]", 
         RowBox[{
          RowBox[{
           RowBox[{"-", 
            SuperscriptBox["\[Lambda]", 
             RowBox[{
              RowBox[{"-", "2"}], "/", "3"}]]}], 
           SuperscriptBox[
            RowBox[{"d", "[", "t", "]"}], "2"]}], "+", 
          RowBox[{
           SuperscriptBox["\[Lambda]", 
            RowBox[{"1", "/", "3"}]], 
           RowBox[{"(", 
            RowBox[{
             SuperscriptBox[
              RowBox[{"d", "[", "r", "]"}], "2"], "+", 
             RowBox[{
              SuperscriptBox["r", "2"], 
              SuperscriptBox[
               RowBox[{"d", "[", 
                SubscriptBox["\[CapitalOmega]", "3"], "]"}], "2"]}]}], 
            ")"}]}]}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"\[Lambda]", "\[Rule]", 
         RowBox[{"xProduct", "[", 
          RowBox[{
           RowBox[{"1", "+", 
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{
               SubscriptBox["r", "i"], "/", "r"}], ")"}], "2"]}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "3"}], "}"}]}], "]"}]}], ",", 
        RowBox[{
         RowBox[{"{", "i", "}"}], "[", 
         RowBox[{"CG", "[", "\"\<associated with charges\>\"", "]"}], "]"}]}],
        "}"}]}]}], ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"ColumnForms", "[", 
        RowBox[{"#1", ",", "2"}], "]"}], "&"}], ")"}], "[", "$", "]"}]}], ",", 
   RowBox[{"CG", "[", "\"\<e[11.46]\>\"", "]"}], ",", "\[IndentingNewLine]", 
   "NL", ",", "\"\<BH mass from \>\"", ",", 
   RowBox[{"$", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"e", "[", "11.9", "]"}], "//", 
      RowBox[{"tuRuleSelect", "[", 
       SubscriptBox["r", "s"], "]"}]}], "//", "tuPatternRemove"}]}], ",", 
   "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{
    RowBox[{"$", "//", 
