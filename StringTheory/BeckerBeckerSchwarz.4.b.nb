           RowBox[{
            RowBox[{"Table", "[", 
             RowBox[{"i", ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", 
                RowBox[{
                 RowBox[{"-", "1"}], "/", "2"}], ",", "totalEn", ",", 
                RowBox[{"-", "1"}]}], "}"}]}], "]"}], ",", 
            RowBox[{"Table", "[", 
             RowBox[{"i", ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", 
                RowBox[{"-", "1"}], ",", "totalEn", ",", 
                RowBox[{"-", "1"}]}], "}"}]}], "]"}]}], "]"}]}]}], ";", 
