  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"statesNS", "[", "totalEn_", "]"}], ":=", 
     RowBox[{"Block", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"$", ",", "$0", ",", "$v", ",", "$s", ",", "$b", ",", "a"}], 
