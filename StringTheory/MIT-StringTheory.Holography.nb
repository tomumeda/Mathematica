          RowBox[{"\[ScriptCapitalO]", "[", "x", "]"}]}], ",", 
         SuperscriptBox["x", "4"]}], "]"}], ",", 
       RowBox[{
        RowBox[{"\[ScriptCapitalO]", "[", "x", "]"}], "\[Rule]", 
        RowBox[{"Tr", "[", 
         RowBox[{
          SubscriptBox["\[Phi]", "1"], ".", 
          SubscriptBox["\[Phi]", "2"], ".", "\[CenterEllipsis]"}], "]"}]}], 
       ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{
