     RowBox[{"MCommutator", "[", 
      RowBox[{
       RowBox[{"\[CurlyPhi]", "[", 
        RowBox[{"x", "[", "i", "]"}], "]"}], ",", 
       RowBox[{"\[CapitalPi]", "[", 
        RowBox[{"x", "[", "j", "]"}], "]"}]}], "]"}], "==", 
     RowBox[{"I", " ", 
