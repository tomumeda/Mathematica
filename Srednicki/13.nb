     RowBox[{"\[ScriptCapitalL]", "[", 
      RowBox[{
       RowBox[{"\[CurlyPhi]", "[", "x", "]"}], ",", 
       RowBox[{
        OverscriptBox["\[CurlyPhi]", "."], "[", "x", "]"}]}], "]"}], "->", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        FractionBox["1", "2"]}], 
       SubscriptBox["Z", "\[CurlyPhi]"], 
       RowBox[{"guu", "[", 
        RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}], 
       RowBox[{"PartialD", "[", 
        RowBox[{
         RowBox[{"\[CurlyPhi]", "[", 
          RowBox[{"x", "[", "\[Eta]", "]"}], "]"}], ",", "\[Mu]"}], "]"}], 
