       RowBox[{
        SuperscriptBox[
         RowBox[{"\[CurlyEpsilon]", "[", "r", "]"}], "2"], 
        RowBox[{"A", "[", "r", "]"}]}], ",", 
       RowBox[{"r", "\[Element]", 
        OverscriptBox[
