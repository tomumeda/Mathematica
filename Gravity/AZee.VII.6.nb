      RowBox[{"tuOpGather", "[", "Log", "]"}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"$", "[", 
      RowBox[{"[", "1", "]"}], "]"}], "=", "t"}], ";", 
