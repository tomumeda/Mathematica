Cell[BoxData[
 RowBox[{"PR", "[", 
  RowBox[{
   RowBox[{"CO", "[", 
    RowBox[{
    "\"\<Exercise 1. Show that a photon moving in a radial direction in a \
subextremal Reissner-Nordstrom black hole follows a path determined by \>\"", 
     ",", 
     RowBox[{
      RowBox[{"tuDPartial", "[", 
       RowBox[{"t", ",", "r"}], "]"}], "\[Rule]", 
      RowBox[{"pm", "[", 
       RowBox[{
        SuperscriptBox["r", "2"], "/", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"r", "-", 
            SubscriptBox["r", "\"\<+\>\""]}], ")"}], 
          RowBox[{"(", 
           RowBox[{"r", "-", 
            SubscriptBox["r", "\"\<-\>\""]}], ")"}]}], ")"}]}], "]"}]}]}], 
    "]"}], ",", "\[IndentingNewLine]", "line", ",", "NL", ",", 
   "\"\<The RN metric: \>\"", ",", 
   RowBox[{"$", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"e", "[", "2.1", "]"}], "[", 
      RowBox[{"[", "1", "]"}], "]"}], "/.", 
     RowBox[{
      RowBox[{"e", "[", "2.1", "]"}], "[", 
      RowBox[{"[", "2", "]"}], "]"}]}]}], ",", "\[IndentingNewLine]", "NL", 
   ",", "\"\<In the radial case for a photon \>\"", ",", 
   RowBox[{"$s", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"d", "[", "\[CapitalOmega]", "]"}], "\[Rule]", "0"}], ",", 
      RowBox[{
       RowBox[{"d", "[", "s", "]"}], "\[Rule]", "0"}]}], "}"}]}], ",", 
   "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"$", "=", 
    RowBox[{"$", "/.", "$s"}]}], ",", "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"$", "=", 
    RowBox[{"tuRuleSolve", "[", 
     RowBox[{"$", ",", 
      RowBox[{"d", "[", "r", "]"}]}], "]"}]}], ",", "\[IndentingNewLine]", 
   "Imply", ",", 
   RowBox[{"$vel", "=", 
    RowBox[{"$", "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"#", "/", 
             RowBox[{"d", "[", "t", "]"}]}], "&"}], "/@", "#"}], "&"}], "/@", 
         "$"}], "//", 
        RowBox[{"tuOpGather", "[", "Sqrt", "]"}]}], "//", "Simplify"}], "//", 
      "PowerExpand"}]}]}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<Apparently the velocity \>\"", ",", 
   RowBox[{
    RowBox[{"$", "[", 
     RowBox[{"[", 
      RowBox[{"1", ",", "1"}], "]"}], "]"}], "\[Rule]", "0"}], ",", 
   "\"\< at \>\"", ",", 
   RowBox[{"r", "->", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["r", "\"\<-\>\""], ",", 
      SubscriptBox["r", "\"\<+\>\""]}], "}"}]}], ",", "\[IndentingNewLine]", 
   "NL", ",", "\"\<\[FilledCircle] The accelerations \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"$1", "=", 
    RowBox[{"tuDPartial", "[", 
     RowBox[{
      RowBox[{"$", "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "2"}], "]"}], "]"}], ",", "t"}], "]"}]}], ",", 
   "\"\<POFF\>\"", ",", "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"$1", "=", 
    RowBox[{"$1", "//", 
     RowBox[{"tuDerivativeExpand", "[", 
      RowBox[{"{", 
       RowBox[{
        SubscriptBox["r", "\"\<-\>\""], ",", 
        SubscriptBox["r", "\"\<+\>\""]}], "}"}], "]"}]}]}], ",", 
   "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"$s", "=", 
    RowBox[{
     RowBox[{"tuDPartial", "[", 
      RowBox[{"r", ",", "t"}], "]"}], "->", 
     RowBox[{"$", "[", 
      RowBox[{"[", 
       RowBox[{"1", ",", "2"}], "]"}], "]"}]}]}], ",", "\"\<PON\>\"", ",", 
   "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"$accel", "=", 
    RowBox[{"$1", "=", 
     RowBox[{
      RowBox[{"tuDPartial", "[", 
       RowBox[{
        RowBox[{"$s", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", "t"}], "]"}], "->", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"$1", "/.", "$s"}], "//", "Simplify"}], ")"}]}]}]}], ",", 
   "\[IndentingNewLine]", "\[IndentingNewLine]", "NL", ",", 
   "\"\<\[FilledCircle] Integrating the inverse velocity\>\"", ",", 
   "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{
    RowBox[{"$", "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"1", "/", "#"}], "&"}], "/@", 
       RowBox[{"$", "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], "/.", 
      RowBox[{
       RowBox[{
        RowBox[{"d", "[", "t", "]"}], "/", 
        RowBox[{"d", "[", "r", "]"}]}], "\[Rule]", 
       RowBox[{"tuDPartial", "[", 
        RowBox[{"t", ",", "r"}], "]"}]}]}]}], ";", "Yield"}], ",", 
   RowBox[{"$", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"xIntegral", "[", 
       RowBox[{"#", ",", "r"}], "]"}], "&"}], "/@", "$"}]}], ",", 
   "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{
    RowBox[{
     RowBox[{"$", "[", 
      RowBox[{"[", "2", "]"}], "]"}], "=", 
