            RowBox[{"M", "/", "R"}]}]}], "]"}]}], ",", "\[IndentingNewLine]", 
        
        RowBox[{
         SubscriptBox["\[Alpha]", "m"], "\[Rule]", 
         RowBox[{"Sqrt", "[", 
          RowBox[{"1", "-", 
           RowBox[{"2", " ", "G", " ", 
            RowBox[{"m", "/", "R"}]}]}], "]"}]}]}], "\[IndentingNewLine]", 
