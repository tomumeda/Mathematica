   "\"\<Use transformation \>\"", ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"e", "[", "2", "]"}], "=", 
     RowBox[{"$", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         SubscriptBox["t", "i"], "\[Rule]", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["\[Alpha]", "M"], "/", 
            SubscriptBox["\[Alpha]", "m"]}], ")"}], "t"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          SubscriptBox["r", "i"], "[", 
          RowBox[{"R", "[", 
           RowBox[{"CG", "[", "\"\<radius of shell\>\"", "]"}], "]"}], "]"}], 
         "\[Rule]", "R"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"tuDPartial", "[", 
           RowBox[{
            SubscriptBox["r", "i"], ",", "r"}], "]"}], "[", "R", "]"}], "->", 
         
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["\[Alpha]", "M"], "/", 
           SubscriptBox["\[Alpha]", "m"]}], ")"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         SubscriptBox["\[Alpha]", "M"], "\[Rule]", 
