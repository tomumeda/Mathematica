Cell[BoxData[{
 RowBox[{
  RowBox[{"PR", "[", 
   RowBox[{
   "\"\<Right handed neutrinos are included because:\>\"", ",", 
    "\[IndentingNewLine]", "NL", ",", 
    RowBox[{
     RowBox[{"$", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"CG", "[", 
          RowBox[{
          "\"\<\[Bullet] L-neutrinos are light and the see-saw mechanism with \
R-neutrinos can be explanation\>\"", ",", "\[IndentingNewLine]", 
           "\"\<\[Bullet] mechanism for matter/anti-matter asymmetry via \
leptogenesis\>\"", ",", "\[IndentingNewLine]", 
           "\"\<\[Bullet] completes L-R pattern between leptons and \
quarks\>\"", ",", "\[IndentingNewLine]", 
           "\"\<\[Bullet] can be unified into Weyl spinor SO[10] \
irrepresentation\>\"", ",", "\[IndentingNewLine]", 
           "\"\<\[Bullet] one of right-handed neutrino can be dark-matter \
candidate\>\""}], "]"}], "\[IndentingNewLine]", "}"}], "[", 
        RowBox[{"CG", "[", "\"\<key points\>\"", "]"}], "]"}], 
       "\[IndentingNewLine]", "}"}]}], ";", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"ColumnForms", "[", 
         RowBox[{"#1", ",", "4"}], "]"}], "&"}], ")"}], "[", "$", "]"}]}], 
    ",", "\[IndentingNewLine]", "line", ",", "NL", ",", 
    "\"\<They choose flavor basis where \>\"", ",", 
    RowBox[{
     RowBox[{"M", "[", 
      RowBox[{"CG", "[", "\"\<3\[Cross]3 mass matrix diagonal\>\"", "]"}], 
      "]"}], "\[Rule]", 
     RowBox[{"DiagonalMatrix", "[", 
      RowBox[{"{", 
       RowBox[{
        SubscriptBox["M", "1"], ",", 
        SubscriptBox["M", "2"], ",", 
        SubscriptBox["M", "3"]}], "}"}], "]"}]}], ",", "\[IndentingNewLine]", 
    "NL", ",", "\"\< and \>\"", ",", 
    RowBox[{"$", "=", 
     RowBox[{"T", "[", 
      RowBox[{
       SubscriptBox["\[Nu]", "R"], ",", "\"\<d\>\"", ",", 
       RowBox[{"{", "1", "}"}]}], "]"}]}], ",", 
    "\"\< is the stable one (which means that its light partner is massless). \
 Hence, the first row of \>\"", ",", 
    SubscriptBox["Y", "\[Nu]"], ",", 
    "\"\< needs to be zero, otherwise, the Lagrangian term \>\"", ",", 
    RowBox[{
     RowBox[{
      RowBox[{"e", "[", "4.2", "]"}], "//", 
      RowBox[{"tuTermSelect", "[", 
       RowBox[{"ct", "[", 
        SubscriptBox["Y", "\[Nu]"], "]"}], "]"}]}], "//", "First"}], ",", 
    "\"\< will contribute to its decay.  They claim this implies a \
\!\(\*SubscriptBox[\(\[DoubleStruckCapitalZ]\), \(2\)]\) symmetry, \>\"", ",", 
    RowBox[{"$", "\[Rule]", 
     RowBox[{"-", "$"}]}], ",", "\"\<, and so \>\"", ",", "$", ",", 
    "\"\< only interacts with gravity.  They will show that \>\"", ",", "$", 
    ",", "\"\< is produced by gravity rather than the thermal bath.\>\""}], 
   "\[IndentingNewLine]", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"PR", "[", 
  RowBox[{
  "\"\<In other words, they choose a flavor basis where mass matrix is \
diagonal \>\"", ",", 
   RowBox[{"M", "\[Rule]", 
    RowBox[{"Table", "[", 
     RowBox[{
      SubscriptBox["M", "i"], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "1", ",", "3"}], "}"}]}], "]"}]}], " ", ",", 
   "\"\< and \>\"", ",", 
   RowBox[{
    RowBox[{"T", "[", 
     RowBox[{
      SubscriptBox["\[Nu]", "R"], ",", "\"\<u\>\"", ",", 
      RowBox[{"{", "1", "}"}]}], "]"}], "[", 
    RowBox[{"CG", "[", "\"\<is stable\>\"", "]"}], "]"}], ",", 
   "\"\< and \>\"", ",", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       SubscriptBox["Y", "\[Nu]"], "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{"1", ";;", "3"}], ",", "_"}], "]"}], "]"}], "\[Rule]", "0"}],
      "}"}], "[", 
    RowBox[{"CG", "[", 
     RowBox[{"\"\<no decay of\>\"", ",", 
      RowBox[{"T", "[", 
       RowBox[{
        SubscriptBox["\[Nu]", "R"], ",", "\"\<u\>\"", ",", 
        RowBox[{"{", "1", "}"}]}], "]"}]}], "]"}], "]"}], ",", 
   "\"\<\[DoubleLongRightArrow]\>\"", ",", 
   RowBox[{"CG", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"T", "[", 
       RowBox[{
        SubscriptBox["\[Nu]", "R"], ",", "\"\<u\>\"", ",", 
        RowBox[{"{", "1", "}"}]}], "]"}], "\[Rule]", 
      RowBox[{"-", 
       RowBox[{"T", "[", 
        RowBox[{
         SubscriptBox["\[Nu]", "R"], ",", "\"\<u\>\"", ",", 
         RowBox[{"{", "1", "}"}]}], "]"}]}]}], ",", 
     "\"\<\!\(\*SubscriptBox[\(\[DoubleStruckCapitalZ]\), \(2\)]\) symmetry\>\
\""}], "]"}]}], "\[IndentingNewLine]", "]"}]}], "Input",
