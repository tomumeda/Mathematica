 RowBox[{"PR", "[", 
  RowBox[{
  "\"\<\[FilledCircle] Follow the logic used in the paper to see if we can \
get eA[10a], eA[10b].\>\"", ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<Assume eA[8]: \>\"", ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"$", "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"eA", "[", 
          RowBox[{"2", ",", "\"\<-sep\>\""}], "]"}], "[", 
         RowBox[{"[", "1", "]"}], "]"}], "\[Rule]", 
        RowBox[{
         RowBox[{"c", "[", "p", "]"}], 
         RowBox[{
          RowBox[{"eA", "[", "\"\<7a\>\"", "]"}], "[", 
          RowBox[{"[", "1", "]"}], "]"}]}]}], "/.", 
       RowBox[{"s", "\[Rule]", "s"}]}]}], "}"}], "/.", "toTimes"}], ",", 
   RowBox[{
   "TEST", "[", "\"\<s\[Rule]-s/s\[Rule]s changes result for c[p]\>\"", "]"}],
    ",", "\[IndentingNewLine]", "Imply", ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"eA", "[", "9", "]"}], "=", 
    RowBox[{"$", "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
