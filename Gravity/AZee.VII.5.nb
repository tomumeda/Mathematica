      RowBox[{"e", "[", "8", "]"}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Expand", "[", "$", "]"}], "/.", 
        RowBox[{
         RowBox[{
          SqrtBox["a_"], "/", "b_"}], "->", 
         SqrtBox[
          RowBox[{"a", "/", 
           RowBox[{"b", "^", "2"}]}]]}]}], "//", "ExpandAll"}]}]}], ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"ColumnForms", "[", 
        RowBox[{"#1", ",", "2"}], "]"}], "&"}], ")"}], "[", "$", "]"}]}], ",", 
