  RowBox[{
  "\"\<Stationary Limit Surface on geodesic (for light emitted tangent to \
surface in equator plane parallel to directions of rotation): \>\"", ",", 
   "\[IndentingNewLine]", "NL", ",", 
   RowBox[{"$s", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"Dt", "[", "r", "]"}], "->", "0"}], ",", 
      RowBox[{
       RowBox[{"Dt", "[", "\[Theta]", "]"}], "->", "0"}], ",", 
      RowBox[{
       RowBox[{"Dt", "[", "s", "]"}], "->", "0"}]}], "}"}]}], ",", 
   "\[IndentingNewLine]", "Imply", ",", 
   RowBox[{"$", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"e", "[", "1", "]"}], "/.", 
      RowBox[{"d", "->", "Dt"}]}], "/.", "$s"}]}], ",", "\[IndentingNewLine]",
    "Imply", ",", 
   RowBox[{"$", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"tuRuleSolve", "[", 
       RowBox[{"$", ",", 
        RowBox[{"Dt", "[", "\[CurlyPhi]", "]"}]}], "]"}], "//", "Simplify"}], 
     "//", "PowerExpand"}]}], ",", "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{
    RowBox[{"$", "=", 
     RowBox[{
      RowBox[{"$", "/.", 
       RowBox[{
        RowBox[{"rr", ":", 
         RowBox[{"Rule", "[", 
          RowBox[{"a_", ",", "b_"}], "]"}]}], ":>", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"#", "/", 
            RowBox[{"Dt", "[", "t", "]"}]}], "&"}], ")"}], "/@", "rr"}]}]}], "//",
       "Simplify"}]}], ";", "\[IndentingNewLine]", "yield"}], ",", 
   RowBox[{
    RowBox[{"$", "=", 
