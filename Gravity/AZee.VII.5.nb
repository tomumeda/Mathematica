Cell[BoxData[
 RowBox[{"PR", "[", 
  RowBox[{"\"\<Extremize: \>\"", ",", 
   RowBox[{
    RowBox[{"$", "=", "$lag"}], ";", 
    RowBox[{"$", "=", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"$", "[", 
        RowBox[{"[", "2", "]"}], "]"}], "2"], "-", 
      SuperscriptBox["m", "2"]}]}]}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"$sv", "=", 
     RowBox[{"{", 
      RowBox[{"t", ",", "r", ",", "\[Theta]", ",", "\[CurlyPhi]"}], "}"}]}], 
    ";", "\[IndentingNewLine]", "NL"}], ",", 
   "\"\<\[Bullet] Assume: Stationary, cylindrical symmetry\>\"", ",", 
   "\[IndentingNewLine]", "imply", ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"T", "[", 
       RowBox[{"g", ",", "\"\<dd\>\""}], "]"}], "[", 
      RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}], "[", 
     RowBox[{"r", ",", "\[Theta]"}], "]"}], "}"}], ",", "\[IndentingNewLine]",
    "\[IndentingNewLine]", "NL", ",", "\"\<Using variables \>\"", ",", 
   RowBox[{"$sv", "=", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"$sv", ",", 
       RowBox[{"T", "[", 
        RowBox[{"g", ",", "\"\<dd\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"t", ",", "t"}], "}"}]}], "]"}], ",", 
       RowBox[{"T", "[", 
        RowBox[{"g", ",", "\"\<dd\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"t", ",", "\[CurlyPhi]"}], "}"}]}], "]"}], ",", 
       RowBox[{"T", "[", 
        RowBox[{"g", ",", "\"\<dd\>\"", ",", 
         RowBox[{"{", 
          RowBox[{"\[CurlyPhi]", ",", "\[CurlyPhi]"}], "}"}]}], "]"}]}], 
      "}"}], "//", "Flatten"}]}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<Euler-Lagrange equation: \>\"", ",", 
   RowBox[{
    RowBox[{"$", "=", 
     RowBox[{"$", "/.", 
      RowBox[{
       RowBox[{"d", "[", "a_", "]"}], ":>", 
       RowBox[{"tuDPartial", "[", 
        RowBox[{"a", ",", "s"}], "]"}]}]}]}], ";", 
    RowBox[{"$", "//", "ColumnSumExp"}], ";", "\[IndentingNewLine]", 
    RowBox[{"$", "=", 
     RowBox[{
      RowBox[{"tuOpIndependentVar", "[", 
       RowBox[{"tuEulerLagrangeTensor", ",", 
        RowBox[{"op$", "[", 
         RowBox[{"$", ",", "$sv", ",", 
          RowBox[{"{", "s", "}"}]}], "]"}]}], "]"}], "/.", 
      RowBox[{
       RowBox[{"s", "[", "dn", "]"}], "\[Rule]", "s"}]}]}], ";", 
    RowBox[{"$", "//", "ColumnSumExp"}], ";", "\[IndentingNewLine]", 
    RowBox[{"$", "=", 
     RowBox[{"$", "//", "tuRuleSimplify"}]}], ";", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"ColumnForms", "[", 
        RowBox[{"#1", ",", "2"}], "]"}], "&"}], ")"}], "[", "$", "]"}]}], ",",
    "\[IndentingNewLine]", "NL", ",", 
   RowBox[{"CR", "[", "\"\<Why the extra equation?\>\"", "]"}], ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<Manipulate eqns 1,4: \>\"", ",", 
   RowBox[{"$", "=", 
    RowBox[{"$", "[", 
     RowBox[{"[", 
      RowBox[{"{", 
       RowBox[{"1", ",", "4"}], "}"}], "]"}], "]"}]}], ",", 
   "\[IndentingNewLine]", "\"\<POFF\>\"", ",", "\[IndentingNewLine]", "Yield",
    ",", 
   RowBox[{
    RowBox[{"$s", "=", 
     RowBox[{
      RowBox[{"$", "[", 
       RowBox[{"[", "1", "]"}], "]"}], "//", 
      RowBox[{"tuTermExtract", "[", 
       RowBox[{
        RowBox[{"tuDPartial", "[", 
         RowBox[{"t", ",", "s"}], "]"}], 
        RowBox[{"tuDPartial", "[", 
         RowBox[{"b_", ",", "s"}], "]"}]}], "]"}]}]}], ";", 
    "\[IndentingNewLine]", "Yield"}], ",", 
   RowBox[{"$s", "=", 
    RowBox[{"$s", "\[Rule]", 
     RowBox[{"(", 
      RowBox[{"tuTotalDiffByParts", "[", 
       RowBox[{"$s", ",", 
