Cell[BoxData[
 RowBox[{"PR", "[", 
  RowBox[{
  "\"\<Penrose diagram from Minkowski metric radial coordinates: \>\"", ",", 
   RowBox[{"$", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"d", "[", "s", "]"}], "^", "2"}], "->", 
       RowBox[{
        RowBox[{"-", 
         RowBox[{
          RowBox[{"d", "[", "t", "]"}], "^", "2"}]}], "+", 
        RowBox[{
         RowBox[{"d", "[", "r", "]"}], "^", "2"}]}]}], ",", 
      RowBox[{"$r", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"r", ">", "0"}], ",", 
         RowBox[{
          RowBox[{"-", "\[Infinity]"}], "<", "t", "<", "\[Infinity]"}]}], 
        "}"}]}]}], "}"}]}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<The transformation: \>\"", ",", 
   RowBox[{"$s1", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"p", "->", 
       RowBox[{"t", "+", "r"}]}], ",", 
      RowBox[{"q", "->", 
       RowBox[{"t", "-", "r"}]}]}], "}"}]}], ",", "\[IndentingNewLine]", 
   "imply", ",", 
   RowBox[{"$s1i", "=", 
    RowBox[{"tuRuleSolve", "[", 
     RowBox[{"$s1", ",", 
      RowBox[{"{", 
       RowBox[{"t", ",", "r"}], "}"}]}], "]"}]}], ",", "\[IndentingNewLine]", 
   "Imply", ",", "\"\< Range: \>\"", ",", 
   RowBox[{"$r", "=", 
    RowBox[{"$r", "/.", "$s1i"}]}], ",", "\[IndentingNewLine]", "imply", ",", 
   
   RowBox[{"$r", "=", 
    RowBox[{"Reduce", "[", 
     RowBox[{"$r", ",", 
      RowBox[{"{", 
       RowBox[{"p", ",", "q"}], "}"}], ",", "Reals"}], "]"}]}], ",", 
   "\[IndentingNewLine]", "\[IndentingNewLine]", "line", ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<The transformation: \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"$s3", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"p", "->", 
       RowBox[{"Tan", "[", "P", "]"}]}], ",", 
      RowBox[{"q", "->", 
       RowBox[{"Tan", "[", "Q", "]"}]}]}], "}"}]}], ",", 
   "\[IndentingNewLine]", "Imply", ",", "\"\< Range: \>\"", ",", 
   RowBox[{
    RowBox[{"$r", "=", 
     RowBox[{"$r", "/.", "$s3"}]}], ";", "\[IndentingNewLine]", "imply"}], 
   ",", 
   RowBox[{"$r", "=", 
    RowBox[{"{", 
     RowBox[{"$r", ",", 
