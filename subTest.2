subSeries[A_] := A[\[Eta]] -> Sum[Subscript[A, l][\[Eta]]*LegendreP[l, \[Mu]]*
       (-I)^l*(2*l + 1), {l, 0, Infinity}]
 
Attributes[Subscript] = {NHoldRest}
 
subSeriesp[A_] := Derivative[1][A][\[Eta]] -> 
     Sum[Derivative[1][Subscript[A, l]][\[Eta]]*LegendreP[l, \[Mu]]*(-I)^l*
       (2*l + 1), {l, 0, Infinity}]
 
Attributes[Derivative] = {NHoldAll, ReadProtected}
