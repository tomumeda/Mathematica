Cell[BoxData[
 RowBox[{"PR1", "[", 
  RowBox[{
  "\"\<(c) The amplitude for the \!\(\*OverscriptBox[\(\[Nu]\), \(_\)]\)+e \
interaction has additional contribution from the s-channel W-exchange diagram \
(C) and where the spinor representation of anti-fermions \!\(\*OverscriptBox[\
\(\[Nu]\), \(_\)]\) is v[\[Nu]] where {1,2} label incoming and {3,4} label \
outgoing particles: \>\"", ",", "\[IndentingNewLine]", 
   RowBox[{"tmp", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"tmpWu", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "/.", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"u", "[", 
            RowBox[{"\[Nu]d", "[", "a_", "]"}], "]"}], "->", 
           RowBox[{"v", "[", 
            RowBox[{
             RowBox[{"\[Nu]d", "[", "a", "]"}], "[", "3", "]"}], "]"}]}], ",", 
          RowBox[{
           RowBox[{"u", "[", 
            RowBox[{"eu", "[", "a_", "]"}], "]"}], "->", 
           RowBox[{"u", "[", 
            RowBox[{
             RowBox[{"eu", "[", "a", "]"}], "[", "4", "]"}], "]"}]}]}], 
         "}"}]}], ")"}], ".", 
      RowBox[{"(", 
       RowBox[{"tmpp", "/.", 
        RowBox[{"M", "->", 
         SubscriptBox["M", "W"]}]}], ")"}], ".", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"tmpWu", "[", 
         RowBox[{"[", "2", "]"}], "]"}], "/.", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"u", "[", 
            RowBox[{"\[Nu]d", "[", "a_", "]"}], "]"}], "->", 
           RowBox[{"v", "[", 
            RowBox[{
             RowBox[{"\[Nu]d", "[", "a", "]"}], "[", "1", "]"}], "]"}]}], ",", 
          RowBox[{
           RowBox[{"u", "[", 
            RowBox[{"eu", "[", "a_", "]"}], "]"}], "->", 
           RowBox[{"u", "[", 
            RowBox[{
             RowBox[{"eu", "[", "a", "]"}], "[", "2", "]"}], "]"}]}], ",", 
          RowBox[{
           RowBox[{"\[Gamma]u", "[", "\[Mu]", "]"}], "->", 
           RowBox[{"\[Gamma]u", "[", "\[Nu]", "]"}]}]}], "}"}]}], ")"}]}], "/.", 
     RowBox[{
      RowBox[{"Wud", "[", 
       RowBox[{"_", ",", "_"}], "]"}], "->", "1"}]}]}], ",", "yield", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"tmpaW", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"tmp", "//.", 
       RowBox[{"simpleDot2", "[", "scalars", "]"}]}], "//", "Simplify"}], "//",
      "Flatten"}]}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<The Fierz identity transforms this into: \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"tmpaW", "=", 
    RowBox[{"tmpaW", "/.", 
     RowBox[{
      RowBox[{"HoldPattern", "[", 
       RowBox[{"Dot", "[", 
        RowBox[{"a__", ",", 
         RowBox[{"v", "[", "c_", "]"}], ",", "b__", ",", 
         RowBox[{"u", "[", "d_", "]"}]}], "]"}], "]"}], "->", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{"Dot", "[", 
         RowBox[{"a", ",", 
          RowBox[{"u", "[", "d", "]"}]}], "]"}]}], 
       RowBox[{"Dot", "[", 
        RowBox[{"b", ",", 
         RowBox[{"v", "[", "c", "]"}]}], "]"}]}]}]}]}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{
   "CR", "[", 
    "\"\< We can relate anti-fermion spinors to fermionic spinors by charge \
conjugation, C, which gives the same value for the matrix element.  Using the \
table on p.71 we can write (Note that \!\(\*SubscriptBox[\(P\), \
\(L\)]\)->\!\(\*SubscriptBox[\(P\), \(R\)]\)): \>\"", "]"}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"tmpvv", "=", 
     RowBox[{"ExtractPositionPattern", "[", 
      RowBox[{"tmpaW", ",", 
       RowBox[{"HoldPattern", "[", 
        RowBox[{"Dot", "[", 
         RowBox[{"a__", ",", 
          RowBox[{"v", "[", "_", "]"}]}], "]"}], "]"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"tmpvv", "[", 
      RowBox[{"[", 
       RowBox[{"1", ",", "2"}], "]"}], "]"}]}]}], ",", "yield", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"tmp", "=", 
    RowBox[{"C", ".", "tmp", ".", "C"}]}], ",", "yield", ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"tmpvv", "[", 
      RowBox[{"[", 
       RowBox[{"1", ",", "2"}], "]"}], "]"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"-", 
         RowBox[{"tmp", "[", 
          RowBox[{"[", 
           RowBox[{"2", ";;", 
            RowBox[{"-", "2"}]}], "]"}], "]"}]}], "/.", 
        RowBox[{
         SubscriptBox["P", "L"], "->", 
         SubscriptBox["P", "R"]}]}], "/.", 
       RowBox[{"v", "->", "u"}]}], "//", 
      RowBox[{"Swap", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"\[Nu]d", "[", "e", "]"}], "[", "1", "]"}], ",", 
         RowBox[{
          RowBox[{"\[Nu]d", "[", "e", "]"}], "[", "3", "]"}]}], "}"}], 
       "]"}]}]}], ";", "tmpvv"}], ",", "yield", ",", "\[IndentingNewLine]", 
   RowBox[{"tmpaW", "=", 
    RowBox[{"ReplacePart", "[", 
     RowBox[{"tmpaW", ",", "tmpvv"}], "]"}]}], ",", "\[IndentingNewLine]", 
   "NL", ",", "\"\<Return to more ambiguous notation: \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"tmpaW", "=", 
    RowBox[{"tmpaW", "/.", 
     RowBox[{
      RowBox[{"u", "[", 
       RowBox[{
        RowBox[{"Tensor", "[", 
         RowBox[{"a_", ",", "_", ",", "_"}], "]"}], "[", "_", "]"}], "]"}], "->", 
      RowBox[{"u", "[", "a", "]"}]}]}]}], ",", "\[IndentingNewLine]", 
   RowBox[{
   "CR", "[", 
    "\"\<The \!\(\*SubscriptBox[\(P\), \(R\)]\) make it not compatible with \
the Z amplitude.  Perhaps we should use CPT invariance, then the current \
would change by \!\(\*SubscriptBox[\(P\), \(R\)]\)->\!\(\*SubscriptBox[\(P\), \
\(L\)]\). \>\"", "]"}], ",", "\[IndentingNewLine]", 
   RowBox[{"tmpaW", "=", 
    RowBox[{"tmpaW", "/.", 
     RowBox[{
      SubscriptBox["P", "R"], "->", 
      SubscriptBox["P", "L"]}]}]}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<This is added to the Z-exchange term: \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"tmpgLRZW", "=", 
    RowBox[{"tmpaZ2", "+", "tmpaW"}]}]}], "\[IndentingNewLine]", 
  "]"}]], "Input",
