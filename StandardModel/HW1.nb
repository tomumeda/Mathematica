      RowBox[{"(", 
       RowBox[{
        RowBox[{"tmpWu", "[", 
         RowBox[{"[", "2", "]"}], "]"}], "/.", 
        RowBox[{"\[Mu]", "->", "\[Nu]"}]}], ")"}]}], "/.", 
     RowBox[{
      RowBox[{"Wud", "[", 
       RowBox[{"_", ",", "_"}], "]"}], "->", "1"}]}]}], ",", "yield", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"tmpaW", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"tmp", "//.", 
       RowBox[{"simpleDot2", "[", "scalars", "]"}]}], "//", "Simplify"}], "//",
      "Flatten"}]}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<The Fierz identity transforms this into Z-exchange form \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"tmpaW", "=", 
    RowBox[{"tmpaW", "/.", 
     RowBox[{
      RowBox[{"HoldPattern", "[", 
       RowBox[{"Dot", "[", 
        RowBox[{"a__", ",", "\[Nu]", ",", "b__", ",", "e"}], "]"}], "]"}], "->", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{"Dot", "[", 
         RowBox[{"a", ",", 
