Cell[BoxData[
 RowBox[{"PR1", "[", 
  RowBox[{
  "\"\<Consider a neutrino scattering from an electron: \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["\[Nu]", "1"], ",", 
      SubscriptBox["e", "2"]}], "}"}], "->", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["\[Nu]", "3"], ",", 
      SubscriptBox["e", "4"]}], "}"}]}], ",", "\[IndentingNewLine]", 
   "\"\<, where the particles have momenta \>\"", ",", 
   RowBox[{"{", 
    RowBox[{
     SubscriptBox["p", "1"], ",", 
     SubscriptBox["p", "2"], ",", 
     SubscriptBox["p", "3"], ",", 
     SubscriptBox["p", "4"]}], "}"}], ",", "\[IndentingNewLine]", 
   "\"\<  Show that, in the case that the dot product of all 4-momenta are \
much less than \!\(\*SuperscriptBox[SubscriptBox[\(M\), \(W\)], \(2\)]\), the \
standard model scattering amplitudes may be written in the form \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"tmpA", "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       RowBox[{"-", "4"}], 
       SubscriptBox["G", "F"]}], 
      SqrtBox["2"]], 
     RowBox[{
      SubscriptBox[
       OverscriptBox["u", "_"], "4"], ".", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"gL", " ", 
         RowBox[{
          RowBox[{
           RowBox[{"\[Gamma]u", "[", "\[Mu]", "]"}], ".", 
           RowBox[{"(", 
            RowBox[{"1", "-", 
             RowBox[{"\[Gamma]u", "[", "5", "]"}]}], ")"}]}], "/", "2"}]}], 
        "+", 
        RowBox[{"gR", " ", 
         RowBox[{
          RowBox[{
           RowBox[{"\[Gamma]u", "[", "\[Mu]", "]"}], ".", 
           RowBox[{"(", 
            RowBox[{"1", "+", 
             RowBox[{"\[Gamma]u", "[", "5", "]"}]}], ")"}]}], "/", "2"}]}]}], 
