Cell[BoxData[
 RowBox[{
  RowBox[{"PR1", "[", 
   RowBox[{
   "\"\<For {u,d}-> W->{\[Nu],e} decay we follow the polarization arguement \
on p.144-p.145 but where {e,p}->{u,d} and {\[Mu],\[Mu]}->{e,\[Nu]} and since \
W couples only to Left helicity products.  The amplitude for W+ and W- is \
\>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"Column", "[", "\[IndentingNewLine]", 
     RowBox[{"tmpM", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"\[ScriptCapitalM]d", "[", "\"\<-\>\"", "]"}], "->", 
         RowBox[{
          RowBox[{"g", "^", "2"}], " ", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             OverscriptBox[
              RowBox[{"ed", "[", "L", "]"}], "_"], ".", 
             RowBox[{"\[Gamma]u", "[", "\[Mu]", "]"}], ".", 
             RowBox[{"\[Nu]d", "[", "L", "]"}]}], ")"}], ".", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"gdd", "[", 
              RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}], 
             RowBox[{"Prop", "[", "W", "]"}]}], ")"}], ".", 
           RowBox[{"(", 
            RowBox[{
             OverscriptBox[
              RowBox[{"ud", "[", "L", "]"}], "_"], ".", 
             RowBox[{"\[Gamma]u", "[", "\[Nu]", "]"}], ".", 
             RowBox[{"dd", "[", "L", "]"}]}], ")"}]}]}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"\[ScriptCapitalM]d", "[", "\"\<+\>\"", "]"}], "->", 
         RowBox[{
          RowBox[{"g", "^", "2"}], " ", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             OverscriptBox[
              RowBox[{"\[Nu]d", "[", "L", "]"}], "_"], ".", 
             RowBox[{"\[Gamma]u", "[", "\[Mu]", "]"}], ".", 
             RowBox[{"ed", "[", "L", "]"}]}], ")"}], ".", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"gdd", "[", 
              RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}], 
             RowBox[{"Prop", "[", "W", "]"}]}], ")"}], ".", 
           RowBox[{"(", 
            RowBox[{
             OverscriptBox[
              RowBox[{"dd", "[", "L", "]"}], "_"], ".", 
             RowBox[{"\[Gamma]u", "[", "\[Nu]", "]"}], ".", 
             RowBox[{"ud", "[", "L", "]"}]}], ")"}]}]}]}]}], "}"}]}], "]"}], 
    ",", "\[IndentingNewLine]", "NL", ",", "\"\<where: \>\"", ",", 
    RowBox[{
     RowBox[{"Prop", "[", "W", "]"}], "->", 
     RowBox[{"I", " ", "/", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"pdu", "[", 
           RowBox[{"W", ",", "\[Alpha]"}], "]"}], ".", 
          RowBox[{"pdd", "[", 
           RowBox[{"W", ",", "\[Alpha]"}], "]"}]}], ")"}], "-", 
        RowBox[{
         SubscriptBox["M", "W"], "^", "2"}]}], ")"}]}]}], ",", 
    "\[IndentingNewLine]", "NL", ",", 
    "\"\< and the spinor correspondence:\>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"Column", "[", "\[IndentingNewLine]", 
     RowBox[{"subuv", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         OverscriptBox[
          RowBox[{"dd", "[", "L_", "]"}], "_"], "->", 
         OverscriptBox[
          RowBox[{"v", "[", 
           RowBox[{"dd", "[", "L", "]"}], "]"}], "_"]}], ",", 
        RowBox[{
         RowBox[{"dd", "[", "L_", "]"}], "\[Rule]", 
         RowBox[{"v", "[", 
          RowBox[{"dd", "[", "L", "]"}], "]"}]}], ",", "\[IndentingNewLine]", 
        
        RowBox[{
