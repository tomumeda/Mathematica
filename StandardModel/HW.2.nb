Cell[BoxData[
 RowBox[{
  RowBox[{"PR1", "[", 
   RowBox[{
   "\"\<Assume lepton particle mass is negligible so (using timespace \
indices) \>\"", ",", 
    RowBox[{"tmp1", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"puu", "[", 
        RowBox[{"L", ",", 
         RowBox[{"timespace", "@", "\[Mu]"}]}], "]"}], 
       RowBox[{"pud", "[", 
        RowBox[{"L", ",", 
         RowBox[{"timespace", "@", "\[Mu]"}]}], "]"}]}], "->", "0"}]}], ",", 
    "\[IndentingNewLine]", 
    "\"\< and divide the momentum into transverse(1) and parallel(2) \
components \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"puu", "[", 
      RowBox[{"L", ",", 
       RowBox[{"timespace", "@", "\[Mu]"}]}], "]"}], "->", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"puu", "[", 
        RowBox[{
         RowBox[{"L", "[", "i", "]"}], ",", 
         RowBox[{"timespace", "@", "\[Mu]"}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", "2"}], "}"}]}], "]"}]}], ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", "NL", ",", 
    "\"\<The sum of the two leptonic (L2) 4-vector in the center of mass, \
i.e., \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"pud", "[", 
      RowBox[{"L2", ",", 
       RowBox[{"timespace", "@", "\[Mu]"}]}], "]"}], "->", 
     RowBox[{
      RowBox[{"pud", "[", 
       RowBox[{"e", " ", ",", 
        RowBox[{"timespace", "@", "\[Mu]"}]}], "]"}], "+", 
      RowBox[{"pud", "[", 
       RowBox[{"\[Nu]", " ", ",", 
        RowBox[{"timespace", "@", "\[Mu]"}]}], "]"}]}]}], ",", "yield", ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"pud", "[", 
      RowBox[{"e", " ", ",", 
       RowBox[{"timespace", "@", "0"}]}], "]"}], "+", 
     RowBox[{"pud", "[", 
      RowBox[{"\[Nu]", " ", ",", 
       RowBox[{"timespace", "@", "0"}]}], "]"}]}], ",", "yield", ",", 
    RowBox[{
     RowBox[{"pud", "[", 
      RowBox[{"L", " ", ",", 
       RowBox[{"timespace", "@", "0"}]}], "]"}], "+", 
     RowBox[{"pud", "[", 
      RowBox[{"L", " ", ",", 
       RowBox[{"timespace", "@", "0"}]}], "]"}]}], ",", 
    "\[IndentingNewLine]", 
    "\"\< where {e,\[Nu]} decay leptons and satisfies: \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"pud", "[", 
      RowBox[{"L2", " ", ",", 
       RowBox[{"timespace", "@", "\[Mu]"}]}], "]"}], 
     RowBox[{"puu", "[", 
      RowBox[{"L2", " ", ",", 
       RowBox[{"timespace", "@", "\[Mu]"}]}], "]"}]}], ",", "yield", ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"subp1", "=", 
     RowBox[{
      RowBox[{"4", " ", 
       RowBox[{"puu", "[", 
        RowBox[{"L", " ", ",", 
         RowBox[{"timespace", "@", "0"}]}], "]"}], 
       RowBox[{"puu", "[", 
        RowBox[{"L", " ", ",", 
         RowBox[{"timespace", "@", "0"}]}], "]"}]}], "->", 
      RowBox[{
       SubscriptBox["M", "W"], "^", "2"}]}]}], ",", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", "NL", ",", "\"\< where \>\"", ",", 
    RowBox[{
     RowBox[{"En", "[", "W", "]"}], "->", 
     SubscriptBox["M", "W"]}], ",", 
    "\"\<, is the mass of the W.\nThen by definition of transverse momentum, \
\>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"sub", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"puu", "[", 
         RowBox[{
          RowBox[{"L", "[", "1", "]"}], ",", 
          RowBox[{"timespace", "@", "1"}]}], "]"}], "->", 
        RowBox[{
         RowBox[{"puu", "[", 
          RowBox[{"L", ",", 
           RowBox[{"timespace", "@", "1"}]}], "]"}], 
         RowBox[{"Sin", "[", "\[Theta]", "]"}]}]}], ",", 
       RowBox[{
        RowBox[{"pud", "[", 
         RowBox[{
          RowBox[{"L", "[", "1", "]"}], ",", 
          RowBox[{"timespace", "@", "1"}]}], "]"}], "->", 
        RowBox[{
         RowBox[{"-", 
          RowBox[{"pud", "[", 
           RowBox[{"L", ",", 
            RowBox[{"timespace", "@", "1"}]}], "]"}]}], 
         RowBox[{"Sin", "[", "\[Theta]", "]"}]}]}]}], "}"}]}], ",", 
    "\[IndentingNewLine]", "NL", ",", 
    "\"\<and contracting the leptonic 4-vector (massless): \>\"", ",", 
    "\[IndentingNewLine]", "tmp1", ",", "yield", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp1", "=", 
     RowBox[{"MapAt", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"ExpandSpaceTimeIndex", "[", 
          RowBox[{"{", "\[Mu]", "}"}], "]"}], "[", "#", "]"}], "&"}], ",", 
       "tmp1", ",", "1"}], "]"}]}], ",", "yield", ",", "\[IndentingNewLine]", 
    
    RowBox[{"tmp1", "=", 
     RowBox[{"tmp1", "/.", 
      RowBox[{
       RowBox[{"pud", "[", 
        RowBox[{"L", ",", 
         RowBox[{"timespace", "@", "i_"}]}], "]"}], ":>", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"i", "==", "0"}], ",", "1", ",", 
          RowBox[{"-", "1"}]}], "]"}], 
        RowBox[{"puu", "[", 
         RowBox[{"L", ",", 
          RowBox[{"timespace", "@", "i"}]}], "]"}]}]}]}]}], ",", 
    "\[IndentingNewLine]", "NL", ",", "\"\<Eliminate \>\"", ",", 
    RowBox[{"tmpe", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"puu", "[", 
        RowBox[{"L", ",", 
         RowBox[{"timespace", "@", "1"}]}], "]"}], ",", 
       RowBox[{"puu", "[", 
        RowBox[{"L", ",", 
         RowBox[{"timespace", "@", "0"}]}], "]"}]}], "}"}]}], ",", 
    "\[IndentingNewLine]", "\"\< in the set of equations \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"sub", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", "tmp1", ",", "subp1"}], "}"}], "//.", 
      RowBox[{"Rule", "->", "Equal"}]}]}], ",", "Imply", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"Eliminate", "[", 
      RowBox[{"tmp", ",", "tmpe"}], "]"}]}], ",", "yield", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"Framed", "[", "\[IndentingNewLine]", 
     RowBox[{"tmpCos", "=", 
      RowBox[{
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"#", "/", 
           RowBox[{"tmp", "[", 
            RowBox[{"[", 
             RowBox[{"2", ",", "2"}], "]"}], "]"}]}], "&"}], ",", "tmp"}], 
        "]"}], "/.", 
       RowBox[{
        RowBox[{
         RowBox[{"Sin", "[", "\[Theta]", "]"}], "^", "2"}], "->", 
        RowBox[{"1", "-", 
         RowBox[{
          RowBox[{"Cos", "[", "\[Theta]", "]"}], "^", "2"}]}]}]}]}], 
     "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", "]"}], 
  ";"}]], "Input",
