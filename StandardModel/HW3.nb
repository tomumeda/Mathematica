          RowBox[{"tmpvNorm", "[", "6", "]"}], "->", 
          RowBox[{
           RowBox[{"vectors", "[", 
            RowBox[{"[", "6", "]"}], "]"}], "/", 
           RowBox[{"Norm", "[", 
            RowBox[{"vectors", "[", 
             RowBox[{"[", "6", "]"}], "]"}], "]"}]}]}]}], "}"}]}], ")"}], "//",
      "Column"}], ",", "\[IndentingNewLine]", "NL", ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"tmpA\[Theta]w", "=", 
