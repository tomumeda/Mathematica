Cell[BoxData[{
 RowBox[{
  RowBox[{"PR1", "[", 
   RowBox[{
   "\"\<The neutral particle mass matrix can be organized by the set: \>\"", 
    ",", "\[IndentingNewLine]", 
    RowBox[{"tmpn", "=", 
     RowBox[{"neutrals", "->", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"Au", "[", "\[Mu]", "]"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"Zuu", "[", 
          RowBox[{"0", ",", "\[Mu]"}], "]"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"B1u", "[", "\[Mu]", "]"}], "}"}]}], "}"}]}]}], ",", 
    "\[IndentingNewLine]", 
    "\"\< Then the mass terms can be expressed by: \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"MatrixForm", "[", "\[IndentingNewLine]", 
     RowBox[{"tmpM", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"0", ",", "0", ",", "0"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"0", ",", 
          RowBox[{
           SuperscriptBox["v2", "2"], "/", "8"}], ",", 
          RowBox[{
           RowBox[{"-", "g1"}], " ", 
           RowBox[{
            SuperscriptBox["v2", "2"], "/", "8"}]}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"0", ",", 
          RowBox[{
           RowBox[{"-", "g1"}], " ", 
           RowBox[{
            SuperscriptBox["v2", "2"], "/", "8"}]}], ",", 
          RowBox[{
           RowBox[{
            SuperscriptBox["g1", "2"], 
            RowBox[{
             SuperscriptBox["v2", "2"], "/", "8"}]}], "+", 
           RowBox[{
            SuperscriptBox["g2", "2"], 
            RowBox[{
             SuperscriptBox["v1", "2"], "/", "8"}]}]}]}], "}"}]}], "}"}]}], 
     "]"}], ",", "\[IndentingNewLine]", "NL", ",", "\"\<Look for \>\"", ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"Zuu", "[", 
       RowBox[{"0", ",", "\[Mu]"}], "]"}], ",", 
      RowBox[{"B1u", "[", "\[Mu]", "]"}]}], "}"}], ",", " ", 
    "\"\< basis transform that diagonalizes mass matrix.  The relevant \
elements: \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"tmp", "=", 
       RowBox[{"tmpM", "[", 
        RowBox[{"[", 
         RowBox[{
          RowBox[{"2", ";;", "3"}], ",", 
          RowBox[{"2", ";;", "3"}]}], "]"}], "]"}]}], ")"}], "//", 
     "MatrixForm"}], ",", "\[IndentingNewLine]", "NL", ",", 
    "\"\<Take transformation matrix of form: \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"tmpT", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Cos", "[", "\[Theta]z", "]"}], ",", 
           RowBox[{"-", 
            RowBox[{"Sin", "[", "\[Theta]z", "]"}]}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Sin", "[", "\[Theta]z", "]"}], ",", 
           RowBox[{"Cos", "[", "\[Theta]z", "]"}]}], "}"}]}], "}"}]}], ")"}], 
     "//", "MatrixForm"}], ",", "\[IndentingNewLine]", "NL", ",", 
    "\"\<The transformed mass matrix: \>\"", ",", "Yield", ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"tmpTT", "=", 
       RowBox[{
        RowBox[{"tmpT", ".", "tmp", ".", 
         RowBox[{"Transpose", "[", "tmpT", "]"}]}], "//", "Simplify"}]}], 
      ")"}], "//", "MatrixForm"}], ",", "\[IndentingNewLine]", "NL", ",", 
    "\"\<And find \[Theta]z such that the off-diagonal elements are zero.  \
Using \>\"", ",", 
    RowBox[{"v1", "\[GreaterGreater]", "v2"}], ",", 
    "\"\< the series expansion of the off-diagonal element around v2: \>\"", 
    ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"tmp", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"tmpTT", "[", 
         RowBox[{"[", 
          RowBox[{"1", ",", "2"}], "]"}], "]"}], "\[Equal]", "0"}], "//", 
       "Simplify"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"tmp", "=", 
      RowBox[{"tmp", "//.", 
       RowBox[{
        RowBox[{"Sin", "[", "a_", "]"}], "\[RuleDelayed]", 
        RowBox[{
         RowBox[{"xTan", "[", "a", "]"}], 
         RowBox[{"Cos", "[", "a", "]"}]}]}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"tmp", "=", 
      RowBox[{
       RowBox[{"Simplify", "[", 
        RowBox[{"tmp", ",", 
         RowBox[{
          RowBox[{"Cos", "[", 
           RowBox[{"\[Theta]z", " ", "2"}], "]"}], ">", "0"}]}], "]"}], "/.", 
       
       RowBox[{"xTan", "\[Rule]", "Tan"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"tmp\[Theta]z", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Solve", "[", 
         RowBox[{"tmp", ",", "\[Theta]z"}], "]"}], "[", 
        RowBox[{"[", 
         RowBox[{"1", ",", "1"}], "]"}], "]"}], "//", "Simplify"}]}]}], ",", 
    "\[IndentingNewLine]", "NL", ",", 
    "\"\<First term of expansion about v2: \>\"", ",", "\[IndentingNewLine]", 
    
    RowBox[{"tmp\[Theta]z", "=", 
     RowBox[{
      RowBox[{"MapAt", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Series", "[", 
          RowBox[{"#", ",", 
           RowBox[{"{", 
            RowBox[{"v2", ",", "0", ",", "3"}], "}"}]}], "]"}], "&"}], ",", 
        "tmp\[Theta]z", ",", "2"}], "]"}], "//", "Normal"}]}], ",", 
    "\[IndentingNewLine]", "NL", ",", 
    "\"\<Then to order (v2/v1)^2 the mass matrix terms become: \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"tmpTTp", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Series", "[", 
              RowBox[{
               RowBox[{"tmpTT", "[", 
                RowBox[{"[", 
                 RowBox[{"i", ",", "j"}], "]"}], "]"}], ",", 
               RowBox[{"{", 
                RowBox[{"\[Theta]z", ",", "0", ",", "1"}], "}"}]}], "]"}], "//",
              "Normal"}], ")"}], "/.", "tmp\[Theta]z"}], "//", "Simplify"}], 
         ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "2"}], "}"}]}], "]"}]}], ")"}], "//", 
     "MatrixForm"}]}], "\[IndentingNewLine]", "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"PR1", "[", 
   RowBox[{
   "\"\<By using the above relationship \>\"", ",", "sub\[Theta]", ",", 
    "\"\< and the correspondence \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"g1", "->", 
       RowBox[{"f", "'"}]}], ",", 
      RowBox[{"g2", "->", "g"}], ",", 
      RowBox[{"v1", "->", 
       RowBox[{"v", "'"}]}], ",", 
      RowBox[{"v2", "->", "v"}]}], "}"}], ",", 
    "\"\< we get the class solutions where the original Z mass has a \
(v2/v1)^2 correction and there is a order Sin[\[Theta]z] mixing of the B1 \
into Z.\>\""}], "\[IndentingNewLine]", "]"}], ";"}]}], "Input",
