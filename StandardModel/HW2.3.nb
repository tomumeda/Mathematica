           RowBox[{"{", 
            RowBox[{"{", 
             RowBox[{"0", ",", 
              RowBox[{"vu", "[", 
               RowBox[{"field", "@", "1"}], "]"}], ",", "0", ",", 
              RowBox[{"vu", "[", 
               RowBox[{"field", "@", "2"}], "]"}]}], "}"}], "}"}], "//", 
           "Transpose"}], ")"}]}]}]}], ")"}], "//", "MatrixForms"}], ",", 
    "\[IndentingNewLine]", "NL", ",", "\"\<If \>\"", ",", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"tmpU", "=", 
       RowBox[{"U", "->", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"0", ",", "0", ",", "0", ",", "0"}], "}"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"{", 
            RowBox[{"0", ",", 
             RowBox[{"Cos", "[", "\[Beta]", "]"}], ",", "0", ",", 
             RowBox[{"-", 
              RowBox[{"Sin", "[", "\[Beta]", "]"}]}]}], "}"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"{", 
            RowBox[{"0", ",", "0", ",", "0", ",", "0"}], "}"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"{", 
            RowBox[{"0", ",", 
             RowBox[{"Sin", "[", "\[Beta]", "]"}], ",", "0", ",", 
