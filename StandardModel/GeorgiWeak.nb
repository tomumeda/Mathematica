  RowBox[{"PR1", "[", 
   RowBox[{"\"\<The {u,d} quark charge current \>\"", ",", " ", 
    RowBox[{"e421", "=", 
     RowBox[{
      RowBox[{"Vdd", "[", 
       RowBox[{"u", ",", "d"}], "]"}], ".", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"ju", "[", "\[Mu]", "]"}], "+", 
        RowBox[{"jdu", "[", 
         RowBox[{"5", ",", "\[Mu]"}], "]"}]}], ")"}]}]}], ",", 
    "\"\< where \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"subj", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"ju", "[", "\[Mu]", "]"}], "->", 
        RowBox[{
         OverscriptBox["u", "_"], ".", 
         RowBox[{"\[Gamma]u", "[", "\[Mu]", "]"}], ".", "d"}]}], ",", 
       RowBox[{
        RowBox[{"jdu", "[", 
         RowBox[{"5", ",", "\[Mu]"}], "]"}], "->", 
        RowBox[{
         OverscriptBox["u", "_"], ".", 
         RowBox[{"\[Gamma]u", "[", "\[Mu]", "]"}], ".", 
         RowBox[{"\[Gamma]d", "[", "5", "]"}], ".", "d"}]}]}], "}"}]}], ",", 
    "\[IndentingNewLine]", 
    "\"\<(4.2.6) the AB hadron decay amplitude via {u,d}->{lepton} is: \>\"", 
    ",", 
    RowBox[{"e426", "=", 
     RowBox[{
      RowBox[{"Vdd", "[", 
       RowBox[{"u", ",", "d"}], "]"}], ".", 
      RowBox[{"Bra", "[", "B", "]"}], ".", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"ju", "[", "\[Mu]", "]"}], "+", 
        RowBox[{"jdu", "[", 
         RowBox[{"5", ",", "\[Mu]"}], "]"}]}], ")"}], ".", 
      RowBox[{"Ket", "[", "A", "]"}], ".", 
      RowBox[{"ld", "[", "\[Mu]", "]"}], ".", 
      FractionBox[
       SubscriptBox["G", "F"], 
       SqrtBox["2"]]}]}], ",", "\"\< where \>\"", ",", 
    RowBox[{"ld", "[", "\[Mu]", "]"}], ",", 
    "\"\< is the lepton matrix element, e.g., \>\"", ",", 
    RowBox[{"subl", "=", 
     RowBox[{
      RowBox[{"ld", "[", "\[Mu]", "]"}], "->", 
      RowBox[{
       OverscriptBox[
        RowBox[{"\[Nu]d", "[", "e", "]"}], "_"], ".", 
       RowBox[{"\[Gamma]d", "[", "\[Mu]", "]"}], ".", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Id", "[", "\[Gamma]", "]"}], "+", 
         RowBox[{"\[Gamma]d", "[", "5", "]"}]}], ")"}], ".", 
       RowBox[{"eu", "[", "\"\<-\>\"", "]"}]}]}]}], ",", 
    "\[IndentingNewLine]", "NL", ",", "\"\<The \>\"", ",", 
    RowBox[{"jdu", "[", 
     RowBox[{"5", ",", "\[Mu]"}], "]"}], ",", 
