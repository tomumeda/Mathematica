       RowBox[{
        RowBox[{
         SubscriptBox["P", "L"], ".", 
         SubscriptBox["P", "L"]}], "->", 
        SubscriptBox["P", "L"]}]}], "//.", 
      RowBox[{"simpleDot1", "[", 
       RowBox[{"{", "}"}], "]"}]}]}], ",", "NL", ",", "\"\<with \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"sub", "=", 
     RowBox[{"{", 
