      RowBox[{"(", 
       RowBox[{"subPLR", "/.", 
        RowBox[{"\[Mu]", "->", "0"}]}], ")"}]}]}], ",", "Yield", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"tmp", "//.", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Reverse", "[", 
