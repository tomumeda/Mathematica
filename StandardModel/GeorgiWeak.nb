 RowBox[{
  RowBox[{"PR1", "[", 
   RowBox[{
   "\"\<ASIDE: chiral lagrangian and fermionic basis.  The properties of \
chiral projection operators: \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"subP", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        SubscriptBox["P", "L"], "->", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "-", 
            RowBox[{"\[Gamma]u", "[", "5", "]"}]}], ")"}], "/", "2"}], 
         ")"}]}], ",", 
       RowBox[{
        SubscriptBox["P", "R"], "->", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "+", 
            RowBox[{"\[Gamma]u", "[", "5", "]"}]}], ")"}], "/", "2"}], 
         ")"}]}], ",", 
       RowBox[{
        RowBox[{
         SubscriptBox["P", "L"], "+", 
         SubscriptBox["P", "R"]}], "->", "1"}]}], "}"}]}], ",", 
    "\[IndentingNewLine]", "NL", ",", "\"\<(1) \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"tmp", "=", 
          RowBox[{
           SubscriptBox["P", "L"], " ", ".", " ", 
           SubscriptBox["P", "R"]}]}], ")"}], "->", 
        RowBox[{"(", 
         RowBox[{"tmp", "/.", "subP"}], ")"}]}], "//.", 
       RowBox[{"simpleDot2", "[", 
        RowBox[{"{", "}"}], "]"}]}], "//", "Expand"}]}], ",", "yield", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"subP0", "=", 
     RowBox[{"tmp", "/.", "simpleGamma"}]}], ",", "and", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"subP0", "=", 
     RowBox[{"Append", "[", 
      RowBox[{
       RowBox[{"{", "subP0", "}"}], ",", 
       RowBox[{"MapAt", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Reverse", "[", "#", "]"}], "&"}], ",", "subP0", ",", "1"}],
         "]"}]}], "]"}]}], ",", "\[IndentingNewLine]", "NL", ",", 
    "\"\<(2) \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"tmp", "=", 
        RowBox[{
         SubscriptBox["P", "L"], ".", 
         RowBox[{"\[Gamma]u", "[", "\[Mu]", "]"}]}]}], ")"}], "\[Rule]", 
      RowBox[{"(", 
       RowBox[{"tmp", "/.", "subP"}], ")"}]}]}], ",", "yield", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"tmp", "//.", 
      RowBox[{"simpleDot2", "[", 
       RowBox[{"{", "}"}], "]"}]}]}], ",", "\"\< with \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"sub", "=", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"RuleX", "[", 
         RowBox[{"ps371", ",", 
          RowBox[{
           RowBox[{"\[Gamma]u", "[", "5", "]"}], ".", 
           RowBox[{"\[Gamma]u", "[", "\[Mu]", "]"}]}]}], "]"}], ",", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"\[Gamma]u", "[", "\[Mu]", "]"}], ".", 
            RowBox[{"\[Gamma]u", "[", "5", "]"}]}], "+", 
           RowBox[{"\[Gamma]u", "[", "\[Mu]", "]"}]}], ")"}], "\[Rule]", 
         RowBox[{
          RowBox[{"\[Gamma]u", "[", "\[Mu]", "]"}], ".", 
          RowBox[{"(", 
           RowBox[{"1", "+", 
            RowBox[{"\[Gamma]u", "[", "5", "]"}]}], ")"}]}]}], ",", 
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"2", " ", "#"}], "&"}], ",", 
          RowBox[{"Reverse", "[", 
           RowBox[{"subP", "[", 
            RowBox[{"[", "2", "]"}], "]"}], "]"}]}], "]"}]}], "}"}], "//", 
      "Flatten"}]}], ",", "imply", ",", "\[IndentingNewLine]", 
    RowBox[{"subPLR", "=", 
     RowBox[{
      RowBox[{"tmp", "//.", "sub"}], "//.", 
      RowBox[{"simpleDot2", "[", 
       RowBox[{"{", "}"}], "]"}]}]}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"subPLR", "=", 
      RowBox[{
       RowBox[{"AppendTo", "[", 
        RowBox[{
         RowBox[{"{", "subPLR", "}"}], ",", "subP0"}], "]"}], "//", 
       "Flatten"}]}], ";", "\[IndentingNewLine]", "NL"}], ",", 
    "\"\<So we can decompose into chiral basis \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"subP\[Psi]", "=", 
     RowBox[{"{", 
