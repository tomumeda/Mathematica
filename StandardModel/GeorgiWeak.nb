    RowBox[{"tmp", "=", 
     RowBox[{"tmp", "//.", "DotExpand"}]}], ",", "Yield", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"tmp", "/.", 
      RowBox[{
       RowBox[{"\[Psi]d", "[", "l_", "]"}], "->", 
       RowBox[{
        SubscriptBox["P", "l"], ".", 
        RowBox[{"\[Psi]d", "[", "l", "]"}]}]}]}]}], ",", "Yield", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"tmp", "//", 
      RowBox[{"ConjugateCTSimplify1", "[", 
