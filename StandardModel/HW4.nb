Cell[BoxData[
 RowBox[{
  RowBox[{"PR1", "[", 
   RowBox[{
    RowBox[{"CR", "[", "\"\<Problem 4-1: \>\"", "]"}], ",", 
    "\"\<Calculate the rate for the decay \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Ku", "[", "\"\<+\>\"", "]"}], " ", "->", 
     RowBox[{
      RowBox[{"\[Pi]u", "[", "0", "]"}], " ", 
      RowBox[{"eu", "[", "\"\<+\>\"", "]"}], 
      RowBox[{"\[Nu]d", "[", "e", "]"}]}]}], " ", ",", "\[IndentingNewLine]", 
    "\"\< in terms of \!\(\*SubscriptBox[\(G\), \(F\)]\) and \
\!\(\*SubscriptBox[\(V\), \(us\)]\).  Assume that SU[3] is a good approximate \
symmetry for hadronic matrix elements, even though the pion is much lighter \
than kaon.  To simplify the calculation, you may set \>\"", ",", 
    RowBox[{
     RowBox[{"md", "[", "\[Pi]", "]"}], "->", "0"}], ",", 
    "\"\< in addition to \>\"", ",", 
    RowBox[{
     RowBox[{"md", "[", "e", "]"}], "->", "0."}], ",", "\[IndentingNewLine]", 
    "NL", ",", "\"\<The amplitude: \>\"", ",", 
    RowBox[{"e443", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"Bra", "[", 
        RowBox[{"\[Pi]u", "[", "0", "]"}], "]"}], ".", 
       RowBox[{"ju", "[", "\[Mu]", "]"}], ".", 
       RowBox[{"Ket", "[", 
        RowBox[{"Ku", "[", "\"\<+\>\"", "]"}], "]"}], ".", 
       RowBox[{"ld", "[", "\[Mu]", "]"}]}], 
      RowBox[{"Vdd", "[", 
       RowBox[{"u", ",", "s"}], "]"}], 
      RowBox[{
       SubscriptBox["G", "F"], "/", 
       SqrtBox["2"]}]}]}], ",", "and", ",", "\[IndentingNewLine]", 
    RowBox[{"subj", "=", 
     RowBox[{
      RowBox[{"ju", "[", "\[Mu]", "]"}], "->", 
      RowBox[{
       OverscriptBox["u", "_"], ".", 
       RowBox[{"\[Gamma]u", "[", "\[Mu]", "]"}], ".", "s"}]}]}], ",", 
    "\"\< and the lepton matrix element is 2.3.3: \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"subl", "=", 
     RowBox[{
      RowBox[{"ld", "[", "\[Mu]", "]"}], "->", 
      RowBox[{
       OverscriptBox[
        RowBox[{"\[Nu]d", "[", "e", "]"}], "_"], ".", 
       RowBox[{"\[Gamma]d", "[", "\[Mu]", "]"}], ".", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Id", "[", "\[Gamma]", "]"}], "+", 
         RowBox[{"\[Gamma]d", "[", "5", "]"}]}], ")"}], ".", 
       RowBox[{"eu", "[", "\"\<-\>\"", "]"}]}]}]}], ",", "Yield", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"e443a", "=", 
     RowBox[{
      RowBox[{"e443", "/.", "subj"}], "/.", "subl"}]}]}], 
   "\[IndentingNewLine]", "]"}], ";"}]], "Input",
