Cell[BoxData[
 RowBox[{
  RowBox[{"PR1", "[", 
   RowBox[{"\"\<What is \>\"", ",", 
    SubscriptBox[
     RowBox[{"SU", "[", "3", "]"}], "\"\<L+R\>\""], ",", "\"\<?\>\"", ",", 
    "\[IndentingNewLine]", "NL", ",", 
    "\"\<Let's take the SU[3]xSU[3] particle wave function exponent to be \
\>\"", ",", 
    RowBox[{"sub\[Pi]", "=", 
     RowBox[{"\[Pi]", "->", 
      RowBox[{
       RowBox[{"\[Pi]d", "[", "a", "]"}], 
       RowBox[{"Td", "[", "a", "]"}]}]}]}], ",", "\"\< where \>\"", ",", 
    RowBox[{"Td", "[", "a", "]"}], ",", 
    "\"\< are Gell-Mann SU[3] matrices, \[Lambda].  The two diagonal \
Gell-Mann matrices are \>\"", ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\[Lambda]d", "[", "3", "]"}], ",", 
      RowBox[{"\[Lambda]d", "[", "8", "]"}]}], "}"}], ",", 
    "\[IndentingNewLine]", "NL", ",", 
    "\"\<Let the unitary transformation of SU[3]xSU[3] be represented by \
\>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"subLR", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"L", "->", 
        RowBox[{"Exp", "[", 
         RowBox[{"I", " ", 
          RowBox[{"ld", "[", "a", "]"}], 
          RowBox[{"Td", "[", "a", "]"}]}], "]"}]}], ",", 
       RowBox[{"R", "->", 
        RowBox[{"Exp", "[", 
         RowBox[{"I", " ", 
          RowBox[{"rd", "[", "c", "]"}], 
          RowBox[{"Td", "[", "c", "]"}]}], "]"}]}]}], "}"}]}], ",", 
    "\"\< where \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"reals", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"ld", "[", "a", "]"}], ",", 
       RowBox[{"rd", "[", "a", "]"}]}], "}"}]}], ",", 
    "\"\< are reals.  We evaluate the requirement that the unitary \
transformation \>\"", ",", "tmp\[CapitalSigma]t", ",", "\[IndentingNewLine]", 
    "\"\< be invariant.  Substitute definitions: \>\"", ",", "Yield", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp", "=", 
     RowBox[{"MapAt", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"#", "/.", "subLR"}], "/.", "tmp\[CapitalSigma]"}], "/.", 
         RowBox[{"(", 
          RowBox[{"sub\[Pi]", "/.", 
           RowBox[{"a", "->", "b"}]}], ")"}]}], "&"}], ",", 
       "tmp\[CapitalSigma]t", ",", "2"}], "]"}]}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"xorder", "=", "2"}], ";", "\[IndentingNewLine]", "NL"}], ",", 
    "\"\<Expanding to O[\>\"", ",", "xorder", ",", 
    "\"\<] noting that diagonal matrices\>\"", ",", 
