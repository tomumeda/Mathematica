  RowBox[{
   RowBox[{
    RowBox[{"tuDifFormSimplify1", "[", 
     RowBox[{"forms_List:", 
      RowBox[{"{", 
       RowBox[{"difForm", "[", "_", "]"}], "}"}]}], "]"}], "[", "exp_", "]"}],
    ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"$0", ",", "$"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"$", "=", 
       RowBox[{"exp", "//", 
        RowBox[{"tuWedgeSimplify1", "[", "forms", "]"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"$", "=", 
       RowBox[{"$", "/.", 
        RowBox[{
         RowBox[{"ww", ":", 
