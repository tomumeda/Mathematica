          RowBox[{"tuHasNoneQ", "[", 
           RowBox[{"ww", ",", 
            RowBox[{"difForm", "[", 
             RowBox[{"_", ",", "_"}], "]"}]}], 
           RowBox[{"(*", 
            RowBox[{"extended", " ", "form"}], "*)"}], "]"}]}]}]}]}], ";", 
      "\[IndentingNewLine]", 
