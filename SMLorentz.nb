Cell[BoxData[{
 RowBox[{
  RowBox[{"Print", "[", 
   RowBox[{"\"\<The transformations: \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"subx", "=", 
     RowBox[{
      RowBox[{"xu", "[", 
       RowBox[{"red", "@", "\[Mu]"}], "]"}], "->", 
      RowBox[{
       RowBox[{
        RowBox[{"\[CapitalLambda]ud", "[", 
         RowBox[{
          RowBox[{"red", "@", "\[Mu]"}], ",", "\[Nu]"}], "]"}], 
        RowBox[{"xu", "[", "\[Nu]", "]"}]}], "+", 
       RowBox[{"\[Xi]u", "[", "\[Mu]", "]"}]}]}]}], ",", "\"\< and \>\"", ",",
     "\[IndentingNewLine]", 
    RowBox[{"suby", "=", 
     RowBox[{
      RowBox[{"yu", "[", 
       RowBox[{"red", "@", "\[Mu]"}], "]"}], "->", 
      RowBox[{
       RowBox[{
        RowBox[{"\[CapitalLambda]ud", "[", 
         RowBox[{
          RowBox[{"red", "@", "\[Mu]"}], ",", "\[Nu]"}], "]"}], 
        RowBox[{"yu", "[", "\[Nu]", "]"}]}], "+", 
       RowBox[{"\[Xi]u", "[", "\[Mu]", "]"}]}]}]}], ",", 
    "\"\<\\nThe separation between two points: \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"subdx", "=", 
     RowBox[{
      RowBox[{"dxu", "[", 
       RowBox[{"red", "@", "\[Mu]"}], "]"}], "->", 
      RowBox[{
       RowBox[{"xu", "[", 
        RowBox[{"red", "@", "\[Mu]"}], "]"}], "-", 
       RowBox[{"yu", "[", 
        RowBox[{"red", "@", "\[Mu]"}], "]"}]}]}]}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"tmp", "=", 
      RowBox[{"subdx", "/.", 
       RowBox[{"{", 
        RowBox[{"subx", ",", "suby"}], "}"}]}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"tmp1", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"LowerIndex", "[", 
         RowBox[{"\[Mu]", ",", "\[Mu]"}], "]"}], "[", "tmp", "]"}], "/.", 
       RowBox[{"\[Nu]", "->", "\[Alpha]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"tmp1", "=", 
      RowBox[{"MapThread", "[", 
       RowBox[{"Times", ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Apply", "[", 
           RowBox[{"List", ",", "tmp1"}], "]"}], ",", 
          RowBox[{"Apply", "[", 
           RowBox[{"List", ",", "tmp"}], "]"}]}], " ", "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\"\<and the scalar separation: \>\""}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"tmp2", "=", 
     RowBox[{
      RowBox[{"Apply", "[", 
       RowBox[{"Rule", ",", "tmp1"}], "]"}], "//", "Simplify"}]}], ",", 
    "\"\<\\n\[Implies] \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp2", "=", 
     RowBox[{"tmp2", "/.", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"xu", "[", "a_", "]"}], "-", 
         RowBox[{"yu", "[", "a_", "]"}]}], ")"}], "->", 
       RowBox[{"dxu", "[", "a", "]"}]}]}]}], ",", "\"\<\\n\[Implies] \>\"", 
    ",", "\[IndentingNewLine]", 
    RowBox[{"tmp2", "=", 
     RowBox[{
      RowBox[{"tmp2", "/.", 
       RowBox[{
        RowBox[{"dxd", "[", "a_", "]"}], "->", 
        RowBox[{
         RowBox[{"\[Eta]dd", "[", 
          RowBox[{"a", ",", 
           RowBox[{"red", "@", "\[Alpha]"}]}], "]"}], " ", 
         RowBox[{"dxu", "[", 
          RowBox[{"red", "@", "\[Alpha]"}], "]"}]}]}]}], "/.", 
      RowBox[{
       RowBox[{"\[CapitalLambda]dd", "[", 
        RowBox[{"a_", ",", "b_"}], "]"}], "->", 
       RowBox[{
        RowBox[{"\[Eta]dd", "[", 
         RowBox[{"a", ",", 
          RowBox[{"red", "@", "\[Beta]"}]}], "]"}], 
        RowBox[{"\[CapitalLambda]ud", "[", 
         RowBox[{
          RowBox[{"red", "@", "\[Beta]"}], ",", "b"}], "]"}]}]}]}]}], ",", 
    "\[IndentingNewLine]", "\"\<\\nSince scalar distances are equal: \>\"", 
    ",", 
    RowBox[{"tmp3", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"tmp2", "[", 
        RowBox[{"[", 
         RowBox[{"2", ",", 
          RowBox[{"3", ";;", "5"}]}], "]"}], "]"}], "==", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"tmp2", "[", 
          RowBox[{"[", 
           RowBox[{"1", ",", "3"}], "]"}], "]"}], "/.", 
         RowBox[{"\[Mu]", "->", "\[Nu]"}]}], ")"}]}], "//", 
      RowBox[{"ToFlavor", "[", 
       RowBox[{"Identity", ",", "red"}], "]"}]}]}], ",", 
    "\[IndentingNewLine]", 
    "\"\<\\nFor infinitessimal transformation near identity: \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{"sub", "=", 
     RowBox[{
      RowBox[{"\[CapitalLambda]ud", "[", 
       RowBox[{"a_", ",", "b_"}], "]"}], "->", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"\[Delta]ud", "[", 
         RowBox[{"a", ",", "b"}], "]"}], "+", 
        RowBox[{"\[Omega]ud", "[", 
         RowBox[{"a", ",", "b"}], "]"}]}], ")"}]}]}], ",", "\"\<\\n\>\"", ",",
     "\[IndentingNewLine]", 
    RowBox[{"tmp3", "=", 
     RowBox[{"tmp3", "/.", "sub"}]}], ",", "\"\< \[Implies] \>\"", ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"tmp3", "=", 
      RowBox[{"tmp3", "//", "Expand"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"tmp3", "=", 
      RowBox[{"tmp3", "//", 
       RowBox[{"KroneckerAbsorb", "[", "\[Delta]", "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"tmp3", "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"tmp3", "/.", 
          RowBox[{
           RowBox[{"\[Omega]ud", "[", 
            RowBox[{"a_", ",", "b_"}], "]"}], " ", "->", 
           RowBox[{"e", " ", 
            RowBox[{"\[Omega]ud", "[", 
             RowBox[{"a", ",", "b"}], "]"}]}]}]}], "/.", 
         RowBox[{
          SuperscriptBox["e", "2"], "->", "0"}]}], "/.", 
        RowBox[{"e", "->", "1"}]}], "//", "Simplify"}]}]}], ",", 
    "\"\<\\n\[Implies] \>\"", ",", "\[IndentingNewLine]", 
    RowBox[{"tmp3", "=", 
     RowBox[{"tmp3", "//", 
      StyleBox[
       RowBox[{"MetricSimplify", "[", "\[Eta]", "]"}], "MR"]}]}]}], 
   "\[IndentingNewLine]", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Print", "[", 
  RowBox[{
