Cell[BoxData[
 RowBox[{"Print", "[", 
  RowBox[{"\"\<General group property of unitary operators: \>\"", ",", 
   RowBox[{
    RowBox[{
     RowBox[{"U", "[", 
      SubscriptBox["\[CapitalLambda]", "1"], "]"}], 
     RowBox[{"U", "[", 
      SubscriptBox["\[CapitalLambda]", "2"], "]"}]}], "==", 
    RowBox[{"U", "[", 
     RowBox[{
      SubscriptBox["\[CapitalLambda]", "1"], 
      SubscriptBox["\[CapitalLambda]", "2"]}], "]"}]}], ",", 
   "\[IndentingNewLine]", "\"\<\\nExpanding in Lie operature, \>\"", ",", " ", 
   RowBox[{"Juu", "[", 
    RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}], ",", 
   "\"\<, near the identity, Id,: \>\"", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{"subr", "=", 
     RowBox[{
      RowBox[{"U", "[", "\[Omega]", "]"}], "->", 
      RowBox[{"Id", " ", "+", " ", 
       RowBox[{
        FractionBox["I", "2"], " ", 
        RowBox[{"\[Omega]dd", "[", 
         RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}], 
        RowBox[{"Juu", "[", 
         RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}]}]}]}]}]}], ",", 
   "\[IndentingNewLine]", "\"\<\\nwhere \>\"", ",", 
   RowBox[{"\[Omega]dd", "[", 
    RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}], ",", "\"\< and \>\"", ",", 
   RowBox[{"Juu", "[", 
    RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}], ",", 
   "\"\< are both anti-symmetric.  Substituting: \\n\>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"sub", "=", 
    RowBox[{
     RowBox[{"\[Omega]dd", "[", 
      RowBox[{"\[Mu]", ",", "\[Nu]"}], "]"}], "->", " ", 
     RowBox[{
      RowBox[{"\[Omega]du", "[", 
       RowBox[{"\[Mu]", ",", "\[Alpha]"}], "]"}], 
      RowBox[{"\[Eta]dd", "[", 
       RowBox[{"\[Alpha]", ",", "\[Nu]"}], "]"}]}]}]}], ",", 
   "\"\< \[DoubleRightArrow] \>\"", ",", 
   RowBox[{"tmp", "=", 
    RowBox[{"tmp", "/.", "sub"}]}], ",", "\[IndentingNewLine]", 
   "\"\<\\nThe unitary operator for translations is: \>\"", ",", 
   "\[IndentingNewLine]", 
   RowBox[{"subt", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"U", "[", "\[Xi]", "]"}], "->", 
      RowBox[{"Id", "-", 
       RowBox[{"I", " ", 
        RowBox[{"\[Xi]d", "[", "\[Mu]", "]"}], 
        RowBox[{"Pu", "[", "\[Mu]", "]"}]}]}]}], "//", 
     RowBox[{"ReplaceIndex", "[", 
      RowBox[{"\[Mu]", ",", "\[Beta]"}], "]"}]}]}], ",", 
   "\[IndentingNewLine]", "\"\<\\nSo \>\"", ",", 
   RowBox[{
    RowBox[{
     RowBox[{"subt", "[", 
      RowBox[{"[", "1", "]"}], "]"}], 
     RowBox[{"subr", "[", 
      RowBox[{"[", "1", "]"}], "]"}]}], "->", 
    RowBox[{
     RowBox[{"subt", "[", 
      RowBox[{"[", "2", "]"}], "]"}], 
     RowBox[{"subr", "[", 
      RowBox[{"[", "2", "]"}], "]"}]}]}], ",", "\[IndentingNewLine]", 
   "\"\<\\n\>\"", ",", "\[IndentingNewLine]", 
   RowBox[{"tmp", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"subt", "[", 
       RowBox[{"[", "2", "]"}], "]"}], 
      RowBox[{"subr", "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], "//", "Expand"}]}], ",", 
   "\"\<\\n\>\"", ",", "\[IndentingNewLine]", 
   RowBox[{"tmp", "/.", 
    RowBox[{"Id", "->", "1"}]}], ",", 
   "\"\< This operation assumes abelian operators.  Let's see if QA terms \
work.\>\""}], "\[IndentingNewLine]", "]"}]], "Input",
