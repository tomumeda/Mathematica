Cell[BoxData[
 RowBox[{"PR", "[", 
  RowBox[{
  "\"\<\[FilledCircle] Hilbert space \[ScriptCapitalH] on \[CapitalSigma] \
related to operators \>\"", ",", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[GothicCapitalO]", "\[Phi]"], "[", "operator", "]"}], 
    "\[LongLeftRightArrow]", 
    RowBox[{"Ket", "[", "\[Phi]", "]"}]}], ",", 
   RowBox[{"CR", "[", "\"\<Unclear\>\"", "]"}], ",", "\[IndentingNewLine]", 
   "NL", ",", "\"\<\[Bullet] Simplest vertex operator \>\"", ",", 
   RowBox[{
    SubscriptBox["V", "\[Kappa]"], "\[Rule]", 
    RowBox[{"Exp", "[", 
     RowBox[{"I", " ", "\[Kappa]", " ", 
      RowBox[{"\[Phi]", "[", "\[Sigma]", "]"}]}], "]"}]}], ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<Most interesting are \>\"", ",", 
   RowBox[{"{", 
    RowBox[{
     SubscriptBox["V", "+"], ",", 
     SubscriptBox["V", "-"]}], "}"}], ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<Naive dimensional analysis(QFT): \>\"", ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"dim", "[", 
       SubscriptBox["\[Phi]", 
        RowBox[{"dim", ">", "2"}]], "]"}], "\[Rule]", 
      RowBox[{"0", "\[Implies]", 
       RowBox[{
        RowBox[{"dim", "[", 
         RowBox[{"Exp", "[", 
          RowBox[{"I", " ", "\[Phi]"}], "]"}], "]"}], "\[NotEqual]", " ", 
        "0"}]}]}], ",", 
     RowBox[{"\[NotExists]", 
      SubscriptBox["\[Phi]", 
       RowBox[{"dim", "\[Equal]", "2"}]]}]}], "}"}], ",", "NL", ",", 
   RowBox[{"CR", "[", "\"\<The following for dim==2?\>\"", "]"}], ",", 
   "\[IndentingNewLine]", "NL", ",", 
   "\"\<Construct \[ScriptCapitalH] by all possible \!\(\*SubscriptBox[\(\
\[GothicCapitalO]\), \(\[Phi]\)]\)\>\"", ",", "\[IndentingNewLine]", "Yield", 
   ",", "\"\<Anomalous dimension generated by: \>\"", ",", 
   RowBox[{"xProduct", "[", 
    RowBox[{
     RowBox[{"tuPartialD", "[", 
      RowBox[{
       RowBox[{"Exp", "[", 
        RowBox[{"I", " ", "\[Kappa]", " ", "\[Phi]"}], "]"}], ",", "i"}], 
      "]"}], ",", 
     RowBox[{"{", "i", "}"}]}], "]"}], ",", "\[IndentingNewLine]", 
   "\"\< which is IR regulated by \!\(\*SubscriptBox[\(m\), \(0\)]\).\>\"", 
   ",", "\[IndentingNewLine]", "NL", ",", "\"\<Compute: \>\"", ",", 
   RowBox[{"$0", "=", 
    RowBox[{
     RowBox[{"BraKet", "[", 
      RowBox[{
       RowBox[{
        SubscriptBox["V", "+"], "[", "\[Sigma]1", "]"}], 
       RowBox[{
        SubscriptBox["V", "-"], "[", "\[Sigma]2", "]"}]}], "]"}], "\[Rule]", 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{
        SubscriptBox["G", "0"], "[", "0", "]"}], "-", 
       RowBox[{"G", "[", 
        RowBox[{"Abs", "[", 
         RowBox[{"\[Sigma]1", "-", "\[Sigma]2"}], "]"}], "]"}]}], "]"}]}]}], 
   ",", "\[IndentingNewLine]", "NL", ",", 
   "\"\<Add UV regulator energy/momentum scale \>\"", ",", 
   RowBox[{"\[CapitalLambda]", "\[Rule]", 
    RowBox[{"1", "/", "a"}]}], ",", "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{"$", "=", 
    RowBox[{
     RowBox[{"$0", "[", 
      RowBox[{"[", "1", "]"}], "]"}], "\[Rule]", 
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox["1", 
        RowBox[{
         RowBox[{"Abs", "[", 
          RowBox[{"\[Sigma]1", "-", "\[Sigma]2"}], "]"}], 
         "\[CapitalLambda]"}]], ")"}], 
      RowBox[{
       RowBox[{"g", "^", "2"}], "/", 
       RowBox[{"(", 
        RowBox[{"2", "\[Pi]"}], ")"}]}]]}]}], ",", "\[IndentingNewLine]", 
   "NL", ",", 
   "\"\<\!\(\*SubscriptBox[\(m\), \(0\)]\) not in equation, take \
\!\(\*SubscriptBox[\(m\), \(0\)]\)\[LongRightArrow]0.  Introduce RG scale \
\[Mu]: \>\"", ",", "\[IndentingNewLine]", "Yield", ",", 
   RowBox[{
    RowBox[{
     RowBox[{"$", "[", 
      RowBox[{"[", "2", "]"}], "]"}], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"$", "[", 
         RowBox[{"[", "2", "]"}], "]"}], "/.", 
        RowBox[{"\[CapitalLambda]", "\[Rule]", "\[Mu]"}]}], ")"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"\[Mu]", "/", "\[CapitalLambda]"}], ")"}], 
       RowBox[{
        SuperscriptBox["g", "2"], "/", 
        RowBox[{"(", 
         RowBox[{"2", "\[Pi]"}], ")"}]}]]}]}], ";", "$"}], ",", 
   "\[IndentingNewLine]", "NL", ",", "\"\<Define: \>\"", ",", 
   RowBox[{"$v", "=", 
    RowBox[{
     SubscriptBox["V", "\"\<\[PlusMinus]\>\""], "\[Rule]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{
         SuperscriptBox[
          SubscriptBox["V", "\"\<\[PlusMinus]\>\""], "Bare"], "[", "\[Sigma]",
          "]"}], "\[Rule]", 
        RowBox[{
         SqrtBox["Z"], 
         RowBox[{
          SuperscriptBox[
           SubscriptBox["V", "\"\<\[PlusMinus]\>\""], "Renorm"], "[", 
          "\[Sigma]", "]"}]}]}], ",", 
       RowBox[{"Z", "->", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"\[Mu]", "/", "\[CapitalLambda]"}], ")"}], 
         RowBox[{
          SuperscriptBox["g", "2"], "/", 
          RowBox[{"(", 
           RowBox[{"4", "\[Pi]"}], ")"}]}]]}]}], "}"}]}]}], ",", 
   "\[IndentingNewLine]", "Imply", ",", 
   RowBox[{"$", "=", 
    RowBox[{
     RowBox[{"BraKet", "[", 
      RowBox[{
       RowBox[{
        SuperscriptBox[
         SubscriptBox["V", "+"], "Renorm"], "[", "\[Sigma]1", "]"}], ".", 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["V", "-"], "Renorm"], "[", "\[Sigma]2", "]"}]}], "]"}], 
